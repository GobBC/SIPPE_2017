	<queryString>
		<![CDATA[SELECT
DISTINCT
( SELECT T.DESCR FROM POA.TIPO_MOV T WHERE T.TIPOMOV = MOV0.TIPOMOV ) TIPOMOV,
MOV0.OFICIO,
MOV0.FECPPTO,
DECODE(B.TIPO_USR,'SPPD','CENTRAL','PARAESTATAL') AS SECTOR,
U.RAMO AS RAMO,
AFECTADOS.RAMO AS RAMO_AFECTADO,
NVL((SELECT
 NVL(DECODE(TM.TIPO_MODIFICACION,2,REVPRES.DESCR_CORTA||' '||REVPRES.DESCR||' presupuestal '||'-'||TM.DESCR,REVPRES.DESCR_CORTA||' '||REVPRES.DESCR||' presupuestal '),' ') CARATULA_DESCR
                FROM
                    POA.CARATULA CAR,
                    POA.REVISIONES_CARATULA REVS,
                    POA.REVISIONES_CARATULA REVPRES,
                    POA.REVISIONES_CARATULA REVPRG,
                    POA.TIPO_SESION TIS,
                    DGI.RAMOS RAM,
                    POA.TIPO_MODIFICACION TM
                WHERE
                CAR.TIPO_MODIFICACION=TM.TIPO_MODIFICACION AND
                    CAR.YEAR = CARATULA.C_ANIO AND
                    CAR.RAMO = U.RAMO AND
                    CAR.ID_CARATULA = CARATULA.C_ID AND
                    REVS.REVISION = CAR.NUMERO_SESION AND
                    REVPRES.REVISION (+)= CAR.NUM_MOD_PRES AND
                    REVPRG.REVISION (+)= CAR.NUM_MOD_PROG AND
                    TIS.CVE_SESION = CAR.TIPO_SESION AND
                    RAM.YEAR = CAR.YEAR AND
                    RAM.RAMO = CAR.RAMO

),' ') AS LIGADO_A_CARATULA,
MOV0.STATUS AS STATUS,
STS.DESCR AS DESCR,
MOV0.INVERSION AS INVERSION,
NVL(IMPACPROG.REL_IMP_PROG,'N') AS PROG,
TO_DATE(MOV0.FECHAELAB,'DD/MM/YYYY') AS FECHAELAB,
BITACORA_FIRMA0.FECHAAUT AS FECHAAUT_FIRMA0,
BITACORA_FIRMA0.APP_LOGIN AS APPLOGIN_FIRMA0,
BITACORA_FIRMA1.FECHAAUT AS FECHAAUT_FIRMA1,
BITACORA_FIRMA1.APP_LOGIN AS APPLOGIN_FIRMA1,
BITACORA_FIRMA2.FECHAAUT AS FECHAAUT_FIRMA2,
BITACORA_FIRMA2.APP_LOGIN AS APPLOGIN_FIRMA2,
BITACORA_FIRMA3.FECHAAUT AS FECHAAUT_FIRMA3,
BITACORA_FIRMA3.APP_LOGIN AS APPLOGIN_FIRMA3,
BITACORA_FIRMA4.FECHAAUT AS FECHAAUT_FIRMA4,
BITACORA_FIRMA4.APP_LOGIN AS APPLOGIN_FIRMA4,

DECODE(BITACORA_FIRMA2.FECHAAUT,NULL,0,(DGI.F_DIAS_LABORABLES_REP_POA(BITACORA_FIRMA0.FECHAAUT,BITACORA_FIRMA1.FECHAAUT))) AS DURACION_EJECUTIVOS,
DECODE(BITACORA_FIRMA2.FECHAAUT,NULL,0,(DGI.F_DIAS_LABORABLES_REP_POA(BITACORA_FIRMA1.FECHAAUT,BITACORA_FIRMA2.FECHAAUT))) AS DURACION_2daREVISION,DECODE(BITACORA_FIRMA2.FECHAAUT,NULL,0,(DGI.F_DIAS_LABORABLES_REP_POA(BITACORA_FIRMA2.FECHAAUT,BITACORA_FIRMA3.FECHAAUT))) AS DURACION_AutPLANEACION,
DECODE(BITACORA_FIRMA2.FECHAAUT,NULL,0,(DGI.F_DIAS_LABORABLES_REP_POA(BITACORA_FIRMA3.FECHAAUT,BITACORA_FIRMA4.FECHAAUT))) AS DURACION_AutPpto
FROM
POA.MOVOFICIOS MOV0,
POA.BITMOVTOS B,
DGI.DGI_USR U,

(SELECT C.YEAR AS C_ANIO,C.RAMO AS C_RAMO,M.ID_CARATULA AS C_ID,M.OFICIO AS C_OFICIO FROM
POA.CARATULA C,
POA.CARATULA_MOVOFICIO M
WHERE
C.ID_CARATULA=M.ID_CARATULA) CARATULA,
POA.VW_MOVOFICIOS_RAMO_AFECTADO AFECTADOS,
POA.ESTATUS_MOV STS,
(
SELECT
	DECODE(SIGN(SUM(IMPACPROG.CONT)),1,'S','N') REL_IMP_PROG,
    IMPACPROG.OFICIO
FROM
	(
		SELECT
			nvl(count(ROWNUM),0) CONT,
            MOVACC.OFICIO AS OFICIO
		FROM
			POA.MOVOFICIOS_ACCION MOVACC
		GROUP BY
            MOVACC.OFICIO

		UNION ALL

		SELECT
			nvl(count(ROWNUM),0) CONT,
            MOVMET.OFICIO AS OFICIO
		FROM
			POA.MOVOFICIOS_META MOVMET
            GROUP BY
            MOVMET.OFICIO

		UNION ALL

		SELECT
			nvl(count(ROWNUM),0) CONT,
            MOVEST.OFICIO AS OFICIO
		FROM
			POA.MOVOFICIOS_ESTIMACION MOVEST
            GROUP BY
            MOVEST.OFICIO

		UNION ALL

		SELECT
			nvl(count(ROWNUM),0) CONT,
            MOVACCEST.OFICIO AS OFICIO
		FROM
			POA.MOVOFICIOS_ACCION_ESTIMACION MOVACCEST
            GROUP BY
            MOVACCEST.OFICIO

