POA.MOVOFICIOS MOV,
POA.FLUJO_FIRMAS F,
POA.BITMOVTOS B,
POA.ESTATUS_MOV E,
DGI.PARAMETROS P,
DGI.TIPOFICIO TF
WHERE

MOV.TIPOMOV=FA.TIPOMOV AND
FA.TIPO_USR=F.TIPO_USR AND
F.ORDEN=FA.ORDEN AND
FA.TIPO_USR=$P{TIPOUSR} AND
TF.OFICIO=MOV.OFICIO AND
TF.OFICIO=B.OFICIO(+) AND
--TF.TIPO=B.TIPO_OFICIO(+) AND
E.ESTATUS=FA.ESTATUS AND
F.APP_LOGIN=B.APP_LOGIN AND
F.TIPO_FLUJO=B.TIPO_FLUJO AND
E.ESTATUS NOT IN('A') AND
MOV.INVERSION='S'

) BITACORA

WHERE
BITACORA.ORDEN=3
) BITACORA_FIRMA3,
(
SELECT
    OFICIO,
    FECHAAUT,
    APP_LOGIN,
    DESCR
    FROM

(
SELECT
    MOV.OFICIO AS OFICIO,
    MOV.FECPPTO AS FECPPTO,
    0 AS NIVEL,
    0 ORDEN,
    0 TIPO_FLUJO ,
    ''  AS TIPO_USR,
    0  AS TIPO_FLUJO1,
    ''  AS TIPO_USR1,
    0  AS ORDEN1,
    MOV.FECPPTO AS FECHAAUT,
    MOV.APP_LOGIN,
    'Envio a autorizacion' DESCR,
    P.GOBIERNO,
    P.SECRETARIA,
    '' TIPOFICIO,
    0 AS MAXIMO,
    0 AS AGRUPA
FROM POA.MOVOFICIOS MOV, DGI.PARAMETROS P

UNION ALL
/*EN BITACORA*/
SELECT MOV.OFICIO,MOV.FECPPTO,FA.NIVEL,FA.ORDEN,FA.TIPO_FLUJO,FA.TIPO_USR,F.TIPO_FLUJO AS TIPO_FLUJO1,F.TIPO_USR AS TIPO_USR1,F.ORDEN AS ORDEN1,B.FECHAAUT AS FECHAAUT,B.APP_LOGIN,E.DESCR,P.GOBIERNO,P.SECRETARIA,
CASE WHEN
FA.ORDEN=(SELECT  MAX(FM.ORDEN - 1)
FROM
POA.FLUJO_AUTORIZACION FM
WHERE FM.TIPO_USR=$P{TIPOUSR} AND
FM.TIPOMOV=MOV.TIPOMOV)
THEN TF.TIPO
ELSE
'0'
END AS TIPOFICIO,
CASE WHEN
FA.ORDEN=(SELECT  MAX(FM.ORDEN - 1) AS MAXIMO
FROM
POA.FLUJO_AUTORIZACION FM
WHERE FM.TIPO_USR=$P{TIPOUSR} AND
FM.TIPOMOV=MOV.TIPOMOV)
THEN FA.ORDEN
ELSE
0
END AS MAXIMO,
0 AS AGRUPA
FROM
POA.FLUJO_AUTORIZACION FA,
POA.MOVOFICIOS MOV,
POA.FLUJO_FIRMAS F,
POA.BITMOVTOS B,
POA.ESTATUS_MOV E,
DGI.PARAMETROS P,
DGI.TIPOFICIO TF
WHERE

MOV.TIPOMOV=FA.TIPOMOV AND
FA.TIPO_USR=F.TIPO_USR AND
F.ORDEN=FA.ORDEN AND
FA.TIPO_USR=$P{TIPOUSR} AND
TF.OFICIO=MOV.OFICIO AND
TF.OFICIO=B.OFICIO(+) AND
TF.TIPO=B.TIPO_OFICIO(+) AND
E.ESTATUS=FA.ESTATUS AND
F.APP_LOGIN=B.APP_LOGIN AND
F.TIPO_FLUJO=B.TIPO_FLUJO AND
E.ESTATUS NOT IN('A') AND
MOV.INVERSION<>'S'

UNION

SELECT MOV.OFICIO,MOV.FECPPTO,FA.NIVEL,FA.ORDEN,FA.TIPO_FLUJO,FA.TIPO_USR,F.TIPO_FLUJO AS TIPO_FLUJO1,F.TIPO_USR AS TIPO_USR1,F.ORDEN AS ORDEN1,B.FECHAAUT AS FECHAAUT,B.APP_LOGIN,E.DESCR,P.GOBIERNO,P.SECRETARIA,
CASE WHEN
FA.ORDEN=(SELECT  MAX(FM.ORDEN - 1)
FROM
POA.FLUJO_AUTORIZACION FM
WHERE FM.TIPO_USR=$P{TIPOUSR} AND
FM.TIPOMOV=MOV.TIPOMOV)
THEN TF.TIPO
ELSE
'0'
END AS TIPOFICIO,
CASE WHEN
FA.ORDEN=(SELECT  MAX(FM.ORDEN - 1) AS MAXIMO
FROM
POA.FLUJO_AUTORIZACION FM
WHERE FM.TIPO_USR=$P{TIPOUSR} AND
FM.TIPOMOV=MOV.TIPOMOV)
THEN FA.ORDEN
ELSE
0
END AS MAXIMO,
0 AS AGRUPA
FROM
POA.FLUJO_AUTORIZACION FA,
POA.MOVOFICIOS MOV,
POA.FLUJO_FIRMAS F,
POA.BITMOVTOS B,
POA.ESTATUS_MOV E,
DGI.PARAMETROS P,
DGI.TIPOFICIO TF
WHERE

MOV.TIPOMOV=FA.TIPOMOV AND
FA.TIPO_USR=F.TIPO_USR AND
F.ORDEN=FA.ORDEN AND
FA.TIPO_USR=$P{TIPOUSR} AND
TF.OFICIO=MOV.OFICIO AND
TF.OFICIO=B.OFICIO(+) AND
--TF.TIPO=B.TIPO_OFICIO(+) AND
E.ESTATUS=FA.ESTATUS AND
F.APP_LOGIN=B.APP_LOGIN AND
F.TIPO_FLUJO=B.TIPO_FLUJO AND
E.ESTATUS NOT IN('A') AND
MOV.INVERSION='S'
) BITACORA

WHERE
BITACORA.ORDEN=4
) BITACORA_FIRMA4




WHERE
MOV0.OFICIO=BITACORA_FIRMA0.OFICIO(+)
AND MOV0.OFICIO=BITACORA_FIRMA1.OFICIO(+)
AND MOV0.OFICIO=BITACORA_FIRMA2.OFICIO(+)
AND MOV0.OFICIO=BITACORA_FIRMA3.OFICIO(+)
AND MOV0.OFICIO=BITACORA_FIRMA4.OFICIO(+)
AND MOV0.OFICIO=IMPACPROG.OFICIO(+)
AND MOV0.OFICIO=B.OFICIO
AND TRIM(U.APP_LOGIN)=TRIM(MOV0.APP_LOGIN)
AND MOV0.OFICIO=AFECTADOS.OFICIO
AND AFECTADOS.OFICIO=CARATULA.C_OFICIO(+)
AND AFECTADOS.RAMO=CARATULA.C_RAMO(+)
AND AFECTADOS.YEAR=CARATULA.C_ANIO(+)
AND MOV0.STATUS=STS.ESTATUS
$P!{QUERY}]]>
