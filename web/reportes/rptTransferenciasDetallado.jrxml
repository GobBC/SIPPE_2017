	<parameter name="IMG_DIR" class="java.lang.String"/>
	<parameter name="OFICIO" class="java.lang.String"/>
	<parameter name="TIPO" class="java.lang.String"/>
	<parameter name="YEAR" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT OFI.CONSEC as consec,
        TRAN.RAMO,
        TRAN.PRG ,
        TRAN.DEPTO,
        TRAN.PARTIDA,
        TRAN.IMPTE as importetran,
         REC.RAMO,
         REC.PRG,
         REC.DEPTO,
         REC.PARTIDA,
         REC.IMPTE as importerec,
         PARTIDA_A.DESCR as descrtran,
         PARAM.GOBIERNO,
         PARAM.SECRETARIA,
         rpad(PARTIDA_B.DESCR,60 ) as descrrec,
        TRAN.STATUS,
         MOVS.FECPPTO  ,
			PARAM.LOGO,
		TRAN.RAMO||'-'||TRAN.DEPTO||'-'||TRAN.PRG||'-'||TRAN.TIPO_PROY||LPAD(TRAN.PROYECTO, PLC.PROYECTO, '0')||'-'|| LPAD(TRAN.META, PLC.META,
   '0')||'-'||LPAD(TRAN.ACCION, PLC.ACCION, '0')||'-'||TRAN.PARTIDA||'-'||TRAN.TIPO_GASTO||'-'|| LPAD(TRAN.FUENTE, PLC.FUENTE, '0')||LPAD(
   TRAN.FONDO, PLC.FONDO, '0')||LPAD(TRAN.RECURSO, PLC.RECURSO, '0')||'-'|| TRAN.REL_LABORAL as codigotran,
		REC.RAMO||'-'||REC.DEPTO||'-'||REC.PRG||'-'||REC.TIPO_PROY||LPAD(REC.PROYECTO, PLC.PROYECTO, '0')||'-'|| LPAD(REC.META, PLC.META,
   '0')||'-'||LPAD(REC.ACCION, PLC.ACCION, '0')||'-'||REC.PARTIDA||'-'||REC.TIPO_GASTO||'-'|| LPAD(REC.FUENTE, PLC.FUENTE, '0')||LPAD(
   REC.FONDO, PLC.FONDO, '0')||LPAD(REC.RECURSO, PLC.RECURSO, '0')||'-'|| REC.REL_LABORAL as codigorec,
	MOVS.JUSTIFICACION
    FROM DGI.OFICONS OFI,
         SPP.TRANSFERENCIAS TRAN,
         SPP.TRANSFREC REC,
         DGI.PARTIDA PARTIDA_A,
         DGI.PARTIDA PARTIDA_B,
         DGI.PARAMETROS PARAM,
         POA.MOVOFICIOS MOVS,
	DGI.PARAM_LONG_CODIGO PLC
   WHERE (TRAN.OFICIO = OFI.OFICIO ) and
         ( REC.OFICIO =TRAN.OFICIO ) and
         ( REC.CONSEC =TRAN.CONSEC ) and
         (TRAN.PARTIDA = PARTIDA_A.PARTIDA ) and
         ( REC.PARTIDA = PARTIDA_B.PARTIDA ) and
         ( OFI.CONSEC =TRAN.CONSEC ) and
         (TRAN.OFICIO = MOVS.OFICIO ) and
         ( PARTIDA_A.YEAR = PARTIDA_B.YEAR ) and
         ( ( OFI.OFICIO = $P{OFICIO} ) AND
         ( OFI.TIPO = $P{TIPO} )) AND
         PARTIDA_A.YEAR = $P{YEAR} AND
	PLC.YEAR = PARTIDA_A.YEAR
		]]>
	</queryString>
