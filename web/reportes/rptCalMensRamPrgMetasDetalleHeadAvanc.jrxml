	</parameter>
	<queryString>
		<![CDATA[SELECT
    $P{PRG},
    NVL(EST.VALOR,0) AS VALOR,
    ESTPER.PERIODO,
    MET.META,
    MET.DESCR META_DESCR,
    MEDMET.DESCR MEDIDAMETA_DESCR,
    MET.CALCULO,
    NVL(AVANC.VALOR,0) AS AVANCE_VALOR,
        (
        SELECT
            DECODE(MET.CALCULO,'AC',NVL(SUM(NVL(EST.VALOR,0)),0),DECODE(MET.CALCULO,'MI',MIN(EST.VALOR),MAX(EST.VALOR)))
		    FROM
                DGI.ESTIMACION EST
		    WHERE
                EST.META = $P{META} AND
                EST.RAMO = $P{RAMO} AND
			    EST.YEAR = $P{YEAR}
    ) VALOR_MAX,
    (SELECT
    DECODE(MET.CALCULO,'AC',NVL(SUM(NVL(AVANC.VALOR,0)),0),DECODE(MET.CALCULO,'MI',MIN(AVANC.VALOR),MAX(AVANC.VALOR)))
	FROM
        DGI.AVANCE AVANC
	WHERE
        AVANC.META = $P{META} AND
        AVANC.RAMO = $P{RAMO} AND
	    AVANC.YEAR =  $P{YEAR}
) VALOR_MAX_AVANCE
    FROM
        DGI.ESTIMACION EST,
        S_POA_META MET,
        DGI.AVANCE  AVANC,
		S_POA_MEDIDA_META MEDMET,
		DGI.ESTIMACION_PERIODO ESTPER
    WHERE
        EST.meta (+) = MET.meta AND
        EST.ramo (+) = MET.ramo AND
        EST.year (+) = MET.year AND
        AVANC.meta(+) = EST.meta AND
        AVANC.ramo(+) = EST.ramo AND
        AVANC.year(+) = EST.year AND
        AVANC.periodo(+) = EST.periodo AND
		MET.CVE_MEDIDA = MEDMET.MEDIDA  AND
		MET.YEAR = MEDMET.YEAR  AND
        EST.PERIODO = ESTPER.PERIODO AND
        MET.RAMO = $P{RAMO}  AND
        MET.YEAR = $P{YEAR} AND
        MET.META =  $P{META}
    GROUP BY
        EST.VALOR,
		ESTPER.PERIODO,
        MET.META,
        MET.DESCR,
        MEDMET.DESCR,
        MET.CALCULO,
        AVANC.VALOR
    ORDER BY
        ESTPER.PERIODO]]>
	</queryString>
