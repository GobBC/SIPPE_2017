	<queryString>
		<![CDATA[SELECT
	POA.PROYECTO.TIPO_PROY,
	POA.PROYECTO.PROY,
	PP.DESCR AS PY_DES,
	E.PATERNO ||'  '||E.MATERNO ||'  '||E.NOMBRE AS LIDER
FROM
	POA.PROYECTO,
	DGI.VW_SP_PROY_ACT PP,
	DGI.VW_EMPLEADOS E
WHERE
	POA.PROYECTO.PROY = PP.PROY AND
	POA.PROYECTO.TIPO_PROY = PP.TIPO_PROY AND
	POA.PROYECTO.YEAR = PP.YEAR AND
	POA.PROYECTO.RAMO = $P{RAMO} AND
	POA.PROYECTO.PRG = $P{PROG} AND
	POA.PROYECTO.TIPO_PROY = $P{TIPO_PROY} AND
	POA.PROYECTO.PROY = $P{PROY} AND
	POA.PROYECTO.YEAR = $P{YEAR} AND
	POA.PROYECTO.RFC = E.RFC(+) AND
	POA.PROYECTO.HOMOCLAVE = E.HOMOCLAVE(+)
ORDER BY
	POA.PROYECTO.TIPO_PROY]]>
	</queryString>
	<field name="TIPO_PROY" class="java.lang.String"/>
	<field name="PROY" class="java.math.BigDecimal"/>
	<field name="PY_DES" class="java.lang.String"/>
	<field name="LIDER" class="java.lang.String"/>
