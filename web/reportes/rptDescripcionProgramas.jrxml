	<queryString>
		<![CDATA[SELECT
       R.RAMO,
       R.RAMO || ' - ' || R.DESCR AS RAMO_DESCR,
       P.PRG,
       P.PRG || ' - ' || P.DESCR AS PRG_DESCR,
       RP.OBJETIVO AS OBJETIVOS_PROGRAMA,-- Se modifico antes era  P.OBJETIVO
       RP.RESULTADO AS RESULTADO_PROGRAMA,-- Se modifico antes era  P.RESULTADO
       F.NOMBRE||' - '||F.DESCR AS FIN,
       RP.PROPOSITO, -- Se modifico antes era  P.PROPOSITO
       PA.GOBIERNO,
       PA.SECRETARIA,
       PA.LOGO,
       SYSDATE AS HOY

FROM
       S_POA_PROGRAMA P,
       DGI.RAMOS R,
       POA.RAMO_PROGRAMA RP,
       DGI.PARAMETROS PA,
       POA.RAMO_FIN RF,
       S_POA_FIN F

WHERE
       R.RAMO = RP.RAMO
       AND P.PRG = RP.PRG
       AND P.YEAR = R.YEAR
       AND RF.RAMO(+) = RP.RAMO
       AND RF.YEAR(+) = RP.YEAR
       AND RF.FIN(+) = RP.FIN
       AND F.FIN(+) = RF.FIN
       AND F.YEAR(+) = RF.YEAR
       AND RP.YEAR = $P{YEAR}
       AND RP.RAMO = $P{RAMO_INI}
       AND RP.PRG = $P{PROGRAMA_INI}
       AND R.YEAR = RP.YEAR]]>
	</queryString>
