	<parameter name="RAMO" class="java.lang.String"/>
	<parameter name="PROY" class="java.lang.String"/>
	<parameter name="PROG" class="java.lang.String"/>
	<parameter name="YEAR" class="java.lang.String"/>
	<parameter name="TIPO" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
    PRO.TIPO_PROY,
    PRO.PROY,
    PP.DESCR PROY_DESC,
    PP.DESCR SECTOR_DESC,
    E.PATERNO ||'  '||E.MATERNO ||'  '||E.NOMBRE AS LIDER,
    PRO.PRG,
    PRO.DEPTO_RESP,
    DEP.DESCR AS DEPTO_DESC
FROM
    POA.PROYECTO PRO,
    DGI.DEPENDENCIA DEP,
    DGI.VW_EMPLEADOS E,
    DGI.VW_SP_PROY_ACT PP
WHERE
    PRO.YEAR = $P{YEAR} AND
    PRO.RAMO = $P{RAMO} AND
    PRO.PRG = $P{PROG} AND
    PRO.TIPO_PROY = $P{TIPO} AND
    PRO.PROY = $P{PROY} AND
    DEP.YEAR = PRO.YEAR AND
    DEP.RAMO = PRO.RAMO AND
    DEP.DEPTO (+) = PRO.DEPTO_RESP AND
    E.RFC (+) = PRO.RFC AND
    E.HOMOCLAVE (+)= PRO.HOMOCLAVE AND
    PP.YEAR = PRO.YEAR AND
    PP.TIPO_PROY = PRO.TIPO_PROY AND
    PP.PROY = PRO.PROY
ORDER BY
	PRO.TIPO_PROY,PRO.PROY]]>
	</queryString>
