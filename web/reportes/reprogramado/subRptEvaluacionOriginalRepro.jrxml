	<property name="ireport.zoom" value="1.1000000000000005"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<style name="Crosstab Data Text" hAlign="Center"/>
	<parameter name="META" class="java.math.BigDecimal"/>
	<parameter name="YEAR" class="java.lang.String"/>
	<parameter name="RAMO" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT 14,1,DGI.ESTIMACION_PERIODO.EVALUACION,
DECODE(MET.CALCULO,'AC',NVL(NVL(B.VALOR,0),0),DECODE(MET.CALCULO,'MI',NVL(MIN(B.VALOR),0),NVL(MAX(B.VALOR),0))) AS VALOR,
MET.DESCR AS META_DESC,
MED.DESCR AS UNIDAD_DESC,
(DECODE(MET.CALCULO,'AC',NVL(SUM(NVL(A.VALOR_ANUAL,0))/3,0),DECODE(MET.CALCULO,'MI',MIN(A.VALOR_ANUAL),MAX(A.VALOR_ANUAL)))) AS VALOR_ANUAL,
MET.CALCULO,
MET.META

FROM
        (
        SELECT
        DECODE(MET.CALCULO,'AC',NVL(SUM(NVL(DGI.ESTIMACION.VALOR,0)),0),DECODE(MET.CALCULO,'MI',MIN(DGI.ESTIMACION.VALOR),MAX(DGI.ESTIMACION.VALOR))) AS VALOR_ANUAL
        FROM
        DGI.ESTIMACION,
        S_POA_META MET,
        DGI.ESTIMACION_PERIODO
        WHERE
        ( DGI.estimacion.meta (+) = MET.META) and
        ( DGI.estimacion.ramo (+) = MET.RAMO) and
        ( DGI.estimacion.year (+) = MET.YEAR) and
        ( DGI.ESTIMACION.PERIODO = DGI.ESTIMACION_PERIODO.PERIODO) AND
        DGI.ESTIMACION.VALOR <> 0 and
        ( ( MET.RAMO = $P{RAMO} ) AND
( MET.YEAR = $P{YEAR} ) AND
( MET.META = $P{META}) )


        GROUP BY
        DGI.ESTIMACION_PERIODO.EVALUACION,
        MET.META,
        MET.CALCULO

        ORDER BY
        MET.META,
        DGI.ESTIMACION_PERIODO.EVALUACION ) A,
(SELECT DGI.ESTIMACION_PERIODO.EVALUACION,
        DECODE(MET.CALCULO,'AC',NVL(SUM(NVL(DGI.ESTIMACION.VALOR,0)),0),DECODE(MET.CALCULO,'MI',MIN(DGI.ESTIMACION.VALOR),MAX(DGI.ESTIMACION.VALOR))) AS VALOR
        FROM
        DGI.ESTIMACION,
        S_POA_META MET,
        DGI.ESTIMACION_PERIODO
        WHERE
        ( DGI.estimacion.meta (+) = MET.META) and
        ( DGI.estimacion.ramo (+) = MET.RAMO) and
        ( DGI.estimacion.year (+) = MET.YEAR) and
        ( DGI.ESTIMACION.PERIODO = DGI.ESTIMACION_PERIODO.PERIODO) AND
               DGI.ESTIMACION.VALOR <> 0 and
        ( ( MET.RAMO = $P{RAMO} ) AND
( MET.YEAR = $P{YEAR} ) AND
( MET.META = $P{META}) )


        GROUP BY
        DGI.ESTIMACION_PERIODO.EVALUACION,
        MET.META,
        MET.CALCULO

        ORDER BY
        MET.META,
        DGI.ESTIMACION_PERIODO.EVALUACION) B,
DGI.ESTIMACION,
S_POA_META MET,
S_POA_MEDIDA_META MED,
DGI.ESTIMACION_PERIODO

WHERE
  ( ( MET.RAMO = $P{RAMO} ) AND
( MET.YEAR = $P{YEAR} ) AND
( MET.META = $P{META}) )AND
B.EVALUACION(+)=DGI.ESTIMACION_PERIODO.EVALUACION AND
DGI.estimacion.meta (+) = MET.META and
DGI.estimacion.ramo (+) = MET.RAMO and
DGI.estimacion.year (+) = MET.YEAR and
MED.MEDIDA = MET.CVE_MEDIDA and
MED.YEAR = MET.YEAR  and
DGI.ESTIMACION.PERIODO = DGI.ESTIMACION_PERIODO.PERIODO

GROUP BY
DGI.ESTIMACION_PERIODO.EVALUACION,
MET.META,
MET.DESCR,
MET.CALCULO,
MED.DESCR,
B.VALOR

ORDER BY
MET.META,
DGI.ESTIMACION_PERIODO.EVALUACION]]>
	</queryString>
