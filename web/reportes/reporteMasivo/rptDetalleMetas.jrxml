	<queryString>
		<![CDATA[SELECT
	YEAR ,
	RAMO,
	RAMO_DESCR,
	PRG,
	PRG_DESCR,
	PROY,
	DESCR,
	TIPO_PROY,
	META,
	META_DESCR,
	ACCION,
	ACCION_DESCR,
	PARTIDA,
	PATIDA_DESCR,
	REQUERIMIENTO,
	REQUERIMIENTO_DESCR,
	CANTIDAD,
	COSTO_UNITARIO,
	COSTO_ANUAL,
	LOCALIDAD,
	LOC_DESCR,
	MPO,
	MPO_DESCR,
	FUENTE||'.'||FONDO||'.'||RECURSO AS FUENTE_FINANCIAMIENTO,
	NVL(FUENTE_FINANCIAMIENTO_DESCR,' ') AS FUENTE_FINANCIAMIENTO_DESCR,
	ENE,
	FEB,
	MAR,
	ABR,
	MAY,
	JUN,
	JUL,
	AGO,
	SEP,
	OCT,
	NOV,
	DIC,
	HOY,
	GOBIERNO,
	SECRETARIA,
	LOGO,
	DEPTO,
	DEPTO_DESCR,
	LINEA,
	NOTA,
	MEDIDA,
	MEDIDA_META

FROM
	(
		SELECT
			DISTINCT
				A.YEAR ,
				A.RAMO,
				A.RAMO_DESCR,
				A.PRG,
				A.PRG_DESCR,
				A.PROY,
				A.DESCR,
				A.TIPO_PROY,
				A.META,
				A.META_DESCR,
				A.ACCION,
				A.ACCION_DESCR,
				AR.PARTIDA,
				AR.PARTIDA ||' '||(SELECT MAX(DESCR) FROM DGI.PARTIDA P WHERE P.YEAR = AR.YEAR AND P.PARTIDA = AR.PARTIDA) AS PATIDA_DESCR,
				AR.REQUERIMIENTO,
				AR.DESCR AS REQUERIMIENTO_DESCR,
				AR.CANTIDAD,
				AR.COSTO_UNITARIO,
				AR.COSTO_ANUAL,
				AR.FUENTE,
				AR.FONDO,
				AR.RECURSO,
				A.LOCALIDAD,
				A.LOC_DESCR,
				A.MPO,
				A.MPO_DESCR,
				NVL(AR.ENE,0) AS ENE,
				NVL(AR.FEB,0) AS FEB,
				NVL(AR.MAR,0) AS MAR,
				NVL(AR.ABR,0) AS ABR,
				NVL(AR.MAY,0) AS MAY,
				NVL(AR.JUN,0) AS JUN,
				NVL(AR.JUL,0) AS JUL,
				NVL(AR.AGO,0) AS AGO,
				NVL(AR.SEP,0) AS SEP,
				NVL(AR.OCT,0) AS OCT,
				NVL(AR.NOV,0) AS NOV,
				NVL(AR.DIC,0) AS DIC,
				A.HOY,
				A.GOBIERNO,
				A.SECRETARIA,
				A.LOGO,
				A.DEPTO,
				A.DEPTO_DESCR,
				A.LINEA,
				A.NOTA,
				A.MEDIDA,
				A.MEDIDA_META ,
				NVL(REC.DESCR,' ') AS FUENTE_FINANCIAMIENTO_DESCR
			FROM
				(
					SELECT
						DISTINCT
							R.YEAR ,
							R.RAMO,
							R.RAMO || ' - ' || R.DESCR AS RAMO_DESCR,
							P.PRG,
							P.PRG || ' - ' || P.DESCR AS PRG_DESCR,
							PRO.PROY,
							PP.DESCR,
							PRO.TIPO_PROY,
							M.META,
							M.META || ' - ' || M.DESCR AS META_DESCR,
							A.ACCION,
							A.ACCION || ' - ' || A.DESCR AS ACCION_DESCR,
							A.LOCALIDAD,
							LOC.DESCR AS LOC_DESCR,
							A.MPO,
							MUN.NOMBREMPO AS MPO_DESCR,
							SYSDATE AS HOY,
							PARAM.GOBIERNO,
							PARAM.SECRETARIA,
							PARAM.LOGO,
							A.DEPTO,
							D.DESCR AS DEPTO_DESCR,
							A.LINEA,
							DECODE(M.PRESUPUESTAR, 'N','META NO COSTEADA',NULL) AS NOTA,
							MM.DESCR AS MEDIDA,
							UMM.DESCR AS MEDIDA_META
						FROM
							DGI.RAMOS R,
							S_POA_PROGRAMA P,
							POA.RAMO_PROGRAMA RP,
							DGI.DEPENDENCIA D,
							DGI.VW_SP_PROY_ACT PP,
							S_POA_META M,
							POA.PROYECTO PRO,
							S_POA_ACCION A,
							S_POA_MEDIDA_META MM,
							DGI.PARAMETROS PARAM,
							S_POA_MEDIDA_META UMM,
							S_POA_LOCALIDAD LOC,
							S_POA_MUNICIPIO MUN
						WHERE
							RP.RAMO = R.RAMO AND
							( PRO.PROY = PP.PROY ) AND
							( PRO.TIPO_PROY = PP.TIPO_PROY ) AND
							( PRO.YEAR = PP.YEAR ) AND
							PRO.PROY = PRO.PROY AND
							PRO.RAMO = R.RAMO AND
							PRO.PRG = P.PRG AND
							PRO.YEAR = R.YEAR AND
							P.YEAR = R.YEAR AND
							A.YEAR = D.YEAR AND
							A.RAMO = D.RAMO AND
							A.DEPTO = D.DEPTO AND
							M.RAMO = R.RAMO AND
							M.PRG = P.PRG AND
							M.YEAR = PRO.YEAR AND
							M.PROY = PRO.PROY(+) AND
							M.TIPO_PROY = PRO.TIPO_PROY(+) AND
							MM.YEAR(+) = A.YEAR AND
							MM.MEDIDA(+) = A.CVE_MEDIDA AND
							UMM.YEAR(+) = M.YEAR AND
							UMM.MEDIDA(+) = M.CVE_MEDIDA AND
							A.META(+) = M.META AND
							A.RAMO(+) = M.RAMO AND
							A.YEAR(+) = M.YEAR AND
							RP.PRG = P.PRG AND
							RP.YEAR = R.YEAR AND
							R.YEAR = $P{YEAR} AND
							MUN.MPO = A.MPO AND
							LOC.LOCALIDAD = A.LOCALIDAD AND
							LOC.MPO = MUN.MPO AND
							$P!{QUERY}
				)A,
				POA.ACCION_REQ AR,
				S_POA_RECURSO REC
			WHERE
				AR.META(+) = A.META AND
				AR.RAMO(+) = A.RAMO AND
				AR.YEAR(+) = A.YEAR AND
				AR.ACCION(+) = A.ACCION AND
				AR.YEAR (+) = A.YEAR AND
				AR.PRG (+) = A.PRG AND
				REC.YEAR (+)= AR.YEAR AND
				REC.FUENTE (+)= AR.FUENTE AND
				REC.FONDO (+)= AR.FONDO AND
				REC.RECURSO (+)= AR.RECURSO
			ORDER BY
				A.RAMO,
				A.PRG, A.PROY,
				A.META,A.ACCION,
				AR.REQUERIMIENTO, AR.PARTIDA
	)]]>
	</queryString>
