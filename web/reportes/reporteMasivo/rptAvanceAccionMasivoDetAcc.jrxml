	<parameter name="YEAR" class="java.lang.Integer"/>
	<parameter name="RAMO" class="java.lang.String"/>
	<parameter name="META" class="java.math.BigDecimal">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="ACCION" class="java.math.BigDecimal">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT SUM(GRAL.ASIGNADO)AS AUTORIZADO,(SUM(GRAL.ASIGNADO)+SUM(GRAL.AMP)+SUM(GRAL.RED)) AS MODIFICADO, SUM(GRAL.DEV)AS DEVENGADO_CIM,(SUM(GRAL.ASIGNADO) - SUM(GRAL.DEV)) AS DIFERENCIA  FROM
        (
        SELECT SUM(PPTO.ASIGNADO) AS ASIGNADO,
        0 AS DEV,
        0 AS AMP,
        0 AS RED
        FROM
        SPP.PPTO PPTO,
        S_POA_ACCION A
        WHERE
        A.YEAR=$P{YEAR}
        AND A.RAMO=$P{RAMO}
        AND A.META=$P{META}
        AND A.ACCION=$P{ACCION}
        AND A.YEAR=PPTO.YEAR
        AND A.RAMO=PPTO.RAMO
        AND A.META=PPTO.META
        AND A.ACCION=PPTO.ACCION
        UNION
        SELECT 0 AS ASIGNADO,
        SUM(CIM.DEV) AS DEV,
        SUM(CIM.AMP) AS AMP,
        SUM(CIM.RED) AS RED
        FROM
        S_CIM_CIERRE_PPTO_ARMO_MEN CIM,
        S_POA_ACCION A
        WHERE
        A.YEAR=$P{YEAR}
        AND A.RAMO=$P{RAMO}
        AND A.META=$P{META}
        AND A.ACCION=$P{ACCION}
        AND A.YEAR=CIM.EJERCICIO
        AND A.RAMO=CIM.RAMO
        AND A.META=CIM.META
        AND A.ACCION=CIM.ACCION) GRAL]]>
	</queryString>
