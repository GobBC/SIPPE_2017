	<queryString>
		<![CDATA[SELECT
	DISTINCT
	R.RAMO,
	R.RAMO || ' - ' || R.DESCR AS DESCR_RAMO,
	P.PRG,
	P.PRG || ' - ' || P.DESCR AS DECR_PRG,
	M.META,
	M.META || ' - ' || M.DESCR AS DESCR_META,
	A.ACCION,
	A.ACCION || ' - ' ||REPLACE(REPLACE(A.DESCR,CHR(10),''),CHR(13),'') AS DESCR_ACCION,
	PY.PROY,
	PY.DESCR,
	PY.TIPO_PROY,
	PRO.LIDER,
	RP.YEAR,
	PAA.MES,
	DECODE(PAA.MES,1,'ENERO',2,'FEBRERO',3,'MARZO',4,'ABRIL',5,'MAYO',6,'JUNIO',7,'JULIO',8,'AGOSTO',9,'SEPTIEMBRE',10,'OCTUBRE',11,'NOVIEMBRE',12,'DICIEMBRE') AS DESCR_MES,
	PAA.REALIZADO,
	NVL(PAA.IMPT_ASIG_XMES,0) AS IMPORTE_ASIGNADO,
	NVL(PAA.IMPT_EJER_XMES,0) AS IMPORTE_EJERCIDO,
    NVL(PPTO.ASIGNADO,0) AS IMPORTE_ASIGNADO_PPTO,
    NVL(CIM.EJERCIDO,0) AS EJERCIDO_CIM,
NVL(CIM.AMPLIACION,0) AS AMP_CIM,
    NVL(CIM.REDUCCION,0) AS RED_CIM,
    ((NVL(PPTO.ASIGNADO,0))+NVL(CIM.REDUCCION,0)+NVL(CIM.AMPLIACION,0)) AS MODIFICADO,
       ( (NVL(PPTO.ASIGNADO,0)) - (NVL(CIM.EJERCIDO,0)) ) AS DIFERENCIA,
	PAA.OBSERVACIONES,
	PARAM.GOBIERNO,
	PARAM.SECRETARIA,
	PARAM.LOGO,
	SYSDATE AS HOY,
	PAA.VALOR AS VALOR_ACCION,
	NVL(AE.VALOR,0) AS VALOR_ESTIMACION,
          PARAM.REPWEB_IMPFIRMA

FROM
	POA.RAMO_PROGRAMA RP,
	DGI.RAMOS R,
	S_POA_PROGRAMA P,
	S_POA_META M,
	S_POA_ACCION A,
(SELECT
META AS META,
RAMO AS RAMO,
ACCION AS ACCION,
YEAR,
MES,
MIN(OBSERVACIONES) AS OBSERVACIONES,
SUM(REALIZADO) AS REALIZADO,
SUM(IMPT_ASIG_XMES) AS IMPT_ASIG_XMES,
SUM(IMPT_EJER_XMES) AS IMPT_EJER_XMES,
SUM(VALOR) AS VALOR
FROM
(SELECT
T.META AS META,
T.RAMO AS RAMO,
T.YEAR AS YEAR,
T.ACCION AS ACCION,
T.MES AS MES,
T.REALIZADO AS REALIZADO,
T.IMPT_ASIG_XMES AS IMPT_ASIG_XMES,
T.IMPT_EJER_XMES AS IMPT_EJER_XMES,
T.OBSERVACIONES AS OBSERVACIONES,
T.VALOR AS VALOR  FROM
DGI.POA_AVANCE_ACCION T

UNION
SELECT A.META,A.RAMO,A.YEAR,A.ACCION,MM.MES,'0',0,0,'',0
FROM
S_POA_ACCION A,
DGI.MESES MM
)R
GROUP BY
META,
RAMO,
ACCION,
YEAR,
MES

) PAA
	/*DGI.POA_AVANCE_ACCION PAA*/,
   (SELECT YEAR,RAMO,META,ACCION,MES,SUM(ASIGNADO) AS ASIGNADO
   FROM
   SPP.PPTO
   GROUP BY
   YEAR,RAMO,META,ACCION,MES)PPTO,
	DGI.ACCION_ESTIMACION AE,
	DGI.VW_SP_PROY_ACT PY,
    (SELECT
        EJERCICIO,
        RAMO,META,
        ACCION,MES,
        SUM(DEV) AS DEVENGADO,
        SUM(DEV) AS EJERCIDO,
        SUM(AMP) AS AMPLIACION,
        SUM(RED) AS REDUCCION
        from S_CIM_CIERRE_PPTO_ARMO_MEN
        GROUP BY
        EJERCICIO,RAMO,META,ACCION,MES) CIM,
	(
		SELECT
			P.RAMO
			,P.YEAR
			,P.PRG
			,P.TIPO_PROY
			,P.PROY
			,DECODE(P.RFC,NULL,NULL,EM.NOMBRE||' '||EM.PATERNO||' '||EM.MATERNO) AS LIDER
		FROM
			POA.PROYECTO P
			,DGI.VW_EMPLEADOS EM
		WHERE
			P.RFC = EM.RFC(+) AND
			P.HOMOCLAVE = EM.HOMOCLAVE(+) AND
			P.YEAR = $P{YEAR}
	) PRO,
	DGI.PARAMETROS PARAM

WHERE
	RP.YEAR = $P{YEAR} AND
	R.YEAR = RP.YEAR AND
	R.RAMO = RP.RAMO AND
	P.YEAR = RP.YEAR AND
	P.PRG = RP.PRG AND
	M.YEAR = RP.YEAR AND
	M.RAMO = RP.RAMO AND
	M.PRG = RP.PRG AND
	PY.YEAR = M.YEAR AND
	PY.PROY = M.PROY AND
	PY.TIPO_PROY = M.TIPO_PROY AND
	PRO.YEAR (+)= M.YEAR AND
	PRO.RAMO = M.RAMO AND
	PRO.PRG = M.PRG AND
	PRO.PROY (+)= M.PROY AND
	PRO.TIPO_PROY (+)= M.TIPO_PROY AND
	A.YEAR = M.YEAR AND
	A.RAMO = M.RAMO AND
	A.PRG = M.PRG AND
	A.META = M.META AND
	PAA.YEAR (+)= A.YEAR AND
	PAA.META (+)= A.META AND
	PAA.RAMO (+)= A.RAMO AND
	PAA.ACCION (+)= A.ACCION AND
	AE.YEAR (+)= PAA.YEAR AND
	AE.ACCION (+)= PAA.ACCION AND
	AE.RAMO (+)= PAA.RAMO AND
	AE.META (+)= PAA.META AND
	AE.PERIODO (+)= PAA.MES AND
   PPTO.YEAR(+) =PAA.YEAR AND
    PPTO.RAMO(+) =PAA.RAMO AND
    PPTO.META(+) =PAA.META AND
    PPTO.ACCION(+) =PAA.ACCION AND
    PPTO.MES(+) = PAA.MES AND
    CIM.EJERCICIO(+)=PAA.YEAR AND
    CIM.RAMO(+)=PAA.RAMO AND
    CIM.META(+)=PAA.META AND
    CIM.ACCION(+)=PAA.ACCION AND
    CIM.MES(+)=PAA.MES AND
    M.PERIODO>=1 AND M.PERIODO<=12 AND
    A.PERIODO>=1 AND A.PERIODO<=12 AND
 $P!{QUERY}


ORDER BY
	R.RAMO,
	P.PRG,
	M.META,
	A.ACCION,
	PAA.MES]]>
	</queryString>
