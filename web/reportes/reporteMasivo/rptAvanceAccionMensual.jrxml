             (  SELECT YEAR,RAMO,META,ACCION,PERIODO,SUM(VALOR) AS VALOR
                FROM (  SELECT T.YEAR,T.RAMO,T.META,T.ACCION,T.MES AS PERIODO,T.VALOR FROM DGI.POA_AVANCE_ACCION T WHERE T.YEAR = $P{YEAR} AND T.RAMO IN ($P!{RAMO_IN_LIST}) UNION
                        SELECT A.YEAR,A.RAMO,A.META,A.ACCION,MM.MES AS PERIODO,0 FROM S_POA_ACCION A, DGI.MESES MM WHERE A.YEAR = $P{YEAR} AND A.RAMO IN ($P!{RAMO_IN_LIST})  )R
