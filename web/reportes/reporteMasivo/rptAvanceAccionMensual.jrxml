        FROM S_POA_ACCION A,
             (  SELECT YEAR,RAMO,META,ACCION,PERIODO,SUM(VALOR) AS VALOR
                FROM (  SELECT T.YEAR,T.RAMO,T.META,T.ACCION,T.MES AS PERIODO,T.VALOR FROM DGI.POA_AVANCE_ACCION T WHERE T.YEAR = $P{YEAR} AND T.RAMO IN ($P!{RAMO_IN_LIST}) UNION
                        SELECT A.YEAR,A.RAMO,A.META,A.ACCION,MM.MES AS PERIODO,0 FROM S_POA_ACCION A, DGI.MESES MM WHERE A.YEAR = $P{YEAR} AND A.RAMO IN ($P!{RAMO_IN_LIST}) )R
                GROUP BY YEAR,RAMO,META,ACCION,PERIODO ) PAA,
            DGI.ACCION_ESTIMACION AE
        WHERE
            A.YEAR = $P{YEAR} AND A.RAMO IN ($P!{RAMO_IN_LIST}) AND
            PAA.YEAR (+)= A.YEAR AND PAA.RAMO (+)= A.RAMO AND PAA.META (+)= A.META AND PAA.ACCION (+)= A.ACCION AND
            AE.META (+)= PAA.META AND AE.RAMO (+)= PAA.RAMO AND AE.YEAR (+)= PAA.YEAR AND AE.ACCION (+)= PAA.ACCION AND AE.PERIODO (+)= PAA.PERIODO AND
            PAA.PERIODO = 1 ) ENE,
