	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<style name="Crosstab Data Text" hAlign="Center"/>
	<parameter name="YEAR" class="java.lang.Integer"/>
	<parameter name="RAMO" class="java.lang.String"/>
	<parameter name="PRG" class="java.lang.String"/>
	<parameter name="META" class="java.math.BigDecimal">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
    $P{PRG},
	EST.VALOR,
    EST.PERIODO,
    MET.META,
    MET.DESCR DESC_COMP,
    MET.MEDIDA,
    MET.CALCULO,
    MEDMET.DESCR DES_UNIMED,
    DECODE(MET.CALCULO,'AC',NVL(SUM(NVL(A.VALOR_ANUAL,0)),0),
        DECODE(MET.CALCULO,'MI',MIN(A.VALOR_ANUAL),MAX(A.VALOR_ANUAL))) AS VALOR_MAX
    FROM
        DGI.ESTIMACION EST,
        S_POA_META MET,
        S_POA_MEDIDA_META MEDMET,
        (
		SELECT
			DECODE(MET.CALCULO,'AC',NVL(SUM(NVL(DGI.ESTIMACION.VALOR,0)),0),DECODE(MET.CALCULO,'MI',MIN(DGI.ESTIMACION.VALOR),MAX(DGI.ESTIMACION.VALOR))) AS VALOR_ANUAL

		FROM
			DGI.ESTIMACION,
			S_POA_META MET,
			DGI.ESTIMACION_PERIODO

			WHERE
				( DGI.estimacion.meta (+) = MET.META) and
				( DGI.estimacion.ramo (+) = MET.RAMO) and
				( DGI.estimacion.year (+) = MET.YEAR) and
                DGI.ESTIMACION.VALOR <> 0 AND
				( DGI.ESTIMACION.PERIODO = DGI.ESTIMACION_PERIODO.PERIODO) AND
				( ( MET.RAMO = $P{RAMO} ) AND
				( MET.YEAR = $P{YEAR} ) AND
				( MET.META = $P{META} ) )

			GROUP BY
				DGI.ESTIMACION_PERIODO.EVALUACION,
				MET.META,
				MET.CALCULO

			ORDER BY
				MET.META,
				DGI.ESTIMACION_PERIODO.EVALUACION

    ) A
    WHERE
         EST.meta (+) = MET.meta and
         EST.ramo (+) = MET.ramo and
         EST.year (+) = MET.year and
         MET.CVE_MEDIDA = MEDMET.MEDIDA  and
         MET.YEAR = MEDMET.YEAR  and
         MET.RAMO = $P{RAMO}  AND
         MET.YEAR = $P{YEAR} AND
         MET.META = $P{META}
     GROUP BY $P{PRG},
	EST.VALOR,
    EST.PERIODO,
    MET.META,
    MET.DESCR,
    MET.MEDIDA,
    MET.CALCULO,
    MEDMET.DESCR]]>
	</queryString>
