	<queryString>
		<![CDATA[SELECT
	MOVACC.OFICIO,
	MOVACC.RAMO,
	MET.PRG,
	MET.DEPTO,
	MET.TIPO_PROY,
	MET.PROY,
	MOVACC.META,
	MOVACC.ACCION,
	ACC.DESCR ACCION_DESCR,
	MED.DESCR MEDIDA_DESCR

FROM
	POA.MOVOFICIOS_ACCION_ESTIMACION MOVACC,
	S_POA_META MET,
	S_POA_ACCION ACC,
	S_POA_MEDIDA_META MED

WHERE
	MOVACC.YEAR = $P{YEAR} AND
	MOVACC.OFICIO = $P{OFICIO} AND
	MOVACC.RAMO = $P{RAMO} AND
	MET.YEAR = MOVACC.YEAR AND
	MET.RAMO = MOVACC.RAMO AND
	MET.PRG = $P{PRG} AND
	MET.TIPO_PROY = $P{TIPO_PROY} AND
	MET.PROY = $P{PROY} AND
	MET.META = MOVACC.META AND
	ACC.YEAR = MOVACC.YEAR AND
	ACC.RAMO = MOVACC.RAMO AND
	ACC.PRG = $P{PRG} AND
	ACC.META = MOVACC.META AND
	ACC.ACCION = MOVACC.ACCION AND
	ACC.YEAR = MET.YEAR AND
	ACC.RAMO = MET.RAMO AND
	ACC.PRG = MET.PRG AND
	ACC.META = MET.META AND
	MED.YEAR = ACC.YEAR AND
	MED.ACTIVO = 'S' AND
	MED.MEDIDA =  ACC.CVE_MEDIDA

GROUP BY
	MOVACC.OFICIO,
	MOVACC.RAMO,
	MET.PRG,
	MET.DEPTO,
	MET.TIPO_PROY,
	MET.PROY,
	MOVACC.META,
	MOVACC.ACCION,
	ACC.DESCR,
	MED.DESCR

ORDER BY
	MOVACC.OFICIO,
	MOVACC.RAMO,
	MET.PRG,
	MET.DEPTO,
	MET.TIPO_PROY,
	MET.PROY,
	MOVACC.META,
	MOVACC.ACCION,
	ACC.DESCR,
	MED.DESCR]]>
	</queryString>
