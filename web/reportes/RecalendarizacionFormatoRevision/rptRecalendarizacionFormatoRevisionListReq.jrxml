	<queryString>
		<![CDATA[SELECT
	ROWNUM,
	MOVACCREQ.RAMO,
	MOVACCREQ.PRG,
	MOVACCREQ.DEPTO,
	MET.TIPO_PROY,
	MET.PROY,
	MOVACCREQ.META,
	MET.DESCR META_DESCR,
	MOVACCREQ.ACCION,
	ACC.DESCR ACCION_DESCR,
	MOVACCREQ.PARTIDA,
	MOVACCREQ.FUENTE,
	MOVACCREQ.FONDO,
	MOVACCREQ.RECURSO,
	MOVACCREQ.REQUERIMIENTO,
	ACCREQ.DESCR REQUERIMIENTO_DESCR,
	(
		SELECT
			nvl(count(ROWNUM),0)
		FROM
			POA.MOVOFICIOS_ACCION_REQ RECAL,
			S_POA_META META
		WHERE
			RECAL.YEAR = $P{YEAR} AND
			RECAL.RAMO = MOVACCREQ.RAMO AND
			RECAL.PRG = MOVACCREQ.PRG AND
			RECAL.DEPTO = MOVACCREQ.DEPTO AND
			META.TIPO_PROY = MET.TIPO_PROY AND
			META.PROY = MET.PROY AND
			RECAL.META = MOVACCREQ.META AND
			RECAL.ACCION = MOVACCREQ.ACCION AND
			RECAL.PARTIDA = MOVACCREQ.PARTIDA AND
			RECAL.FUENTE = MOVACCREQ.FUENTE AND
			RECAL.FONDO = MOVACCREQ.FONDO AND
			RECAL.RECURSO = MOVACCREQ.RECURSO AND
			META.YEAR = RECAL.YEAR AND
			META.RAMO = RECAL.RAMO AND
			META.PRG = RECAL.PRG AND
			META.META = RECAL.META
	) REGISTROS,
	(
		SELECT
			(
				SUM(PPTO.ENE)+
				SUM(PPTO.FEB)+
				SUM(PPTO.MAR)+
				SUM(PPTO.ABR)+
				SUM(PPTO.MAY)+
				SUM(PPTO.JUN)+
				SUM(PPTO.JUL)+
				SUM(PPTO.AGO)+
				SUM(PPTO.SEP)+
				SUM(PPTO.OCT)+
				SUM(PPTO.NOV)+
				SUM(PPTO.DIC)

			) COSTO_TOTAL_META
		FROM
			SPP.VW_PRESUP_ASIG_MEN  PPTO
		WHERE
			PPTO.YEAR = $P{YEAR} AND
			PPTO.RAMO = MOVACCREQ.RAMO AND
			PPTO.PRG = MOVACCREQ.PRG AND
			PPTO.DEPTO = MOVACCREQ.DEPTO AND
			PPTO.TIPO_PROY = MET.TIPO_PROY AND
			PPTO.PROYECTO = MET.PROY AND
			PPTO.META = MOVACCREQ.META
	) COSTO_TOTAL_META,
	(
		SELECT
			(
				(
					MOVACCREQC.ENE+
					MOVACCREQC.FEB+
					MOVACCREQC.MAR+
					MOVACCREQC.ABR+
					MOVACCREQC.MAY+
					MOVACCREQC.JUN+
					MOVACCREQC.JUL+
					MOVACCREQC.AGO+
					MOVACCREQC.SEP+
					MOVACCREQC.OCT+
					MOVACCREQC.NOV+
					MOVACCREQC.DIC
				)*MOVACCREQC.COSTO_UNITARIO
			) MONTO_RECALENDARIZAR
		FROM
			POA.MOVOFICIOS_ACCION_REQ MOVACCREQC,
			S_POA_META META
		WHERE
			MOVACCREQC.OFICIO = $P{OFICIO} AND
			MOVACCREQC.YEAR = $P{YEAR} AND
			MOVACCREQC.RAMO = MOVACCREQ.RAMO AND
			MOVACCREQC.PRG = MOVACCREQ.PRG AND
			MOVACCREQC.DEPTO = MOVACCREQ.DEPTO AND
			META.TIPO_PROY = MET.TIPO_PROY AND
			META.PROY = MET.PROY AND
			MOVACCREQC.META = MOVACCREQ.META AND
			MOVACCREQC.ACCION = MOVACCREQ.ACCION AND
			MOVACCREQC.PARTIDA = MOVACCREQ.PARTIDA AND
			MOVACCREQC.FUENTE = MOVACCREQ.FUENTE AND
			MOVACCREQC.FONDO = MOVACCREQ.FONDO AND
			MOVACCREQC.RECURSO = MOVACCREQ.RECURSO AND
			MOVACCREQC.REQUERIMIENTO = MOVACCREQ.REQUERIMIENTO AND
			META.YEAR = MOVACCREQC.YEAR AND
			META.RAMO = MOVACCREQC.RAMO AND
			META.PRG = MOVACCREQC.PRG AND
			META.META = MOVACCREQC.META
	) MONTO_RECALENDARIZAR

FROM
	POA.MOVOFICIOS_ACCION_REQ MOVACCREQ,
	S_POA_META MET,
	S_POA_ACCION ACC,
	S_POA_ACCION_REQ ACCREQ
WHERE
	MOVACCREQ.YEAR = $P{YEAR} AND
	MOVACCREQ.OFICIO = $P{OFICIO} AND
	MOVACCREQ.RAMO = $P{RAMO} AND
	MET.YEAR = MOVACCREQ.YEAR AND
	MET.RAMO = MOVACCREQ.RAMO AND
	MET.PRG = MOVACCREQ.PRG AND
	MET.META = MOVACCREQ.META AND
	ACC.YEAR = MOVACCREQ.YEAR AND
	ACC.RAMO = MOVACCREQ.RAMO AND
	ACC.PRG = MOVACCREQ.PRG AND
	ACC.META = MOVACCREQ.META AND
	ACC.ACCION = MOVACCREQ.ACCION AND
	ACCREQ.YEAR = MOVACCREQ.YEAR AND
	ACCREQ.RAMO = MOVACCREQ.RAMO AND
	ACCREQ.PRG = MOVACCREQ.PRG AND
	ACCREQ.META = MOVACCREQ.META AND
	ACCREQ.ACCION = MOVACCREQ.ACCION AND
	ACCREQ.REQUERIMIENTO = MOVACCREQ.REQUERIMIENTO]]>
	</queryString>
