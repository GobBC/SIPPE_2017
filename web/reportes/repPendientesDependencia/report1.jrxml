	<parameter name="OFICIO" class="java.math.BigDecimal"/>
	<parameter name="TIPOUSR" class="java.lang.String"/>
	<parameter name="TIPOFICIO" class="java.lang.String"/>
	<parameter name="TIPOMOV" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
CASE E.ORDEN
WHEN 1 THEN
' '
ELSE
 E.DESCR
END  AS DESCR,
(TO_CHAR((SELECT MAX(FECHAAUT) FROM POA.BITMOVTOS WHERE OFICIO=$P{OFICIO}),'DD/MM/YYYY HH:MI')) AS FECHA_AUT

FROM
POA.ESTATUS_MOV E
WHERE E.ORDEN IN((SELECT E.ORDEN-1 AS ORDEN FROM POA.MOVOFICIOS MOV,POA.ESTATUS_MOV E WHERE MOV.OFICIO=$P{OFICIO} AND MOV.TIPOMOV=$P{TIPOMOV} AND E.ESTATUS=MOV.STATUS))]]>
	</queryString>
