	<queryString>
		<![CDATA[select
    M.OFICIO,
    CASE WHEN T.TIPO IS NULL THEN TO_CHAR(M.OFICIO) ELSE M.OFICIO||'-'||T.TIPO END OFICIO_COMP,
    NVL(c.ID_CARATULA,'no aplica') as id_caratula,
    M.TIPOMOV,
    R.RAMO,
    R.DESCR,
    PA.GOBIERNO,
    PA.SECRETARIA,
    PA.PARAESTATAL,
    M.STATUS,
    E.ORDEN,
    T.TIPO,
    NVL(TO_CHAR(M.FECPPTO,'DD/MM/YYYY HH:MM'),'S/E') AS FECPPTO
from
    POA.MOVOFICIOS M,
    POA.VW_MOVOFICIOS_RAMO_AFECTADO MRA,
    POA.CARATULA_MOVOFICIO c,
    DGI.RAMOS R,
    DGI.PARAMETROS PA,
    POA.ESTATUS_MOV E,
    DGI.TIPOFICIO T
where
    m.OFICIO = c.oficio(+)
    AND MRA.OFICIO = M.OFICIO
    AND MRA.YEAR = R.YEAR
    AND R.YEAR = $P{YEAR}
    AND R.RAMO = MRA.RAMO
    AND M.STATUS NOT IN('R','C','A','K','X')
    AND M.STATUS = E.ESTATUS
    AND M.OFICIO = T.OFICIO(+)
    --AND (T.TIPO NOT IN('U') OR T.TIPO IS NULL)
	AND
	$P!{QUERY}
ORDER BY R.RAMO,M.OFICIO]]>
	</queryString>
