	<queryString>
		<![CDATA[SELECT
	RAMPRG.RAMO,
	RAM.DESCR RAMO_DESCR,
	PRG.DESCR PROGRAMA_DESCR,
	PRG.PRG,
	RAMPRG.DEPTO ,
	DEP.DESCR DEPENDENCIA_DESCR,
	PARM.GOBIERNO,
	PARM.SECRETARIA,
	EVAL.DESCREVAL,
	PROY.PROY,
	PROY.TIPO_PROY,
	VWSPPROYACT.DESCR PROYECTO_DESCR,
	PROY.DEPTO,
	0 AS FOLIO,
	RAMPRG.RESULTADO,
	SYSDATE FECHA

FROM
	POA.RAMO_PROGRAMA RAMPRG,
	DGI.RAMOS RAM,
	S_POA_PROGRAMA PRG,
	POA.PROYECTO PROY,
	DGI.VW_SP_PROY_ACT VWSPPROYACT,
	DGI.DEPENDENCIA DEP,
	DGI.EVALUACION EVAL,
	DGI.PARAMETROS PARM

WHERE
	RAMPRG.RAMO = $P{RAMO} AND
	RAMPRG.YEAR = $P{YEAR_CAPTURA}  AND
	RAM.RAMO = RAMPRG.RAMO  AND
	RAM.YEAR = RAMPRG.YEAR  AND
	PRG.YEAR = RAMPRG.YEAR  AND
	PRG.PRG = RAMPRG.PRG  AND
	PROY.YEAR = RAMPRG.YEAR AND
	PROY.RAMO = RAMPRG.RAMO AND
	PROY.PRG = RAMPRG.PRG AND
	PROY.PROY = VWSPPROYACT.PROY  AND
	PROY.TIPO_PROY = VWSPPROYACT.TIPO_PROY  AND
	PROY.YEAR = VWSPPROYACT.YEAR  AND
	DEP.YEAR (+) = PROY.YEAR AND
	DEP.RAMO (+) = PROY.RAMO AND
	DEP.DEPTO(+) = PROY.DEPTO_RESP AND
	EVAL.YEAR = $P{YEAR_CAPTURA}

ORDER BY
	RAMPRG.RAMO
	,RAMPRG.PRG
	,PROY.TIPO_PROY
	,PROY.PROY]]>
	</queryString>
