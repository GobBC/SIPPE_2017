	<queryString>
		<![CDATA[SELECT
	PPTO.YEAR,
	PPTO.RAMO,
	R.DESCR AS RAMO_DESCR,
    PPTO.FUENTE||'-'||PPTO.FONDO||'-'||PPTO.RECURSO AS FUENTE_FIN,
    RE.DESCR AS FFR_DESCR,
	SUM(PPTO.ASIGNADO) AS ASIGNADO,
	PRAM.GOBIERNO,
	PRAM.SECRETARIA,
	SYSDATE AS FECHA

FROM
	SPP.PPTO PPTO,
	DGI.RAMOS R,
	S_POA_RECURSO RE,
	DGI.PARAMETROS PRAM

WHERE
	PPTO.YEAR = $P{YEAR} AND
	PPTO.RAMO IN($P!{RAMO_IN_LIST}) AND
	$P!{QUERY}
	R.YEAR = PPTO.YEAR AND
	R.RAMO = PPTO.RAMO AND
	RE.YEAR = PPTO.YEAR AND
	RE.FUENTE = PPTO.FUENTE  AND
	RE.FONDO = PPTO.FONDO AND
	RE.RECURSO = PPTO.RECURSO

GROUP BY
	PPTO.YEAR,
	PPTO.RAMO,
    	R.DESCR,
    	PPTO.FUENTE||'-'||PPTO.FONDO||'-'||PPTO.RECURSO,
    	RE.DESCR ,
	PRAM.GOBIERNO,
	PRAM.SECRETARIA,
	SYSDATE

ORDER BY
	PPTO.YEAR,
	PPTO.RAMO,
	R.DESCR,
    	PPTO.FUENTE||'-'||PPTO.FONDO||'-'||PPTO.RECURSO]]>
	</queryString>
