	<queryString>
		<![CDATA[SELECT
	PPTO.YEAR,
	PPTO.RAMO,
	R.DESCR AS RAMO_DESCR,
	PPTO.PRG,
	P.DESCR AS PROGRAMA_DESCR,
	PPTO.DEPTO,
	D.DESCR AS DEPARTAMENTO_DESCR,
	PPTO.TIPO_PROY||PPTO.PROYECTO||'-'||PPTO.META||'-'||PPTO.ACCION||'-'||PPTO.FUENTE||''||PPTO.FONDO||''||PPTO.RECURSO AS CODIGO_PROG,
	G.GRUPO,
	G.DESCR GRUPO_DESCR,
	PPTO.PARTIDA,
	PART.DESCR AS PARTIDA_DESCR,
	DECODE(PPTO.REL_LABORAL,'0',' ',PPTO.REL_LABORAL) AS REL_LABORAL,
    	NVL(REL.DESCR,' ') AS REL_DESCR,
	--SUM(PPTO.ASIGNADO) AS ASIGNADO,
	PRAM.GOBIERNO,
	PRAM.SECRETARIA,
	SYSDATE AS FECHA,
    (NVL(PPTO.ENEACT,0) + NVL(PPTO.FEBACT,0)+ NVL(PPTO.MARACT,0)+ NVL(PPTO.ABRACT,0)+ NVL(PPTO.MAYACT,0)+ NVL(PPTO.JUNACT,0)+ NVL(PPTO.JULACT,0)+ NVL(PPTO.AGOACT,0)+    NVL(PPTO.SEPACT,0)+ NVL(PPTO.OCTACT,0)+ NVL(PPTO.NOVACT,0)+ NVL(PPTO.DICACT,0)) AS ASIGNADO

FROM
	SPP.VW_PRESUP_ASIG_MEN PPTO,
	S_POA_ACCION ACC,
	DGI.RAMOS R,
	S_POA_PROGRAMA P,
	DGI.DEPENDENCIA D,
	S_POA_VW_SP_PROY_ACT VSPPA,
	S_POA_META M,
	S_POA_FUENTE FU,
	S_POA_FONDO FO,
	S_POA_RECURSO RE,
	DGI.PARTIDA PART,
	S_DGI_GRUPOS G,
	S_DGI_SUBGRUPOS SG,
	S_DGI_SUBSUBGPO SSG,
	S_POA_REL_LABORAL REL,
	DGI.PARAMETROS PRAM

WHERE
	PPTO.YEAR = $P{YEAR} AND
	PPTO.RAMO IN($P!{RAMO_IN_LIST}) AND
	$P!{QUERY}
	ACC.YEAR = PPTO.YEAR AND
	ACC.RAMO = PPTO.RAMO AND
	ACC.META = PPTO.META AND
	ACC.ACCION = PPTO.ACCION AND
	R.YEAR = PPTO.YEAR AND
	R.RAMO = PPTO.RAMO AND
	P.YEAR = PPTO.YEAR AND
	P.PRG = PPTO.PRG AND
	D.YEAR = PPTO.YEAR AND
	D.RAMO = PPTO.RAMO AND
	D.DEPTO = PPTO.DEPTO AND
	VSPPA.YEAR = PPTO.YEAR AND
	VSPPA.TIPO_PROY = PPTO.TIPO_PROY AND
	VSPPA.PROY = PPTO.PROYECTO AND
	M.YEAR = PPTO.YEAR AND
	M.RAMO = PPTO.RAMO AND
	M.META = PPTO.META AND
	FU.YEAR = PPTO.YEAR AND
	FU.FUENTE = PPTO.FUENTE AND
	FO.YEAR = PPTO.YEAR AND
	FO.FUENTE = PPTO.FUENTE AND
	FO.FONDO = PPTO.FONDO AND
	RE.YEAR = PPTO.YEAR AND
	RE.FUENTE = PPTO.FUENTE  AND
	RE.FONDO = PPTO.FONDO AND
	RE.RECURSO = PPTO.RECURSO AND
	PART.YEAR = PPTO.YEAR AND
	PART.PARTIDA = PPTO.PARTIDA AND
	SSG.YEAR = PART.YEAR AND
	SSG.SUBSUBGRUPO = PART.SUBSUBGPO AND
	SG.YEAR = SSG.YEAR AND
	SG.SUBGRUPO = SSG.SUBGRUPO AND
	G.YEAR = SG.YEAR AND
	G.GRUPO = SG.GRUPO  AND
	REL.YEAR (+)= PPTO.YEAR AND
	REL.REL_LABORAL (+)= PPTO.REL_LABORAL

GROUP BY
	PPTO.YEAR,
	PPTO.RAMO,
	R.DESCR,
	PPTO.PRG,
	P.DESCR,
	PPTO.DEPTO,
	D.DESCR,
	PPTO.TIPO_PROY||PPTO.PROYECTO||'-'||PPTO.META||'-'||PPTO.ACCION||'-'||PPTO.FUENTE||''||PPTO.FONDO||''||PPTO.RECURSO,
	G.GRUPO,
	G.DESCR,
	PPTO.PARTIDA,
	PART.DESCR,
	PPTO.REL_LABORAL,
    REL.DESCR,
	PRAM.GOBIERNO,
	PRAM.SECRETARIA,
	SYSDATE,
	(NVL(PPTO.ENEACT,0) + NVL(PPTO.FEBACT,0)+ NVL(PPTO.MARACT,0)+ NVL(PPTO.ABRACT,0)+ NVL(PPTO.MAYACT,0)+ NVL(PPTO.JUNACT,0)+ NVL(PPTO.JULACT,0)+ NVL(PPTO.AGOACT,0)+    NVL(PPTO.SEPACT,0)+ NVL(PPTO.OCTACT,0)+ NVL(PPTO.NOVACT,0)+ NVL(PPTO.DICACT,0))

ORDER BY
	PPTO.YEAR,
	PPTO.RAMO,
	R.DESCR,
	PPTO.PRG,
	P.DESCR,
	PPTO.DEPTO,
	D.DESCR,
	PPTO.TIPO_PROY||PPTO.PROYECTO||'-'||PPTO.META||'-'||PPTO.ACCION||'-'||PPTO.FUENTE||''||PPTO.FONDO||''||PPTO.RECURSO,
	G.GRUPO,
	G.DESCR,
	PPTO.PARTIDA,
	PART.DESCR,
	PPTO.REL_LABORAL,
    	REL.DESCR,
   	PRAM.GOBIERNO,
	PRAM.SECRETARIA,
	SYSDATE]]>
