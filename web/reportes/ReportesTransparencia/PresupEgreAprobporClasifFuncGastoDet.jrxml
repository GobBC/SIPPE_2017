	<parameter name="YEAR" class="java.lang.Integer"/>
	<parameter name="IMG_DIR" class="java.lang.String"/>
	<parameter name="MILESPESOS" class="java.lang.String"/>
	<parameter name="QUERY" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
F.DESCR AS FINALIDAD_DESCR,
F.FINALIDAD,
FUN.DESCR AS FUNCION_DESCR,
FUN.FUNCION,
SUB.SUBFUNCION,
SUB.DESCR AS SUBFUNCION_DESCR,
DECODE($P{MILESPESOS},'S',SUM(NVL(P.ASIGNADO,0))/1000,SUM(NVL(P.ASIGNADO,0))) AS ASIGNADO,
PARAM.GOBIERNO,
PARAM.SECRETARIA,
PARAM.LOGO,
SYSDATE AS HOY
FROM
SPP.PPTO P,
DGI.RAMOS R,
S_POA_FINALIDAD F,
S_POA_FUNCION FUN,
S_POA_SUBFUNCION SUB,
DGI.PARAMETROS PARAM
WHERE
F.YEAR=P.YEAR
AND F.FINALIDAD=P.FINALIDAD
AND F.YEAR=P.YEAR
AND P.YEAR=$P{YEAR}
AND FUN.YEAR=P.YEAR
AND FUN.FUNCION=P.FUNCION
AND FUN.FINALIDAD=P.FINALIDAD
AND SUB.YEAR=P.YEAR
AND SUB.FINALIDAD=P.FINALIDAD
AND SUB.FUNCION=P.FUNCION
AND SUB.SUBFUNCION=P.SUBFUNCION
AND R.RAMO=P.RAMO
AND R.YEAR=P.YEAR
AND(R.TRANSITORIO IN('N') OR R.TRANSITORIO IS NULL)
AND $P!{QUERY}
GROUP BY
F.DESCR,
F.FINALIDAD,
FUN.DESCR,
FUN.FUNCION,
SUB.SUBFUNCION,
SUB.DESCR,
PARAM.GOBIERNO,
PARAM.SECRETARIA,
PARAM.LOGO
ORDER BY
F.FINALIDAD,
FUN.FUNCION,
SUB.SUBFUNCION]]>
	</queryString>
