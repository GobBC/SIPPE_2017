	<parameter name="YEAR" class="java.lang.Integer"/>
	<parameter name="IMG_DIR" class="java.lang.String"/>
	<parameter name="MILESPESOS" class="java.lang.String"/>
	<parameter name="QUERY" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
F.DESCR,
F.FINALIDAD,
DECODE($P{MILESPESOS},'S',SUM(NVL(P.ASIGNADO,0))/1000,SUM(NVL(P.ASIGNADO,0))) AS ASIGNADO,
PARAM.GOBIERNO,
PARAM.SECRETARIA,
PARAM.LOGO,
SYSDATE AS HOY
FROM
SPP.PPTO P,
DGI.RAMOS R,
S_POA_FINALIDAD F,
DGI.PARAMETROS PARAM
WHERE
F.YEAR=P.YEAR
AND F.FINALIDAD=P.FINALIDAD
AND F.YEAR=P.YEAR
AND P.YEAR=$P{YEAR}
AND P.RAMO=R.RAMO
AND P.YEAR=R.YEAR
AND (R.TRANSITORIO IN('N') OR R.TRANSITORIO IS NULL)
AND $P!{QUERY}
GROUP BY
F.DESCR,
F.FINALIDAD,
PARAM.GOBIERNO,
PARAM.SECRETARIA,
PARAM.LOGO]]>
	</queryString>
