	<parameter name="YEAR" class="java.lang.Integer"/>
	<parameter name="IMG_DIR" class="java.lang.String"/>
	<parameter name="MILESPESOS" class="java.lang.String"/>
	<parameter name="QUERY" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
TEP.TIPO_ENTE_PUBLICO,
TEP.DESCRIPCION,
DECODE($P{MILESPESOS},'S',SUM(NVL(P.ASIGNADO,0))/1000,SUM(NVL(P.ASIGNADO,0))) AS ASIGNADO,
PARAM.GOBIERNO,
PARAM.SECRETARIA,
PARAM.LOGO,
SYSDATE AS HOY
FROM
SPP.PPTO P,
DGI.RAMOS R,
DGI.DEPENDENCIA D,
DGI.PARAMETROS PARAM,
S_POA_SECTOR_CONAC SC,
S_POA_SECTOR_TIPO ST,
S_POA_SECTOR_ECONOMICO SE,
S_POA_SUBSECTOR SUBS,
S_POA_TIPO_ENTE_PUBLICO TEP
WHERE
P.YEAR=$P{YEAR}
AND D.RAMO=P.RAMO
AND D.DEPTO=P.DEPTO
AND D.YEAR=P.YEAR
AND D.SECTOR_CONAC=SC.SECTOR_CONAC
AND D.SECTOR_TIPO=ST.SECTOR_TIPO
AND D.SECTOR_CONAC=ST.SECTOR_CONAC
AND D.SECTOR_ECONOMICO=SE.SECTOR_ECONOMICO
AND D.SECTOR_TIPO=SE.SECTOR_TIPO
AND D.SECTOR_CONAC=SE.SECTOR_CONAC
AND D.SUBSECTOR=SUBS.SUBSECTOR
AND D.SECTOR_CONAC=SUBS.SECTOR_CONAC
AND D.SECTOR_ECONOMICO=SUBS.SECTOR_ECONOMICO
AND D.SECTOR_TIPO=SUBS.SECTOR_TIPO
AND D.TIPO_ENTE_PUBLICO=TEP.TIPO_ENTE_PUBLICO
AND D.SUBSECTOR=TEP.SUBSECTOR
AND D.SECTOR_CONAC=TEP.SECTOR_CONAC
AND D.SECTOR_ECONOMICO=TEP.SECTOR_ECONOMICO
AND D.SECTOR_TIPO=TEP.SECTOR_TIPO
AND P.RAMO=R.RAMO
AND P.YEAR=R.YEAR
AND (R.TRANSITORIO IN('N') OR R.TRANSITORIO IS NULL)
AND $P!{QUERY}
GROUP BY
TEP.TIPO_ENTE_PUBLICO,
TEP.DESCRIPCION,
PARAM.GOBIERNO,
PARAM.SECRETARIA,
PARAM.LOGO
ORDER BY
TEP.TIPO_ENTE_PUBLICO]]>
	</queryString>
