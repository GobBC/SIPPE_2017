	<parameter name="MILESPESOS" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
PRO.DESCR,
DECODE($P{MILESPESOS},'S',SUM(NVL(P.ASIGNADO,0))/1000,SUM(NVL(P.ASIGNADO,0))) AS IMPORTE,
PARAM.GOBIERNO,
PARAM.SECRETARIA,
PARAM.LOGO,
SYSDATE AS HOY
FROM
S_SPP_PPTO P,
S_POA_CATALOGO_PROYECTO PRO,
DGI.PARAMETROS PARAM
WHERE
P.YEAR=$P{YEAR} AND
P.YEAR=PRO.YEAR AND
P.PROYECTO =PRO.PROY
GROUP BY
PRO.DESCR,
PARAM.GOBIERNO,
PARAM.SECRETARIA,
PARAM.LOGO]]>
	</queryString>
