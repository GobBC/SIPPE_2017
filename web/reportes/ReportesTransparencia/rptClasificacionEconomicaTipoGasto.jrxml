	<parameter name="YEAR" class="java.lang.Integer"/>
	<parameter name="MILESPESOS" class="java.lang.String"/>
	<parameter name="IMG_DIR" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
TG.DESCR,
P.TIPO_GASTO,
DECODE($P{MILESPESOS},'S',SUM(NVL(P.ASIGNADO,0))/1000,SUM(NVL(P.ASIGNADO,0))) AS IMPORTE,
PARAM.GOBIERNO,
PARAM.SECRETARIA,
PARAM.LOGO,
SYSDATE AS HOY
FROM
SPP.PPTO P,
S_POA_TIPO_GASTO TG,
DGI.PARAMETROS PARAM,
DGI.RAMOS R
WHERE
P.YEAR=$P{YEAR} AND
P.YEAR=TG.YEAR AND
P.TIPO_GASTO=TG.TIPO_GASTO AND
R.RAMO=P.RAMO AND
R.YEAR=P.YEAR AND
(R.TRANSITORIO IN('N') OR TRANSITORIO IS NULL)
GROUP BY
TG.DESCR,
P.TIPO_GASTO,
PARAM.GOBIERNO,
PARAM.SECRETARIA,
PARAM.LOGO]]>
	</queryString>
