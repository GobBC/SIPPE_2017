	<queryString>
		<![CDATA[SELECT
	A.RAMO,
	A.PRG AS PRG,
	PRG.DESCR AS DESCR_PRG,
    	DECODE(PROY.OBRA,'S','OBRA','') AS OBRA,
	B.TIPO_PROY||B.PROY AS PROY,
	PROY.DESCR AS PROYECTO,
    DECODE(B.OBRA,'G','GASTO CORRIENTE',DECODE(B.OBRA,'I','INVERSIÓN PÚBLICA',DECODE(B.OBRA,'A','GASTO CORRIENTE /INVERSIÓN PÚBLICA'))) AS TIPO_META,
	A.META AS META_ID,
	A.META||'-'||B.DESCR AS META,
	NVL(DGI.F_META_ESTIMACION(1, A.META, A.RAMO, $P{YEAR}),'0') AS MET_ENE,
	NVL(DGI.F_META_ESTIMACION(2, A.META, A.RAMO, $P{YEAR}),'0') AS MET_FEB,
	NVL(DGI.F_META_ESTIMACION(3, A.META, A.RAMO, $P{YEAR}),'0') AS MET_MAR,
	NVL(DGI.F_META_ESTIMACION(4, A.META, A.RAMO, $P{YEAR}),'0') AS MET_ABR,
	NVL(DGI.F_META_ESTIMACION(5, A.META, A.RAMO, $P{YEAR}),'0') AS MET_MAY,
	NVL(DGI.F_META_ESTIMACION(6, A.META, A.RAMO, $P{YEAR}),'0') AS MET_JUN,
	NVL(DGI.F_META_ESTIMACION(7, A.META, A.RAMO, $P{YEAR}),'0') AS MET_JUL,
	NVL(DGI.F_META_ESTIMACION(8, A.META, A.RAMO, $P{YEAR}),'0') AS MET_AGO,
	NVL(DGI.F_META_ESTIMACION(9, A.META, A.RAMO, $P{YEAR}),'0') AS MET_SEP,
	NVL(DGI.F_META_ESTIMACION(10, A.META, A.RAMO, $P{YEAR}),'0') AS MET_OCT,
	NVL(DGI.F_META_ESTIMACION(11, A.META, A.RAMO, $P{YEAR}),'0') AS MET_NOV,
	NVL(DGI.F_META_ESTIMACION(12, A.META, A.RAMO, $P{YEAR}),'0') AS MET_DIC,
	UMM.MEDIDA||'-'||UMM.DESCR AS MEDIDA_META,
	MM.MEDIDA||'-'||MM.DESCR AS MEDIDA,
	A.ACCION AS ACCION_ID,
	A.ACCION||'-'||F.DESCR AS ACCION,
	NVL(DGI.F_ACCION_ESTIMACION(1, A.META, A.RAMO, $P{YEAR}, A.ACCION),'0') AS ACC_ENE,
	NVL( DGI.F_ACCION_ESTIMACION(2, A.META, A.RAMO, $P{YEAR}, A.ACCION),'0') AS ACC_FEB,
	NVL( DGI.F_ACCION_ESTIMACION(3, A.META, A.RAMO, $P{YEAR}, A.ACCION),'0') AS ACC_MAR,
	NVL( DGI.F_ACCION_ESTIMACION(4, A.META, A.RAMO, $P{YEAR}, A.ACCION),'0') AS ACC_ABR,
	NVL( DGI.F_ACCION_ESTIMACION(5, A.META, A.RAMO, $P{YEAR}, A.ACCION),'0') AS ACC_MAY,
	NVL( DGI.F_ACCION_ESTIMACION(6, A.META, A.RAMO, $P{YEAR}, A.ACCION),'0') AS ACC_JUN,
	NVL( DGI.F_ACCION_ESTIMACION(7, A.META, A.RAMO, $P{YEAR}, A.ACCION),'0') AS ACC_JUL,
	NVL( DGI.F_ACCION_ESTIMACION(8, A.META, A.RAMO, $P{YEAR}, A.ACCION),'0') AS ACC_AGO,
	NVL( DGI.F_ACCION_ESTIMACION(9, A.META, A.RAMO, $P{YEAR}, A.ACCION),'0') AS ACC_SEP,
	NVL( DGI.F_ACCION_ESTIMACION(10, A.META, A.RAMO, $P{YEAR}, A.ACCION),'0') AS ACC_OCT,
	NVL(DGI.F_ACCION_ESTIMACION(11, A.META, A.RAMO, $P{YEAR}, A.ACCION),'0') AS ACC_NOV,
	NVL( DGI.F_ACCION_ESTIMACION(12, A.META, A.RAMO, $P{YEAR}, A.ACCION),'0') AS ACC_DIC,
	F.LINEA AS LINEA,
	A.PARTIDA||' - '||P.DESCR AS PARTIDA,
	A.DESCR AS REQUERIMIENTO,
	A.CANTIDAD,
	A.COSTO_UNITARIO,
	A.COSTO_ANUAL,
	A.ENE,
	A.FEB,
	A.MAR,
	A.ABR,
	A.MAY,
	A.JUN,
	A.JUL,
	A.AGO,
	A.SEP,
	A.OCT,
	A.NOV,A.DIC,
	A.DEPTO,
	F.DEPTO AS UE,
	A.JUSTIFICACION AS JUSTIFICACION,
	DEP.DESCR AS DEPTO_DESCR,
	DEPUE.DESCR AS DEPTO_DESCR_UE,
	F.MPO AS MPO,
	MUN.NOMBREMPO AS MPO_DESCR,
	F.LOCALIDAD,
	LOC.DESCR AS LOC_DESCR

FROM
	S_POA_ACCION_REQ A,
	S_POA_META B,
	S_POA_PROGRAMA PRG,
	POA.RAMO_PROGRAMA RP,
	DGI.VW_SP_PROY_ACT PROY,
	S_POA_ACCION F ,
	DGI.PARTIDA P,
	DGI.DEPENDENCIA DEP,
	DGI.DEPENDENCIA DEPUE,
	S_POA_MEDIDA_META UMM,
	S_POA_MEDIDA_META MM,
	S_POA_MUNICIPIO MUN,
	S_POA_LOCALIDAD LOC


WHERE
	DEPUE.YEAR=A.YEAR AND
	DEPUE.RAMO=A.RAMO AND
	DEPUE.DEPTO=A.DEPTO AND
	DEP.YEAR=A.YEAR AND
	DEP.RAMO=A.RAMO AND
	DEP.DEPTO=A.DEPTO
	AND A.YEAR = B.YEAR
	AND A.RAMO = B.RAMO
	AND A.META = B.META
	AND A.YEAR = B.YEAR
	AND A.RAMO = RP.RAMO
	AND A.PRG = PRG.PRG
	AND PRG.PRG = RP.PRG
	AND PRG.YEAR = RP.YEAR
	AND A.YEAR  = PRG.YEAR
	AND B.YEAR = PROY.YEAR
	AND B.TIPO_PROY = PROY.TIPO_PROY
	AND B.PROY = PROY.PROY
	AND A.YEAR = F.YEAR
	AND A.RAMO = F.RAMO
	AND A.META = F.META
	AND A.ACCION = F.ACCION
	AND A.YEAR = P.YEAR
	AND A.PARTIDA = P.PARTIDA
	AND UMM.YEAR(+) = B.YEAR
	AND UMM.MEDIDA(+) = B.CVE_MEDIDA
	AND MM.YEAR(+) = F.YEAR
	AND MM.MEDIDA(+) = F.CVE_MEDIDA
	AND MUN.MPO = F.MPO
	AND LOC.LOCALIDAD = F.LOCALIDAD
	AND LOC.MPO = MUN.MPO
	AND A.YEAR = $P{YEAR}
	AND A.RAMO  IN ($P!{RAMO_IN_LIST})
	$P!{QUERY}

ORDER BY
	A.RAMO,
	A.PRG,
	B.TIPO_PROY,
	A.META,
	A.ACCION,
	A.PARTIDA]]>
	</queryString>
