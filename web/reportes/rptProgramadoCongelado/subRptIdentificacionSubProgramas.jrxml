	<queryString>
		<![CDATA[SELECT
	DGI.RAMOS.RAMO,
	POA.RAMO_PROGRAMA.DEPTO,
	DGI.RAMOS.DESCR AS RAMO_DESC,
	POA.RAMO_PROGRAMA.PRG,
	PRG.DESCR AS PROG_DESC,
	S_POA_PROYECTO_AUTORIZDO.PROY,
	S_POA_PROYECTO_AUTORIZDO.TIPO_PROY,
	DGI.DEPENDENCIA.DESCR AS DEPEN_DESC,
	DGI.PARAMETROS.GOBIERNO,
	DGI.PARAMETROS.SECRETARIA,
	'' AS FOLIO,
	L.NOMBRE ||' '||L.PATERNO ||' '|| L.MATERNO AS LIDER,
	sysdate as fecha

FROM
	S_POA_PROGRAMA PRG,
	POA.RAMO_PROGRAMA,
	DGI.RAMOS ,
	DGI.DEPENDENCIA,
	DGI.PARAMETROS,
	S_POA_PROYECTO_AUTORIZDO,
	DGI.VW_EMPLEADOS L

WHERE
	DGI.RAMOS.YEAR = $P{YEAR} AND
	DGI.RAMOS.RAMO = $P{RAMO} AND
	PRG.YEAR = DGI.RAMOS.YEAR AND
	PRG.PRG = $P{PROG} AND
	POA.RAMO_PROGRAMA.PRG = PRG.PRG AND
	POA.RAMO_PROGRAMA.YEAR = PRG.YEAR AND
	POA.RAMO_PROGRAMA.RAMO = DGI.RAMOS.RAMO AND
	DGI.DEPENDENCIA.DEPTO (+) = POA.RAMO_PROGRAMA.DEPTO  AND
	DGI.DEPENDENCIA.YEAR (+) = POA.RAMO_PROGRAMA.YEAR AND
	DGI.DEPENDENCIA.RAMO (+) = POA.RAMO_PROGRAMA.RAMO AND
	S_POA_PROYECTO_AUTORIZDO.RAMO = POA.RAMO_PROGRAMA.RAMO AND
	S_POA_PROYECTO_AUTORIZDO.PRG = POA.RAMO_PROGRAMA.PRG AND
	S_POA_PROYECTO_AUTORIZDO.YEAR = POA.RAMO_PROGRAMA.YEAR AND
	L.RFC (+)= S_POA_PROYECTO_AUTORIZDO.RFC AND
	L.HOMOCLAVE (+)= S_POA_PROYECTO_AUTORIZDO.HOMOCLAVE]]>
	</queryString>
