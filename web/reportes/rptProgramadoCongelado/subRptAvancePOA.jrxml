	<queryString>
		<![CDATA[SELECT M.META,
	    M.RAMO,
	    M.YEAR,
		 M.LINEA,
	    DECODE(DGI.F_POA_TRAE_PRESUP_META(M.META,M.RAMO,M.YEAR,M.PRG),null,0,DGI.F_POA_TRAE_PRESUP_META(M.META,M.RAMO,M.YEAR,M.PRG)) AS IMPT_PRESUP_META,
        M.DESCR,
       MED.DESCR AS UNIDAD_DESC,
	POA.F_META_DE_PLANTILLA( $P{YEAR},$P{RAMO}, M.META) AS META_PLANTILLA
FROM   S_POA_META_AUTORIZADO M,
       S_POA_MEDIDA_META MED
WHERE  M.RAMO = $P{RAMO}
       AND M.PRG = $P{PROG}
       AND M.TIPO_PROY = $P{TIPO}
       AND M.PROY = $P{PROY}
       AND M.YEAR = $P{YEAR}
       AND MED.MEDIDA = M.CVE_MEDIDA
       and MED.YEAR = M.YEAR

ORDER BY M.META]]>
