	<parameter name="RPT_DIR" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["C:\\aplicacionesJava\\POA\\web\\reportes\\reprogramadoRevision\\"]]></defaultValueExpression>
	</parameter>
	<parameter name="OFICIO" class="java.lang.String"/>
	<parameter name="RAMO" class="java.lang.String"/>
	<parameter name="META" class="java.lang.String"/>
	<parameter name="YEAR" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="ID_CARATULA" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT MM.ACCION, MM.ACCION||'-'|| MM.DESCR AS DESCR, MM.OFICIO, MM.DEPTO,DEP.DESCR AS DEPTO_DESC,
    AC.CVE_MEDIDA||'-'||SMMA.DESCR AS MEDIDA,
    TO_CHAR((SELECT DECODE('AC',AC.CALCULO,SUM(ME.VALOR),DECODE('MI',AC.CALCULO,MIN(ME.VALOR),MAX(ME.VALOR))) AS EST
        FROM POA.HIST_ACCION_ESTIMACION ME
        WHERE MM.OFICIO = ME.OFICIO
        AND MM.RAMO = ME.RAMO
        AND MM.META = ME.META
        AND MM.ACCION = ME.ACCION)) EST,
    AC.CALCULO, AC.LINEA,
    MM.LOCALIDAD||'-'||LOC.DESCR AS LOC,
    (SELECT SUM(NVL(COSTO_ANUAL,0))
        FROM POA.HIST_ACCION_REQ MAR
        WHERE
        MAR.OFICIO = MM.OFICIO
        AND MAR.YEAR = MM.YEAR
        AND MAR.RAMO = MM.RAMO
        AND MAR.PRG = MM.PRG
        AND MAR.META = MM.META
        AND MAR.ACCION = MM.ACCION) MAR,
        (SELECT COUNT(1)
        FROM POA.HIST_ACCION_REQ MAR
        WHERE
        MAR.OFICIO = MM.OFICIO
        AND MAR.YEAR = MM.YEAR
        AND MAR.RAMO = MM.RAMO
        AND MAR.PRG = MM.PRG
        AND MAR.META = MM.META
        AND MAR.ACCION = MM.ACCION) CMAR,
        DECODE(TRIM(MM.LINEA),TRIM(AC.LINEA),'',MM.LINEA) AS LINEA_A,
        DECODE(TRIM(MM.DESCR),TRIM(AC.DESCR),'',MM.ACCION||'-'||MM.DESCR) AS DESCR_A,
        DECODE(MM.CVE_MEDIDA, AC.CVE_MEDIDA,'',MM.CVE_MEDIDA||'-'||SMM.DESCR) AS MEDIDA_A,
        DECODE(MM.CALCULO,AC.CALCULO,'',MM.CALCULO) AS CALCULO_A,
        DECODE(
            (SELECT DECODE('AC',MM.CALCULO,SUM(ME.VALOR),DECODE('MI',MM.CALCULO,MIN(ME.VALOR),MAX(ME.VALOR))) AS EST
                FROM POA.MOVOFICIOS_ACCION_ESTIMACION ME
                WHERE MM.OFICIO = ME.OFICIO
                AND MM.RAMO = ME.RAMO
                AND MM.META = ME.META
                AND MM.ACCION = ME.ACCION),
            (SELECT DECODE('AC',MM.CALCULO,SUM(ME.VALOR),DECODE('MI',MM.CALCULO,MIN(ME.VALOR),MAX(ME.VALOR))) AS EST
                FROM POA.HIST_ACCION_ESTIMACION ME
                WHERE MM.OFICIO = ME.OFICIO
                AND MM.RAMO = ME.RAMO
                AND MM.META = ME.META
                AND MM.ACCION = ME.ACCION),'',
              (SELECT DECODE('AC',MM.CALCULO,SUM(ME.VALOR),DECODE('MI',MM.CALCULO,MIN(ME.VALOR),MAX(ME.VALOR))) AS EST
                FROM POA.MOVOFICIOS_ACCION_ESTIMACION ME
                WHERE MM.OFICIO = ME.OFICIO
                AND MM.RAMO = ME.RAMO
                AND MM.META = ME.META
                AND MM.ACCION = ME.ACCION)) AS EST_A,
        DECODE((SELECT SUM(NVL(COSTO_ANUAL,0))
            FROM POA.MOVOFICIOS_ACCION_REQ MAR
            WHERE
            MAR.OFICIO (+)= MM.OFICIO
            AND MAR.YEAR = MM.YEAR
            AND MAR.RAMO = MM.RAMO
            AND MAR.PRG = MM.PRG
            AND MAR.META = MM.META
            AND MAR.ACCION = MM.ACCION),
            (SELECT SUM(NVL(COSTO_ANUAL,0))
            FROM POA.HIST_ACCION_REQ MAR
            WHERE
            MAR.OFICIO = MM.OFICIO
            AND MAR.YEAR = MM.YEAR
            AND MAR.RAMO = MM.RAMO
            AND MAR.PRG = MM.PRG
            AND MAR.META = MM.META
            AND MAR.ACCION = MM.ACCION),0,
            (SELECT SUM(NVL(COSTO_ANUAL,0))
            FROM POA.MOVOFICIOS_ACCION_REQ MAR
            WHERE
            MAR.OFICIO (+)= MM.OFICIO
            AND MAR.YEAR = MM.YEAR
            AND MAR.RAMO = MM.RAMO
            AND MAR.PRG = MM.PRG
            AND MAR.META = MM.META
            AND MAR.ACCION = MM.ACCION)) AS MAR_A ,
        DECODE((SELECT COUNT(1)
            FROM POA.MOVOFICIOS_ACCION_REQ MAR
            WHERE
            MAR.OFICIO = MM.OFICIO
            AND MAR.YEAR = MM.YEAR
            AND MAR.RAMO = MM.RAMO
            AND MAR.PRG = MM.PRG
            AND MAR.META = MM.META
            AND MAR.ACCION = MM.ACCION),
            (SELECT COUNT(1)
            FROM POA.HIST_ACCION_REQ MAR
            WHERE
            MAR.OFICIO = MM.OFICIO
            AND MAR.YEAR = MM.YEAR
            AND MAR.RAMO = MM.RAMO
            AND MAR.PRG = MM.PRG
            AND MAR.META = MM.META
            AND MAR.ACCION = MM.ACCION),'',
            (SELECT COUNT(1)
            FROM POA.MOVOFICIOS_ACCION_REQ MAR
            WHERE
            MAR.OFICIO = MM.OFICIO
            AND MAR.YEAR = MM.YEAR
            AND MAR.RAMO = MM.RAMO
            AND MAR.PRG = MM.PRG
            AND MAR.META = MM.META
            AND MAR.ACCION = MM.ACCION)) AS CMAR_A,
    (SELECT
        NVL(SUM(AV.VALOR),0)
    FROM
        DGI.POA_AVANCE_ACCION AV,
        POA.CARATULA C
    WHERE
        AV.YEAR = $P{YEAR}
        AND AV.RAMO = MM.RAMO
        AND AV.META = MM.META
        AND AV.ACCION = MM.ACCION
        AND C.ID_CARATULA = ''
        AND AV.MES = EXTRACT(MONTH FROM C.FECHA_REVISION)) AS AV_ACCION,
        MM.NVA_CREACION
FROM POA.MOVOFICIOS_ACCION MM,
    POA.HIST_ACCION AC,
    S_POA_MEDIDA_META SMM,
    S_POA_MEDIDA_META SMMA,
    DGI.DEPENDENCIA DEP,
    S_POA_LOCALIDAD LOC,
    POA.MOVOFICIOS MV
WHERE
    MM.OFICIO = $P{OFICIO}
    AND MM.RAMO = $P{RAMO}
    AND MM.META = $P{META}
    AND AC.OFICIO (+)= MM.OFICIO
    AND AC.RAMO (+)= MM.RAMO
    AND AC.META (+)= MM.META
    AND AC.ACCION (+)= MM.ACCION
    AND SMM.YEAR (+)= MM.YEAR
    AND SMM.MEDIDA (+)= MM.CVE_MEDIDA

    AND SMMA.YEAR (+)= AC.YEAR
    AND SMMA.MEDIDA (+)= AC.CVE_MEDIDA

    AND DEP.YEAR (+)= MM.YEAR
    AND DEP.RAMO (+)= MM.RAMO
    AND DEP.DEPTO (+)= MM.DEPTO
    AND LOC.MPO = MM.MPO
    AND LOC.LOCALIDAD = MM.LOCALIDAD
    AND MV.OFICIO = MM.OFICIO
    AND MV.STATUS = 'A'
UNION
SELECT MM.ACCION, MM.ACCION||'-'|| MM.DESCR AS DESCR, MM.OFICIO, MM.DEPTO,DEP.DESCR AS DEPTO_DESC,
    AC.CVE_MEDIDA||'-'||SMMA.DESCR AS MEDIDA,
    TO_CHAR((SELECT DECODE('AC',AC.CALCULO,SUM(ME.VALOR),DECODE('MI',AC.CALCULO,MIN(ME.VALOR),MAX(ME.VALOR))) AS EST
        FROM POA.HIST_ACCION_ESTIMACION ME
        WHERE MM.OFICIO = ME.OFICIO
        AND MM.RAMO = ME.RAMO
        AND MM.META = ME.META
        AND MM.ACCION = ME.ACCION)) EST,
    AC.CALCULO, AC.LINEA,
    MM.LOCALIDAD||'-'||LOC.DESCR AS LOC,
    (SELECT SUM(NVL(COSTO_ANUAL,0))
        FROM POA.ACCION_REQ MAR
        WHERE
        MAR.YEAR (+)= EXTRACT(YEAR FROM MV.FECHAELAB)
        AND MAR.YEAR = MM.YEAR
        AND MAR.RAMO = MM.RAMO
        AND MAR.PRG = MM.PRG
        AND MAR.META = MM.META
        AND MAR.ACCION = MM.ACCION) MAR,
    (SELECT COUNT(1)
        FROM POA.ACCION_REQ MAR
        WHERE
        MAR.YEAR (+)= EXTRACT(YEAR FROM MV.FECHAELAB)
        AND MAR.YEAR = MM.YEAR
        AND MAR.RAMO = MM.RAMO
        AND MAR.PRG = MM.PRG
        AND MAR.META = MM.META
        AND MAR.ACCION = MM.ACCION) CMAR,
        DECODE(TRIM(MM.LINEA),TRIM(AC.LINEA),'',MM.LINEA) AS LINEA_A,
        DECODE(MM.DESCR,AC.DESCR,'',MM.ACCION||'-'||MM.DESCR) AS DESCR_A,
        TO_CHAR(DECODE(MM.CVE_MEDIDA, AC.CVE_MEDIDA,'',MM.CVE_MEDIDA||'-'||SMM.DESCR)) AS MEDIDA_A,
        DECODE(MM.CALCULO,AC.CALCULO,'',MM.CALCULO) AS CALCULO_A,
        DECODE(
            (SELECT DECODE('AC',MM.CALCULO,SUM(ME.VALOR),DECODE('MI',MM.CALCULO,MIN(ME.VALOR),MAX(ME.VALOR))) AS EST
                FROM POA.MOVOFICIOS_ACCION_ESTIMACION ME
                WHERE MM.OFICIO = ME.OFICIO
                AND MM.RAMO = ME.RAMO
                AND MM.META = ME.META
                AND MM.ACCION = ME.ACCION),
            (SELECT DECODE('AC',MM.CALCULO,SUM(ME.VALOR),DECODE('MI',MM.CALCULO,MIN(ME.VALOR),MAX(ME.VALOR))) AS EST
                FROM DGI.ACCION_ESTIMACION ME
                WHERE EXTRACT(YEAR FROM MV.FECHAELAB) = ME.YEAR
                AND MM.RAMO = ME.RAMO
                AND MM.META = ME.META
                AND MM.ACCION = ME.ACCION),'',
              (SELECT DECODE('AC',MM.CALCULO,SUM(ME.VALOR),DECODE('MI',MM.CALCULO,MIN(ME.VALOR),MAX(ME.VALOR))) AS EST
                FROM POA.MOVOFICIOS_ACCION_ESTIMACION ME
                WHERE MM.OFICIO = ME.OFICIO
                AND MM.RAMO = ME.RAMO
                AND MM.META = ME.META
                AND MM.ACCION = ME.ACCION)) AS EST_A,
        DECODE((SELECT SUM(NVL(COSTO_ANUAL,0))
            FROM POA.MOVOFICIOS_ACCION_REQ MAR
            WHERE
            MAR.OFICIO (+)= MM.OFICIO
            AND MAR.YEAR = MM.YEAR
            AND MAR.RAMO = MM.RAMO
            AND MAR.PRG = MM.PRG
            AND MAR.META = MM.META
            AND MAR.ACCION = MM.ACCION),
            (SELECT SUM(NVL(COSTO_ANUAL,0))
            FROM POA.ACCION_REQ MAR
            WHERE
            MAR.YEAR (+)= EXTRACT(YEAR FROM MV.FECHAELAB)
            AND MAR.YEAR = MM.YEAR
            AND MAR.RAMO = MM.RAMO
            AND MAR.PRG = MM.PRG
            AND MAR.META = MM.META
            AND MAR.ACCION = MM.ACCION),0,
            (SELECT SUM(NVL(COSTO_ANUAL,0))
            FROM POA.MOVOFICIOS_ACCION_REQ MAR
            WHERE
            MAR.OFICIO (+)= MM.OFICIO
            AND MAR.YEAR = MM.YEAR
            AND MAR.RAMO = MM.RAMO
            AND MAR.PRG = MM.PRG
            AND MAR.META = MM.META
            AND MAR.ACCION = MM.ACCION)) AS MAR_A ,
        DECODE((SELECT COUNT(1)
            FROM POA.MOVOFICIOS_ACCION_REQ MAR
            WHERE
            MAR.OFICIO = MM.OFICIO
            AND MAR.YEAR = MM.YEAR
            AND MAR.RAMO = MM.RAMO
            AND MAR.PRG = MM.PRG
            AND MAR.META = MM.META
            AND MAR.ACCION = MM.ACCION),
            (SELECT COUNT(1)
            FROM POA.ACCION_REQ MAR
            WHERE
            MAR.YEAR = EXTRACT(YEAR FROM MV.FECHAELAB)
            AND MAR.YEAR = MM.YEAR
            AND MAR.RAMO = MM.RAMO
            AND MAR.PRG = MM.PRG
            AND MAR.META = MM.META
            AND MAR.ACCION = MM.ACCION),'',
            (SELECT COUNT(1)
            FROM POA.MOVOFICIOS_ACCION_REQ MAR
            WHERE
            MAR.OFICIO = MM.OFICIO
            AND MAR.YEAR = MM.YEAR
            AND MAR.RAMO = MM.RAMO
            AND MAR.PRG = MM.PRG
            AND MAR.META = MM.META
            AND MAR.ACCION = MM.ACCION)) AS CMAR_A,
    (SELECT
        NVL(SUM(AV.VALOR),0)
    FROM
        DGI.POA_AVANCE_ACCION AV,
        POA.CARATULA C
    WHERE
        AV.YEAR = $P{YEAR}
        AND AV.RAMO = MM.RAMO
        AND AV.META = MM.META
        AND AV.ACCION = MM.ACCION
        AND C.ID_CARATULA = ''
        AND AV.MES = EXTRACT(MONTH FROM C.FECHA_REVISION)) AS AV_ACCION,
        MM.NVA_CREACION
FROM POA.MOVOFICIOS_ACCION MM,
    POA.ACCION AC,
    S_POA_MEDIDA_META SMM,
    S_POA_MEDIDA_META SMMA,
    DGI.DEPENDENCIA DEP,
    S_POA_LOCALIDAD LOC,
    POA.MOVOFICIOS MV
WHERE
    MM.OFICIO = $P{OFICIO}
    AND MV.OFICIO = MM.OFICIO
    AND MV.STATUS <> 'A'
    AND MM.RAMO =$P{RAMO}
    AND MM.META = $P{META}
    AND AC.YEAR = EXTRACT(YEAR FROM MV.FECHAELAB)
    AND AC.RAMO = MM.RAMO
    AND AC.META = MM.META
    AND AC.ACCION = MM.ACCION
    AND SMM.YEAR = MM.YEAR
    AND SMM.MEDIDA = MM.CVE_MEDIDA
    AND SMMA.YEAR (+)= MM.YEAR
    AND SMMA.MEDIDA (+)= MM.CVE_MEDIDA
    AND DEP.YEAR (+)= MM.YEAR
    AND DEP.RAMO (+)= MM.RAMO
    AND DEP.DEPTO (+)= MM.DEPTO
    AND LOC.MPO = MM.MPO
    AND LOC.LOCALIDAD = MM.LOCALIDAD
UNION
SELECT MM.ACCION, MM.ACCION||'-'|| MM.DESCR AS DESCR, MM.OFICIO, MM.DEPTO,DEP.DESCR AS DEPTO_DESC,' ' AS MEDIDA,
        '' AS EST,
        '' AS CALCULO, '' AS LINEA,
        MM.LOCALIDAD||'-'||LOC.DESCR AS LOC,
        0 AS MAR,
        0 AS CMAR,
        MM.LINEA AS LINEA_A,
        MM.ACCION||'-'||MM.DESCR AS DESCR_A,
        TO_CHAR(MM.CVE_MEDIDA||'-'||SMM.DESCR) AS MEDIDA_A,
        MM.CALCULO AS CALCULO_A,
        TO_CHAR((SELECT DECODE('AC',MM.CALCULO,SUM(ME.VALOR),DECODE('MI',MM.CALCULO,MIN(ME.VALOR),MAX(ME.VALOR))) AS EST
            FROM POA.MOVOFICIOS_ACCION_ESTIMACION ME
            WHERE MM.OFICIO = ME.OFICIO
            AND MM.RAMO = ME.RAMO
            AND MM.META = ME.META
            AND MM.ACCION = ME.ACCION)) AS EST_A,
        (SELECT SUM(NVL(COSTO_ANUAL,0))
            FROM POA.MOVOFICIOS_ACCION_REQ MAR
            WHERE
            MAR.OFICIO (+)= MM.OFICIO
            AND MAR.YEAR = MM.YEAR
            AND MAR.RAMO = MM.RAMO
            AND MAR.PRG = MM.PRG
            AND MAR.META = MM.META
            AND MAR.ACCION = MM.ACCION) AS MAR_A ,
DECODE((SELECT COUNT(1)
            FROM POA.MOVOFICIOS_ACCION_REQ MAR
            WHERE
            MAR.OFICIO = MM.OFICIO
            AND MAR.YEAR = MM.YEAR
            AND MAR.RAMO = MM.RAMO
            AND MAR.PRG = MM.PRG
            AND MAR.META = MM.META
            AND MAR.ACCION = MM.ACCION),
            (SELECT COUNT(1)
            FROM POA.ACCION_REQ MAR
            WHERE
            MAR.YEAR = EXTRACT(YEAR FROM MV.FECHAELAB)
            AND MAR.YEAR = MM.YEAR
            AND MAR.RAMO = MM.RAMO
            AND MAR.PRG = MM.PRG
            AND MAR.META = MM.META
            AND MAR.ACCION = MM.ACCION),'',
            (SELECT COUNT(1)
            FROM POA.MOVOFICIOS_ACCION_REQ MAR
            WHERE
            MAR.OFICIO = MM.OFICIO
            AND MAR.YEAR = MM.YEAR
            AND MAR.RAMO = MM.RAMO
            AND MAR.PRG = MM.PRG
            AND MAR.META = MM.META
            AND MAR.ACCION = MM.ACCION)) AS CMAR_A,
            0 AS AV_ACCION,
            MM.NVA_CREACION
    FROM POA.MOVOFICIOS_ACCION MM,
        S_POA_MEDIDA_META SMM,
        DGI.DEPENDENCIA DEP,
        S_POA_LOCALIDAD LOC,
        POA.MOVOFICIOS MV
    WHERE
        MM.OFICIO = $P{OFICIO}
        AND MV.OFICIO = MM.OFICIO
        AND MV.STATUS <> 'A'
        AND MM.RAMO =$P{RAMO}
        AND MM.META = $P{META}
        AND MM.NVA_CREACION = 'S'
        AND SMM.YEAR = MM.YEAR
        AND SMM.MEDIDA = MM.CVE_MEDIDA
        AND DEP.YEAR (+)= MM.YEAR
        AND DEP.RAMO (+)= MM.RAMO
        AND DEP.DEPTO (+)= MM.DEPTO
        AND LOC.MPO = MM.MPO
        AND LOC.LOCALIDAD = MM.LOCALIDAD
UNION ALL
SELECT MM.ACCION, MM.ACCION||'-'|| MM.DESCR AS DESCR,
     MV.OFICIO,
     MM.DEPTO,
     DEP.DESCR AS DEPTO_DESC,
     SMM.DESCR AS MEDIDA,
        TO_CHAR(CASE MV.STATUS WHEN  'A' THEN
            (SELECT DECODE(MM.CALCULO,'AC',SUM(ME.VALOR),DECODE(MM.CALCULO,'MI',MIN(ME.VALOR),MAX(ME.VALOR)))
             FROM POA.HIST_ACCION_ESTIMACION ME
             WHERE  MM.YEAR = ME.YEAR
                    AND MM.RAMO = ME.RAMO
                    AND MM.META = ME.META
                    AND MM.ACCION = ME.ACCION)
             ELSE
                (SELECT DECODE(MM.CALCULO,'AC',SUM(ME.VALOR),DECODE(MM.CALCULO,'MI',MIN(ME.VALOR),MAX(ME.VALOR)))
                 FROM DGI.ACCION_ESTIMACION ME
                 WHERE  MM.YEAR = ME.YEAR
                        AND MM.RAMO = ME.RAMO
                        AND MM.META = ME.META
                        AND MM.ACCION = ME.ACCION)
            END)
        AS EST,
        MM.CALCULO AS CALCULO, MM.LINEA AS LINEA,
        MM.LOCALIDAD||'-'||LOC.DESCR AS LOC,
        0 AS MAR,
        0 AS CMAR,
        ' ' AS LINEA_A,
        ' ' AS DESCR_A,
        ' ' AS MEDIDA_A,
        ' ' AS CALCULO_A,
        (CASE MV.STATUS WHEN 'A' THEN
                DECODE(
                (SELECT DECODE('AC',MM.CALCULO,SUM(ME.VALOR),DECODE('MI',MM.CALCULO,MIN(ME.VALOR),MAX(ME.VALOR))) AS EST
                    FROM POA.MOVOFICIOS_ACCION_ESTIMACION ME
                    WHERE MV.OFICIO = ME.OFICIO
                    AND MM.RAMO = ME.RAMO
                    AND MM.META = ME.META
                    AND MM.ACCION = ME.ACCION),
                (SELECT DECODE('AC',MM.CALCULO,SUM(ME.VALOR),DECODE('MI',MM.CALCULO,MIN(ME.VALOR),MAX(ME.VALOR))) AS EST
                    FROM POA.HIST_ACCION_ESTIMACION ME
                    WHERE EXTRACT(YEAR FROM MV.FECHAELAB) = ME.YEAR
                    AND MM.RAMO = ME.RAMO
                    AND MM.META = ME.META
                    AND MM.ACCION = ME.ACCION),'',
                  (SELECT DECODE('AC',MM.CALCULO,SUM(ME.VALOR),DECODE('MI',MM.CALCULO,MIN(ME.VALOR),MAX(ME.VALOR))) AS EST
                    FROM POA.MOVOFICIOS_ACCION_ESTIMACION ME
                    WHERE MV.OFICIO = ME.OFICIO
                    AND MM.RAMO = ME.RAMO
                    AND MM.META = ME.META
                    AND MM.ACCION = ME.ACCION))
            ELSE
                DECODE(
                (SELECT DECODE('AC',MM.CALCULO,SUM(ME.VALOR),DECODE('MI',MM.CALCULO,MIN(ME.VALOR),MAX(ME.VALOR))) AS EST
                    FROM POA.MOVOFICIOS_ACCION_ESTIMACION ME
                    WHERE MV.OFICIO = ME.OFICIO
                    AND MM.RAMO = ME.RAMO
                    AND MM.META = ME.META
                    AND MM.ACCION = ME.ACCION),
                (SELECT DECODE('AC',MM.CALCULO,SUM(ME.VALOR),DECODE('MI',MM.CALCULO,MIN(ME.VALOR),MAX(ME.VALOR))) AS EST
                    FROM DGI.ACCION_ESTIMACION ME
                    WHERE EXTRACT(YEAR FROM MV.FECHAELAB) = ME.YEAR
                    AND MM.RAMO = ME.RAMO
                    AND MM.META = ME.META
                    AND MM.ACCION = ME.ACCION),'',
                  (SELECT DECODE('AC',MM.CALCULO,SUM(ME.VALOR),DECODE('MI',MM.CALCULO,MIN(ME.VALOR),MAX(ME.VALOR))) AS EST
                    FROM POA.MOVOFICIOS_ACCION_ESTIMACION ME
                    WHERE MV.OFICIO = ME.OFICIO
                    AND MM.RAMO = ME.RAMO
                    AND MM.META = ME.META
                    AND MM.ACCION = ME.ACCION))
           END )
        AS EST_A,
        0 AS MAR_A ,
        ' ' AS CMAR_A,
            0 AS AV_ACCION,
            'N' NVA_CREACION
    FROM
        S_POA_ACCION MM,
        S_POA_MEDIDA_META SMM,
        POA.MOVOFICIOS_ACCION_ESTIMACION MAE,
        DGI.DEPENDENCIA DEP,
        S_POA_LOCALIDAD LOC,
        POA.MOVOFICIOS MV
    WHERE
        MV.OFICIO = $P{OFICIO}
        AND MM.YEAR = $P{YEAR}
        AND MM.RAMO = $P{RAMO}
        AND MM.META = $P{META}
        AND MAE.OFICIO = MV.OFICIO
        AND MAE.RAMO = MM.RAMO
        AND MAE.META = MM.META
        AND MAE.ACCION = MM.ACCION
        AND MAE.NVA_CREACION = 'N'
        AND MAE.OFICIO NOT IN
            (SELECT OFICIO FROM POA.MOVOFICIOS_ACCION MA
                WHERE MA.OFICIO = MAE.OFICIO
                AND MA.RAMO = MAE.RAMO
                AND MA.META = MAE.META
                AND MA.ACCION = MAE.ACCION)
        AND MAE.PERIODO = 1
        AND SMM.YEAR = MM.YEAR
        AND SMM.MEDIDA = MM.CVE_MEDIDA
        AND DEP.YEAR (+)= MM.YEAR
        AND DEP.RAMO (+)= MM.RAMO
        AND DEP.DEPTO (+)= MM.DEPTO
        AND LOC.MPO = MM.MPO
        AND LOC.LOCALIDAD = MM.LOCALIDAD]]>
	</queryString>
