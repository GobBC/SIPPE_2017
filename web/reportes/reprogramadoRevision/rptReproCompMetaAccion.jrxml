	<parameter name="SUBREPORT_DIR" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["C:\\SIMG\\Desarrollo\\Web\\Java\\repositorio_local\\Aplicaciones Web\\POA\\web\\reportes\\reprogramadoRevision\\"]]></defaultValueExpression>
	</parameter>
	<parameter name="YEAR" class="java.lang.String">
		<defaultValueExpression><![CDATA[$P{YEAR}]]></defaultValueExpression>
	</parameter>
	<parameter name="QUERY" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="ID_CARATULA" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT TR.RAMO, TR.R_DESC,TR.OFICIO, TR.PRG, TR.PRG_DESC,TR.TIPO_PROY,TR.PROY, TR.PROYC, TR.PROY_DEC, TR.DEPTO_RESP,TR.DEPTO_DESC, TR.META, TR.DESCR, TR.MEDIDA,TR.EST_H,TR.CALCULO,TR.TIPO_COMP,TR.LINEA, TR.LINEA_A, TR.DESCR_A, TR.CVE_MEDIDA_A, TR.EST_A, TR.CALCULO_A, TR.TIPO_COMP_A,TR.ESTATUS_D,TR.MMETA, TR.DESCR_M, TO_NUMBER(TR.AV_META) AS AV_META, P.SECRETARIA, P.GOBIERNO, R.ABREVIATURA,   (
SELECT
 NVL(DECODE(TM.TIPO_MODIFICACION,2,REVPRES.DESCR_CORTA||' '||REVPRES.DESCR||' presupuestal '||'-'||TM.DESCR,REVPRES.DESCR_CORTA||' '||REVPRES.DESCR||' presupuestal '),' ')      CARATULA_DESCR
                FROM
                    POA.CARATULA CAR,
                    POA.REVISIONES_CARATULA REVS,
                    POA.REVISIONES_CARATULA REVPRES,
                    POA.REVISIONES_CARATULA REVPRG,
                    POA.TIPO_SESION TIS,
                    DGI.RAMOS RAM,
                    POA.TIPO_MODIFICACION TM
                WHERE
                CAR.TIPO_MODIFICACION=TM.TIPO_MODIFICACION AND
                    CAR.YEAR = CM.YEAR AND
                    CAR.RAMO = TR.RAMO AND
                    CAR.ID_CARATULA = CM.ID_CARATULA AND
                    REVS.REVISION = CAR.NUMERO_SESION AND
                    REVPRES.REVISION (+)= CAR.NUM_MOD_PRES AND
                    REVPRG.REVISION (+)= CAR.NUM_MOD_PROG AND
                    TIS.CVE_SESION = CAR.TIPO_SESION AND
                    RAM.YEAR = CAR.YEAR AND
                    RAM.RAMO = CAR.RAMO

) AS CARATULA, TR.NVA_CREACION
FROM
(
    SELECT MM.RAMO,R.DESCR AS R_DESC,MM.OFICIO,MM.PRG,PRG.DESCR AS PRG_DESC,MM.TIPO_PROY,MM.PROY, MM.TIPO_PROY||MM.PROY AS PROYC,PROY.DESCR AS PROY_DEC,PA.DEPTO_RESP,DEP.DESCR AS DEPTO_DESC,MM.META AS META, M.META||'-'||M.DESCR AS DESCR,MM.META AS MMETA, MM.META||'-'||MM.DESCR AS DESCR_M, M.CVE_MEDIDA||'-'||SMM.DESCR AS MEDIDA,
        (SELECT DECODE(MM.CALCULO,'AC',SUM(ME.VALOR),DECODE(MM.CALCULO,'MI',MIN(ME.VALOR),MAX(ME.VALOR))) AS EST
            FROM POA.HIST_ESTIMACION ME
            WHERE MM.OFICIO = ME.OFICIO
            AND MM.RAMO = ME.RAMO
            AND MM.META = ME.META) EST_H,
        M.CALCULO, TC.DESCR AS TIPO_COMP, M.LINEA,
        DECODE(M.LINEA,MM.LINEA,NULL,MM.LINEA) AS LINEA_A,
        DECODE(M.META||'-'||M.DESCR,MM.META||'-'||MM.DESCR,NULL,MM.META||'-'||MM.DESCR) AS DESCR_A,
        --NULL AS CVE_MEDIDA_A,
        TO_CHAR(DECODE(M.CVE_MEDIDA,MM.CVE_MEDIDA,NULL ,MM.CVE_MEDIDA||'-'||SMMA.DESCR)) AS CVE_MEDIDA_A,
        TO_CHAR(DECODE((SELECT DECODE(MM.CALCULO,'AC',SUM(ME.VALOR),DECODE(MM.CALCULO,'MI',MIN(ME.VALOR),MAX(ME.VALOR))) AS EST
            FROM POA.HIST_ESTIMACION ME
            WHERE MM.OFICIO = ME.OFICIO
            AND MM.RAMO = ME.RAMO
            AND MM.META = ME.META)
            ,(SELECT DECODE(MM.CALCULO,'AC',SUM(ME.VALOR),DECODE(MM.CALCULO,'MI',MIN(ME.VALOR),MAX(ME.VALOR))) AS EST
            FROM POA.MOVOFICIOS_ESTIMACION ME
            WHERE MM.OFICIO = ME.OFICIO
            AND MM.RAMO = ME.RAMO
            AND MM.META = ME.META)
            ,NULL,(SELECT DECODE(MM.CALCULO,'AC',SUM(ME.VALOR),DECODE(MM.CALCULO,'MI',MIN(ME.VALOR),MAX(ME.VALOR))) AS EST
            FROM POA.MOVOFICIOS_ESTIMACION ME
            WHERE MM.OFICIO = ME.OFICIO
            AND MM.RAMO = ME.RAMO
            AND MM.META = ME.META))) AS EST_A,
            DECODE(M.CALCULO,MM.CALCULO,NULL,MM.CALCULO) AS CALCULO_A,
            --NULL AS TIPO_COMP_A,
            DECODE(M.TIPO_COMPROMISO,MM.TIPO_COMPROMISO,TO_CHAR(' '),TCA.DESCR) AS TIPO_COMP_A,
            EM.DESCR AS ESTATUS_D,
            TO_CHAR((SELECT
    DECODE(M.CALCULO,'AC',SUM(AV.VALOR),DECODE(M.CALCULO,'MI',MIN(AV.VALOR),MAX(AV.VALOR)))
--NVL(SUM(AV.VALOR),0)
FROM
    DGI.AVANCE AV,
    POA.CARATULA_MOVOFICIO CM,
    POA.CARATULA C
WHERE
    AV.YEAR = $P{YEAR} --PARAMETRO YEAR
    AND AV.RAMO = M.RAMO
    AND AV.META = M.META
    AND CM.OFICIO = MV.OFICIO
    AND C.ID_CARATULA = CM.ID_CARATULA --PARAMETRO CARATULA
    AND AV.PERIODO <= EXTRACT(MONTH FROM C.FECHA_REVISION)
	)) AS AV_META,
        MM.NVA_CREACION
    FROM
        POA.HIST_META M,
        POA.MOVOFICIOS_META MM,
        POA.PROYECTO PA,
        S_POA_MEDIDA_META SMM,
        S_POA_MEDIDA_META SMMA,
        DGI.POA_TIPO_COMPROMISO TC,
        DGI.POA_TIPO_COMPROMISO TCA,
        DGI.PARAMETROS P,
        DGI.RAMOS R,
        S_POA_PROGRAMA PRG,
        DGI.VW_SP_PROY_ACT PROY,
        DGI.DEPENDENCIA DEP,
        POA.MOVOFICIOS MV,
        POA.ESTATUS_MOV EM
    WHERE
    M.OFICIO (+)= MM.OFICIO
    AND M.RAMO (+)= MM.RAMO
    AND M.PRG (+)= MM.PRG
    AND M.META (+)= MM.META
    AND R.YEAR = MM.YEAR
    AND R.RAMO = MM.RAMO
    AND PRG.YEAR = MM.YEAR
    AND PRG.PRG = MM.PRG
    AND PROY.YEAR = MM.YEAR
    AND PROY.TIPO_PROY = MM.TIPO_PROY
    AND PROY.PROY = MM.PROY
    AND PA.YEAR = MM.YEAR
    AND PA.RAMO = MM.RAMO
    AND PA.PRG = MM.PRG
    AND PA.TIPO_PROY = MM.TIPO_PROY
    AND PA.PROY = MM.PROY
    AND SMM.YEAR (+)= M.YEAR
    AND SMM.MEDIDA (+)= M.CVE_MEDIDA
    AND SMMA.YEAR (+)= MM.YEAR
    AND SMMA.MEDIDA (+)= MM.CVE_MEDIDA
    AND TC.TIPO_COMPROMISO (+)= M.TIPO_COMPROMISO
    AND TCA.TIPO_COMPROMISO (+)= MM.TIPO_COMPROMISO
    AND DEP.YEAR (+)= PA.YEAR
    AND DEP.RAMO (+)= PA.RAMO
    AND DEP.DEPTO (+)= PA.DEPTO_RESP
    AND MV.OFICIO = MM.OFICIO
    AND MV.STATUS = 'A'
    AND MV.STATUS = EM.ESTATUS
UNION ALL
    SELECT MM.RAMO,R.DESCR AS R_DESC,MM.OFICIO,MM.PRG,PRG.DESCR AS PRG_DESC,MM.TIPO_PROY,MM.PROY, MM.TIPO_PROY||MM.PROY AS PROY,PROY.DESCR AS PROY_DEC,PA.DEPTO_RESP,DEP.DESCR AS DEPTO_DESC,MM.META AS META, M.META||'-'||M.DESCR AS DESCR,MM.META AS MMETA, MM.META||'-'||MM.DESCR AS DESCR_M, MM.CVE_MEDIDA||'-'||SMM.DESCR AS MEDIDA,
        (SELECT DECODE(MM.CALCULO,'AC',SUM(ME.VALOR),DECODE(MM.CALCULO,'MI',MIN(ME.VALOR),MAX(ME.VALOR))) AS EST
            FROM DGI.ESTIMACION ME, POA.MOVOFICIOS MV
            WHERE MM.YEAR = EXTRACT(YEAR FROM MV.FECHAELAB)
            AND MM.YEAR = ME.YEAR
            AND MM.RAMO = ME.RAMO
            AND MM.META = ME.META
            AND MV.OFICIO = MM.OFICIO) EST_H,
        MM.CALCULO, TC.DESCR AS TIPO_COMP, M.LINEA,
        DECODE(M.LINEA,MM.LINEA,'',MM.LINEA) AS LINEA_A,
        DECODE(M.META||'-'||M.DESCR,MM.META||'-'||MM.DESCR,'',MM.META||'-'||MM.DESCR) AS DESCR_A,
        DECODE(M.CVE_MEDIDA,MM.CVE_MEDIDA,'',MM.CVE_MEDIDA||'-'||SMMA.DESCR) AS CVE_MEDIDA_A,
        DECODE((SELECT DECODE(MM.CALCULO,'AC',SUM(ME.VALOR),DECODE(MM.CALCULO,'MI',MIN(ME.VALOR),MAX(ME.VALOR))) AS EST
            FROM DGI.ESTIMACION ME, POA.MOVOFICIOS MV
            WHERE MM.YEAR = EXTRACT(YEAR FROM MV.FECHAELAB)
            AND MM.RAMO = ME.RAMO
            AND MM.META = ME.META
            AND MV.OFICIO = MM.OFICIO)
            ,(SELECT DECODE(MM.CALCULO,'AC',SUM(ME.VALOR),DECODE(MM.CALCULO,'MI',MIN(ME.VALOR),MAX(ME.VALOR))) AS EST
            FROM POA.MOVOFICIOS_ESTIMACION ME
            WHERE MM.OFICIO = ME.OFICIO
            AND MM.RAMO = ME.RAMO
            AND MM.META = ME.META)
            ,'',(SELECT DECODE(MM.CALCULO,'AC',SUM(ME.VALOR),DECODE(MM.CALCULO,'MI',MIN(ME.VALOR),MAX(ME.VALOR))) AS EST
            FROM POA.MOVOFICIOS_ESTIMACION ME
            WHERE MM.OFICIO = ME.OFICIO
            AND MM.RAMO = ME.RAMO
            AND MM.META = ME.META)) AS EST_A,
            DECODE(M.CALCULO,MM.CALCULO,'',MM.CALCULO) AS CVE_MEDIDA_A,
            DECODE(M.TIPO_COMPROMISO,MM.TIPO_COMPROMISO,'',TCA.DESCR) AS TIPO_COMP_A,
            EM.DESCR AS ESTATUS_D,
            TO_CHAR((SELECT
    DECODE(M.CALCULO,'AC',SUM(AV.VALOR),DECODE(M.CALCULO,'MI',MIN(AV.VALOR),MAX(AV.VALOR)))
--NVL(SUM(AV.VALOR),0)
FROM
    DGI.AVANCE AV,
    POA.CARATULA_MOVOFICIO CM,
    POA.CARATULA C
WHERE
    AV.YEAR = $P{YEAR} --PARAMETRO YEAR
    AND AV.RAMO = M.RAMO
    AND AV.META = M.META
    AND CM.OFICIO = MV.OFICIO
    AND C.ID_CARATULA = CM.ID_CARATULA --PARAMETRO CARATULA
    AND AV.PERIODO <= EXTRACT(MONTH FROM C.FECHA_REVISION))) AS AV_META,
        MM.NVA_CREACION
    FROM
        S_POA_META M,
        POA.MOVOFICIOS_META MM,
        POA.PROYECTO PA,
        S_POA_MEDIDA_META SMM,
        S_POA_MEDIDA_META SMMA,
        DGI.POA_TIPO_COMPROMISO TC,
        DGI.POA_TIPO_COMPROMISO TCA,
        DGI.PARAMETROS P,
        DGI.RAMOS R,
        S_POA_PROGRAMA PRG,
        DGI.VW_SP_PROY_ACT PROY,
        DGI.DEPENDENCIA DEP,
        POA.MOVOFICIOS MV,
        POA.ESTATUS_MOV EM
    WHERE
 M.YEAR = EXTRACT(YEAR FROM MV.FECHAELAB)
    AND M.RAMO = MM.RAMO
    AND M.PRG = MM.PRG
    AND M.META = MM.META
    --AND MM.RAMO = ?
    --AND MM.META = ?
    AND R.YEAR = MM.YEAR
    AND R.RAMO = MM.RAMO
    AND PRG.YEAR = MM.YEAR
    AND PRG.PRG = MM.PRG
    AND PROY.YEAR = MM.YEAR
    AND PROY.TIPO_PROY = MM.TIPO_PROY
    AND PROY.PROY = MM.PROY
    AND PA.YEAR = MM.YEAR
    AND PA.RAMO = MM.RAMO
    AND PA.PRG = MM.PRG
    AND PA.TIPO_PROY = MM.TIPO_PROY
    AND PA.PROY = MM.PROY
    AND SMM.YEAR = M.YEAR
    AND SMM.MEDIDA = M.CVE_MEDIDA
    AND SMMA.YEAR (+)= MM.YEAR
    AND SMMA.MEDIDA (+)= MM.CVE_MEDIDA
    AND TC.TIPO_COMPROMISO = M.TIPO_COMPROMISO
    AND TCA.TIPO_COMPROMISO = MM.TIPO_COMPROMISO
    AND DEP.YEAR (+)= PA.YEAR
    AND DEP.RAMO (+)= PA.RAMO
    AND DEP.DEPTO (+)= PA.DEPTO_RESP
    AND MV.OFICIO = MM.OFICIO
    AND MV.STATUS <> 'A'
    AND MV.STATUS = EM.ESTATUS
UNION ALL
    SELECT MM.RAMO,R.DESCR AS R_DESC,MM.OFICIO,MM.PRG,PRG.DESCR AS PRG_DESC,MM.TIPO_PROY,MM.PROY, MM.TIPO_PROY||MM.PROY AS PROY,PROY.DESCR AS PROY_DEC,PA.DEPTO_RESP,DEP.DESCR AS DEPTO_DESC,0 AS META, ' ' AS DESCR,MM.META AS MMETA, ' ' AS DESCR_M, ' ' AS MEDIDA,
        NULL EST_H,
        ' ' AS CALCULO,
        ' ' TIPO_COMP,
        ' ' AS LINEA,
        MM.LINEA AS LINEA_A,
        MM.META||'-'||MM.DESCR AS DESCR_A,

TO_CHAR(DECODE(MM.CVE_MEDIDA,NULL,' ' ,MM.CVE_MEDIDA||'-'||SMM.DESCR)) AS CVE_MEDIDA_A,
        TO_CHAR((SELECT DECODE(MM.CALCULO,'AC',SUM(ME.VALOR),DECODE(MM.CALCULO,'MI',MIN(ME.VALOR),MAX(ME.VALOR))) AS EST
            FROM POA.MOVOFICIOS_ESTIMACION ME
            WHERE MM.OFICIO = ME.OFICIO
            AND MM.RAMO = ME.RAMO
            AND MM.META = ME.META)) AS EST_A,
        MM.CALCULO AS CVE_MEDIDA_A,
        TCA.DESCR AS TIPO_COMP_A,
        EM.DESCR AS ESTATUS_D,
        TO_CHAR((SELECT
    DECODE(MM.CALCULO,'AC',SUM(AV.VALOR),DECODE(MM.CALCULO,'MI',MIN(AV.VALOR),MAX(AV.VALOR)))
--NVL(SUM(AV.VALOR),0)
FROM
    DGI.AVANCE AV,
    POA.CARATULA_MOVOFICIO CM,
    POA.CARATULA C
WHERE
    AV.YEAR = $P{YEAR} --PARAMETRO YEAR
    AND AV.RAMO = MM.RAMO
    AND AV.META = MM.META
    AND CM.OFICIO = MV.OFICIO
    AND C.ID_CARATULA = CM.ID_CARATULA --PARAMETRO CARATULA
    AND AV.PERIODO <= EXTRACT(MONTH FROM C.FECHA_REVISION))) AS AV_META,
        MM.NVA_CREACION
    FROM
        POA.MOVOFICIOS_META MM,
        POA.PROYECTO PA,
        S_POA_MEDIDA_META SMM,
        DGI.POA_TIPO_COMPROMISO TC,
        DGI.POA_TIPO_COMPROMISO TCA,
        DGI.PARAMETROS P,
        DGI.RAMOS R,
        S_POA_PROGRAMA PRG,
        DGI.VW_SP_PROY_ACT PROY,
        DGI.DEPENDENCIA DEP,
        POA.MOVOFICIOS MV,
        POA.ESTATUS_MOV EM
    WHERE
 R.YEAR = MM.YEAR
    AND R.RAMO = MM.RAMO
    AND PRG.YEAR = MM.YEAR
    AND PRG.PRG = MM.PRG
    AND PROY.YEAR = MM.YEAR
    AND PROY.TIPO_PROY = MM.TIPO_PROY
    AND PROY.PROY = MM.PROY
    AND PA.YEAR = MM.YEAR
    AND PA.RAMO = MM.RAMO
    AND PA.PRG = MM.PRG
    AND PA.TIPO_PROY = MM.TIPO_PROY
    AND PA.PROY = MM.PROY
    AND SMM.YEAR = MM.YEAR
    AND SMM.MEDIDA = MM.CVE_MEDIDA
    AND TC.TIPO_COMPROMISO = MM.TIPO_COMPROMISO
    AND TCA.TIPO_COMPROMISO = MM.TIPO_COMPROMISO
    AND DEP.YEAR (+)= PA.YEAR
    AND DEP.RAMO (+)= PA.RAMO
    AND DEP.DEPTO (+)= PA.DEPTO_RESP
    AND MV.OFICIO = MM.OFICIO
    AND MV.STATUS <> 'A'
    AND MM.NVA_CREACION = 'S'
    AND MV.STATUS = EM.ESTATUS

UNION ALL

SELECT DISTINCT
    M.RAMO,
    R.DESCR AS R_DESC,
    MA.OFICIO AS OFICIO,
    M.PRG,
    PRG.DESCR AS PRG_DESC,
    M.TIPO_PROY,
    M.PROY,
    M.TIPO_PROY||M.PROY AS PROYC,
    PROY.DESCR AS PROY_DEC,
    PA.DEPTO_RESP,
    DEP.DESCR AS DEPTO_DESC,
    M.META AS META,
    M.META||'-'||M.DESCR AS DESCR,
    M.META AS MMETA,
    '' AS DESCR_M,
    M.CVE_MEDIDA||'-'||SMM.DESCR AS MEDIDA,
    (SELECT DECODE(M.CALCULO,'AC',SUM(ME.VALOR),DECODE(M.CALCULO,'MI',MIN(ME.VALOR),MAX(ME.VALOR))) AS EST
     FROM DGI.ESTIMACION ME
     WHERE  M.YEAR = ME.YEAR
            AND M.RAMO = ME.RAMO
            AND M.META = ME.META) EST_H,
     M.CALCULO,
    TC.DESCR AS TIPO_COMP,
    M.LINEA,
    '' AS LINEA_A,
    '' AS DESCR_A,
    '' AS CVE_MEDIDA_A,
    '' AS EST_A,
    '' AS CALCULO_A,
    '' AS TIPO_COMP_A,
    EM.DESCR AS ESTATUS_D,
    TO_CHAR((SELECT
                DECODE(M.CALCULO,'AC',SUM(AV.VALOR),DECODE(M.CALCULO,'MI',MIN(AV.VALOR),MAX(AV.VALOR)))
                --NVL(SUM(AV.VALOR),0)
            FROM
                DGI.AVANCE AV,
                POA.CARATULA C
            WHERE
                AV.YEAR = $P{YEAR} --PARAMETRO YEAR
                AND AV.RAMO = M.RAMO
                AND AV.META = M.META
                AND AV.PERIODO <= EXTRACT(MONTH FROM C.FECHA_REVISION)
)) AS AV_META, 'N' AS NVA_CREACION
FROM
    S_POA_META M,
    POA.MOVOFICIOS_ACCION MA,
    POA.MOVOFICIOS MV,
    DGI.VW_SP_PROY_ACT PROY,
    POA.PROYECTO PA,
    S_POA_MEDIDA_META SMM,
    DGI.DEPENDENCIA DEP,
    S_POA_PROGRAMA PRG,
    DGI.RAMOS R,
    DGI.POA_TIPO_COMPROMISO TC,
    POA.ESTATUS_MOV EM
WHERE
    MV.OFICIO = MA.OFICIO
    AND M.YEAR = MA.YEAR
    AND M.RAMO = MA.RAMO
    AND M.PRG = MA.PRG
    AND PROY.YEAR = M.YEAR
    AND PROY.TIPO_PROY = M.TIPO_PROY
    AND PROY.PROY = M.PROY
    AND M.META = MA.META
    AND MA.OFICIO NOT IN(
        SELECT OFICIO
        FROM POA.MOVOFICIOS_META MM
        WHERE MM.RAMO = MA.RAMO
                AND MM.META = MA.META
    )
    AND PA.YEAR = M.YEAR
    AND PA.RAMO = M.RAMO
    AND PA.PRG = M.PRG
    AND PA.TIPO_PROY = M.TIPO_PROY
    AND PA.PROY = M.PROY
    AND SMM.YEAR (+)= M.YEAR
    AND SMM.MEDIDA (+)= M.CVE_MEDIDA
    AND DEP.YEAR (+)= PA.YEAR
    AND DEP.RAMO (+)= PA.RAMO
    AND DEP.DEPTO (+)= PA.DEPTO_RESP
    AND PRG.YEAR = M.YEAR
    AND PRG.PRG = M.PRG
    AND R.YEAR = M.YEAR
    AND R.RAMO = M.RAMO
    AND TC.TIPO_COMPROMISO (+)= M.TIPO_COMPROMISO
    AND MV.STATUS = EM.ESTATUS

UNION ALL
SELECT DISTINCT MM.RAMO,R.DESCR AS R_DESC,MV.OFICIO,MM.PRG,PRG.DESCR AS PRG_DESC,MM.TIPO_PROY,MM.PROY, MM.TIPO_PROY||MM.PROY AS PROY,PROY.DESCR AS PROY_DEC,PA.DEPTO_RESP,DEP.DESCR AS DEPTO_DESC,MM.META AS META, MM.META||'-'||MM.DESCR AS DESCR,MM.META AS MMETA, MM.META||'-'||MM.DESCR AS DESCR_M, SMM.DESCR AS MEDIDA,
        (CASE MV.STATUS WHEN  'A' THEN
            (SELECT DECODE(MM.CALCULO,'AC',SUM(ME.VALOR),DECODE(MM.CALCULO,'MI',MIN(ME.VALOR),MAX(ME.VALOR)))
             FROM POA.HIST_ESTIMACION ME
             WHERE  MM.YEAR = ME.YEAR
                    AND MM.RAMO = ME.RAMO
                    AND MM.META = ME.META)
             ELSE
                (SELECT DECODE(MM.CALCULO,'AC',SUM(ME.VALOR),DECODE(MM.CALCULO,'MI',MIN(ME.VALOR),MAX(ME.VALOR)))
                 FROM DGI.ESTIMACION ME
                 WHERE  MM.YEAR = ME.YEAR
                        AND MM.RAMO = ME.RAMO
                        AND MM.META = ME.META)
            END
        )EST_H,
        MM.CALCULO AS CALCULO,
        TC.DESCR AS TIPO_COMP,
        MM.LINEA AS LINEA,
        ' ' AS LINEA_A,
        ' ' AS DESCR_A,
        ' ' AS CVE_MEDIDA_A,
        TO_CHAR(DECODE((SELECT DECODE(MM.CALCULO,'AC',SUM(ME.VALOR),DECODE(MM.CALCULO,'MI',MIN(ME.VALOR),MAX(ME.VALOR))) AS EST
            FROM POA.HIST_ESTIMACION ME
            WHERE MV.OFICIO = ME.OFICIO
            AND MM.RAMO = ME.RAMO
            AND MM.META = ME.META)
            ,(SELECT DECODE(MM.CALCULO,'AC',SUM(ME.VALOR),DECODE(MM.CALCULO,'MI',MIN(ME.VALOR),MAX(ME.VALOR))) AS EST
            FROM POA.MOVOFICIOS_ESTIMACION ME
            WHERE MV.OFICIO = ME.OFICIO
            AND MM.RAMO = ME.RAMO
            AND MM.META = ME.META)
            ,NULL,(SELECT DECODE(MM.CALCULO,'AC',SUM(ME.VALOR),DECODE(MM.CALCULO,'MI',MIN(ME.VALOR),MAX(ME.VALOR))) AS EST
            FROM POA.MOVOFICIOS_ESTIMACION ME
            WHERE MV.OFICIO = ME.OFICIO
            AND MM.RAMO = ME.RAMO
            AND MM.META = ME.META))) AS EST_A,
        ' ' AS CVE_MEDIDA_A,
        ' ' AS TIPO_COMP_A,
        EM.DESCR AS ESTATUS_D,
        TO_CHAR((SELECT
        DECODE(MM.CALCULO,'AC',SUM(AV.VALOR),DECODE(MM.CALCULO,'MI',MIN(AV.VALOR),MAX(AV.VALOR)))
                --NVL(SUM(AV.VALOR),0)
            FROM
                DGI.AVANCE AV,
                POA.CARATULA C
            WHERE
                AV.YEAR = MM.YEAR --PARAMETRO YEAR
                AND AV.RAMO = MM.RAMO
                AND AV.META = MM.META
                AND AV.PERIODO <= EXTRACT(MONTH FROM C.FECHA_REVISION)
        )) AS AV_META, 'N' AS NVA_CREACION
    FROM
        S_POA_META MM,
        POA.MOVOFICIOS_ESTIMACION MES,
        POA.PROYECTO PA,
        S_POA_MEDIDA_META SMM,
        DGI.POA_TIPO_COMPROMISO TC,
        DGI.PARAMETROS P,
        DGI.RAMOS R,
        S_POA_PROGRAMA PRG,
        DGI.VW_SP_PROY_ACT PROY,
        DGI.DEPENDENCIA DEP,
        POA.MOVOFICIOS MV,
        POA.ESTATUS_MOV EM
    WHERE
    MES.OFICIO = MV.OFICIO
    AND MM.YEAR = MES.YEAR
    AND MM.RAMO = MES.RAMO
    AND MM.META = MES.META
    AND R.YEAR = MM.YEAR
    AND R.RAMO = MM.RAMO
    AND PRG.YEAR = MM.YEAR
    AND PRG.PRG = MM.PRG
    AND PROY.YEAR = MM.YEAR
    AND PROY.TIPO_PROY = MM.TIPO_PROY
    AND PROY.PROY = MM.PROY
    AND PA.YEAR = MM.YEAR
    AND PA.RAMO = MM.RAMO
    AND PA.PRG = MM.PRG
    AND PA.TIPO_PROY = MM.TIPO_PROY
    AND PA.PROY = MM.PROY
    AND SMM.YEAR = MM.YEAR
    AND SMM.MEDIDA = MM.CVE_MEDIDA
    AND TC.TIPO_COMPROMISO = MM.TIPO_COMPROMISO
    AND DEP.YEAR (+)= PA.YEAR
    AND DEP.RAMO (+)= PA.RAMO
    AND DEP.DEPTO (+)= PA.DEPTO_RESP
    AND MV.STATUS = EM.ESTATUS
    AND MV.TIPOMOV = 'C'
UNION ALL

SELECT DISTINCT MM.RAMO,R.DESCR AS R_DESC,MV.OFICIO,MM.PRG,PRG.DESCR AS PRG_DESC,MM.TIPO_PROY,MM.PROY, MM.TIPO_PROY||MM.PROY AS PROY,PROY.DESCR AS PROY_DEC,PA.DEPTO_RESP,DEP.DESCR AS DEPTO_DESC,MM.META AS META, MM.META||'-'||MM.DESCR AS DESCR,MM.META AS MMETA, MM.META||'-'||MM.DESCR AS DESCR_M, SMM.DESCR AS MEDIDA,
        (SELECT DECODE(MM.CALCULO,'AC',SUM(ME.VALOR),DECODE(MM.CALCULO,'MI',MIN(ME.VALOR),MAX(ME.VALOR)))
                 FROM DGI.ESTIMACION ME
                 WHERE  MM.YEAR = ME.YEAR
                        AND MM.RAMO = ME.RAMO
                        AND MM.META = ME.META
        ) EST_H,
        MM.CALCULO AS CALCULO,
        TC.DESCR AS TIPO_COMP,
        MM.LINEA AS LINEA,
        ' ' AS LINEA_A,
        ' ' AS DESCR_A,
        ' ' AS CVE_MEDIDA_A,
        TO_CHAR(DECODE((SELECT DECODE(MM.CALCULO,'AC',SUM(ME.VALOR),DECODE(MM.CALCULO,'MI',MIN(ME.VALOR),MAX(ME.VALOR))) AS EST
            FROM POA.HIST_ESTIMACION ME
            WHERE MV.OFICIO = ME.OFICIO
            AND MM.RAMO = ME.RAMO
            AND MM.META = ME.META)
            ,(SELECT DECODE(MM.CALCULO,'AC',SUM(ME.VALOR),DECODE(MM.CALCULO,'MI',MIN(ME.VALOR),MAX(ME.VALOR))) AS EST
            FROM POA.MOVOFICIOS_ESTIMACION ME
            WHERE MV.OFICIO = ME.OFICIO
            AND MM.RAMO = ME.RAMO
            AND MM.META = ME.META)
            ,NULL,(SELECT DECODE(MM.CALCULO,'AC',SUM(ME.VALOR),DECODE(MM.CALCULO,'MI',MIN(ME.VALOR),MAX(ME.VALOR))) AS EST
            FROM POA.MOVOFICIOS_ESTIMACION ME
            WHERE MV.OFICIO = ME.OFICIO
            AND MM.RAMO = ME.RAMO
            AND MM.META = ME.META))) AS EST_A,
        ' ' AS CVE_MEDIDA_A,
        ' ' AS TIPO_COMP_A,
        EM.DESCR AS ESTATUS_D,
        TO_CHAR((SELECT
        DECODE(MM.CALCULO,'AC',SUM(AV.VALOR),DECODE(MM.CALCULO,'MI',MIN(AV.VALOR),MAX(AV.VALOR)))
                --NVL(SUM(AV.VALOR),0)
            FROM
                DGI.AVANCE AV,
                POA.CARATULA C
            WHERE
                AV.YEAR = MM.YEAR --PARAMETRO YEAR
                AND AV.RAMO = MM.RAMO
                AND AV.META = MM.META
                AND AV.PERIODO <= EXTRACT(MONTH FROM C.FECHA_REVISION)
        )) AS AV_META, 'N' AS NVA_CREACION
    FROM
        S_POA_META MM,
        POA.MOVOFICIOS_ACCION_ESTIMACION MES,
        POA.PROYECTO PA,
        S_POA_MEDIDA_META SMM,
        DGI.POA_TIPO_COMPROMISO TC,
        DGI.PARAMETROS P,
        DGI.RAMOS R,
        S_POA_PROGRAMA PRG,
        DGI.VW_SP_PROY_ACT PROY,
        DGI.DEPENDENCIA DEP,
        POA.MOVOFICIOS MV,
        POA.ESTATUS_MOV EM
    WHERE
    MES.OFICIO = MV.OFICIO
    AND MM.YEAR = MES.YEAR
    AND MM.RAMO = MES.RAMO
    AND MM.META = MES.META
    AND R.YEAR = MM.YEAR
    AND R.RAMO = MM.RAMO
    AND PRG.YEAR = MM.YEAR
    AND PRG.PRG = MM.PRG
    AND PROY.YEAR = MM.YEAR
    AND PROY.TIPO_PROY = MM.TIPO_PROY
    AND PROY.PROY = MM.PROY
    AND PA.YEAR = MM.YEAR
    AND PA.RAMO = MM.RAMO
    AND PA.PRG = MM.PRG
    AND PA.TIPO_PROY = MM.TIPO_PROY
    AND PA.PROY = MM.PROY
    AND SMM.YEAR = MM.YEAR
    AND SMM.MEDIDA = MM.CVE_MEDIDA
    AND TC.TIPO_COMPROMISO = MM.TIPO_COMPROMISO
    AND DEP.YEAR (+)= PA.YEAR
    AND DEP.RAMO (+)= PA.RAMO
    AND DEP.DEPTO (+)= PA.DEPTO_RESP
    AND MV.STATUS = EM.ESTATUS
    AND MV.TIPOMOV = 'C'
) TR,
DGI.PARAMETROS P,
DGI.RAMOS R,
POA.CARATULA_MOVOFICIO CM
WHERE
($P!{QUERY})
AND R.YEAR = $P{YEAR}
AND R.RAMO = TR.RAMO
AND CM.OFICIO (+) = TR.OFICIO
ORDER BY TR.RAMO, TR.PRG,TR.PROY||','||TR.TIPO_PROY,TR.DEPTO_RESP, TR.META DESC, TR.OFICIO]]>
	</queryString>
