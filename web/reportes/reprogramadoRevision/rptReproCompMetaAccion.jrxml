<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="rptReproCompMetaAccion" language="groovy" pageWidth="1120" pageHeight="612" orientation="Landscape" whenNoDataType="NoDataSection" columnWidth="1080" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" uuid="6084646e-3180-4c1f-a43b-4c39db818c37">
