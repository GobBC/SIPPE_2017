	<queryString>
		<![CDATA[SELECT
	NVL(ENE,0) IMP1,
	NVL(FEB,0) IMP2,
	NVL(MAR,0) IMP3,
	NVL(ABR,0) IMP4,
	NVL(MAY,0) IMP5,
	NVL(JUN,0) IMP6,
	NVL(JUL,0) IMP7,
	NVL(AGO,0) IMP8,
	NVL(SEP,0) IMP9,
	NVL(OCT,0) IMP10,
	NVL(NOV,0) IMP11,
	NVL(DIC,0) IMP12
FROM
	(
		SELECT
			PERIODO,
			VALOR,META
		FROM
			POA.MOVOFICIOS_ACCION_ESTIMACION
		WHERE
            OFICIO = $P{OFICIO} AND
			RAMO = $P{RAMO} AND
			META = $P{META} AND
            ACCION = $P{ACCION}
	 )
PIVOT
	(
		SUM(NVL(VALOR,0))
		FOR PERIODO IN(1 ENE, 2 FEB, 3 MAR, 4 ABR, 5 MAY, 6 JUN, 7 JUL, 8 AGO, 9 SEP, 10 OCT, 11 NOV, 12 DIC)
	)]]>
	</queryString>
