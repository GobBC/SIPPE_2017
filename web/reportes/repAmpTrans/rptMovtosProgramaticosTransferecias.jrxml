FROM
SPP.TRANSFERENCIAS T
WHERE T.OFICIO=$P{OFICIO}
AND(LENGTH(NVL($P{TIPOFICIO},'00'))>1)
GROUP BY T.RAMO,T.META
UNION
SELECT T.RAMO,T.META
