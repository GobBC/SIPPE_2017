	<queryString>
		<![CDATA[SELECT A.RAMO,A.META
FROM
SPP.AMPLIACIONES A,
DGI.OFICONS O
WHERE A.OFICIO=$P{OFICIO}
AND O.OFICIO=A.OFICIO
AND O.CONSEC=A.CONSEC
AND O.TIPO=$P{TIPOFICIO}
AND  (LENGTH(NVL($P{TIPOFICIO},'00'))=1)
GROUP BY A.RAMO,A.META

UNION

SELECT A.RAMO,A.META
FROM
SPP.AMPLIACIONES A
WHERE A.OFICIO=$P{OFICIO}
AND  (LENGTH(NVL($P{TIPOFICIO},'00'))>1)
GROUP BY A.RAMO,A.META]]>
