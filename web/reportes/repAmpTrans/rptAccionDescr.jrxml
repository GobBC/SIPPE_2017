	<queryString>
		<![CDATA[SELECT
A.RAMO AS RAMO,
A.ACCION AS ACCION,
A.DESCR AS ACCION_DESCR,
MED.DESCR AS MEDIDA_DESCR,
L.DESCR AS LOCALIDAD_DESCR,
A.CALCULO AS CALCULO
FROM
DGI.MEDIDA_META MED,
DGI.LOCALIDAD L,
S_POA_ACCION A
WHERE
A.CVE_MEDIDA=MED.MEDIDA
AND A.YEAR=MED.YEAR
AND A.META=$P{META}
AND A.DEPTO=$P{DEPTO}
AND A.ACCION=$P{ACCION}
AND A.YEAR=$P{YEAR}
AND A.RAMO=$P{RAMO}
AND A.LOCALIDAD=L.LOCALIDAD
AND A.MPO=L.MPO
AND A.ACCION IS NOT NULL

UNION

SELECT
AA.RAMO AS RAMO,
AA.ACCION AS ACCION,
AA.DESCR AS ACCION_DESCR,
MED.DESCR AS MEDIDA_DESCR,
L.DESCR AS LOCALIDAD_DESCR,
AA.CALCULO AS CALCULO
FROM
DGI.MEDIDA_META MED,
DGI.LOCALIDAD L,
POA.MOVOFICIOS_ACCION AA
WHERE
AA.CVE_MEDIDA=MED.MEDIDA
AND AA.YEAR=MED.YEAR
AND AA.OFICIO=$P{OFICIO}
AND AA.META=$P{META}
AND AA.DEPTO=$P{DEPTO}
AND AA.ACCION=$P{ACCION}
AND AA.YEAR=$P{YEAR}
AND AA.RAMO=$P{RAMO}
AND AA.LOCALIDAD=L.LOCALIDAD
AND AA.MPO=L.MPO
AND AA.ACCION IS NOT NULL]]>
