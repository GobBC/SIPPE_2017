	<queryString>
		<![CDATA[SELECT
M.RAMO AS RAMO,
M.META AS META,
M.DESCR AS META_DESCR,
MED.DESCR AS MEDIDA_DESCR,
M.CALCULO AS CALCULO
FROM
DGI.MEDIDA_META MED,
S_POA_META M,
POA.PROYECTO P
WHERE
M.META=$P{META}
AND M.YEAR=$P{YEAR}
AND M.RAMO=$P{RAMO}
AND M.CVE_MEDIDA=MED.MEDIDA
AND M.YEAR=MED.YEAR
AND P.PROY=M.PROY
AND P.RAMO=M.RAMO
AND P.TIPO_PROY=M.TIPO_PROY
AND P.PRG=M.PRG
AND M.META IS NOT NULL

UNION

SELECT
MM.RAMO AS RAMO,
MM.META AS META,
MM.DESCR AS META_DESCR,
MED.DESCR AS MEDIDA_DESCR,
MM.CALCULO AS CALCULO
FROM
DGI.MEDIDA_META MED,
POA.MOVOFICIOS_META MM,
POA.PROYECTO P
WHERE
 MM.OFICIO=$P{OFICIO}
AND MM.YEAR=MED.YEAR
AND MM.RAMO=$P{RAMO}
AND MM.CVE_MEDIDA=MED.MEDIDA
AND MM.META=$P{META}
AND P.PROY=MM.PROY
AND P.RAMO=MM.RAMO
AND P.TIPO_PROY=MM.TIPO_PROY
AND P.PRG=MM.PRG
AND MM.META IS NOT NULL]]>
