	<queryString>
		<![CDATA[SELECT AMP.OFICIO,
         OFI.TIPO,
         MOVS.FECHAELAB,
         P.GOBIERNO,
         P.SECRETARIA,
         MOVS.STATUS,
         P.LOGO,
DBMS_LOB.substr(MOVS.JUSTIFICACION, DBMS_LOB.getlength(MOVS.JUSTIFICACION), 1) as JUSTIFICACION
    FROM SPP.AMPLIACIONES AMP,
         DGI.OFICONS OFI,
         POA.MOVOFICIOS MOVS,
         DGI.PARAMETROS P,
         S_DGI_PARAM_LONG_COD PLC
   WHERE ( AMP.OFICIO = OFI.OFICIO ) and
         ( AMP.CONSEC = OFI.CONSEC ) and
         ( MOVS.OFICIO = AMP.OFICIO ) and
         ( ( OFI.OFICIO = $P{OFICIO}) AND
         ( OFI.TIPO =$P{TIPOFICIO} ) ) AND
         PLC.YEAR = $P{YEAR} AND
         (LENGTH(NVL($P{TIPOFICIO},'00'))=1)
         GROUP BY
         AMP.OFICIO,
         OFI.TIPO,
         MOVS.FECHAELAB,
         P.GOBIERNO,
         P.SECRETARIA,
         MOVS.STATUS,
         P.LOGO,
        DBMS_LOB.substr(MOVS.JUSTIFICACION, DBMS_LOB.getlength(MOVS.JUSTIFICACION), 1)

UNION

        SELECT AMP.OFICIO,
         '' AS TIPO,
         MOVS.FECHAELAB,
         P.GOBIERNO,
         P.SECRETARIA,
         MOVS.STATUS,
         P.LOGO,
DBMS_LOB.substr(MOVS.JUSTIFICACION, DBMS_LOB.getlength(MOVS.JUSTIFICACION), 1) as JUSTIFICACION
    FROM SPP.AMPLIACIONES AMP,
                 POA.MOVOFICIOS MOVS,
         DGI.PARAMETROS P,
         S_DGI_PARAM_LONG_COD PLC
   WHERE
         ( MOVS.OFICIO = AMP.OFICIO ) and
          AMP.OFICIO=$P{OFICIO} AND
         PLC.YEAR = $P{YEAR} AND
         (LENGTH(NVL($P{TIPOFICIO},'00'))>1 )
         GROUP BY
         AMP.OFICIO,
         MOVS.FECHAELAB,
         P.GOBIERNO,
         P.SECRETARIA,
         MOVS.STATUS,
         P.LOGO,
         DBMS_LOB.substr(MOVS.JUSTIFICACION, DBMS_LOB.getlength(MOVS.JUSTIFICACION), 1)]]>
	</queryString>
