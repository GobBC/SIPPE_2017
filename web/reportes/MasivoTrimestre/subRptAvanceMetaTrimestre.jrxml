	<queryString>
		<![CDATA[SELECT MET.META,
       MET.RAMO,
       MET.YEAR,
       MET.LINEA,
       MET.CLAVE,
 (SELECT max(length(DGI.AVANCE.OBSERVA)) as valor
       FROM DGI.AVANCE,
         DGI.ESTIMACION_PERIODO
   WHERE ( DGI.AVANCE.PERIODO = DGI.ESTIMACION_PERIODO.PERIODO ) and
         ( ( DGI.AVANCE.META = MET.META ) AND
         ( DGI.AVANCE.RAMO = MET.RAMO ) AND
         ( DGI.AVANCE.YEAR = MET.YEAR ) AND
			( DGI.ESTIMACION_PERIODO.EVALUACION <= $P{PERIODO}) and
         (DGI.AVANCE.PERIODO IN(3,6,9,12)) AND
         ( DGI.AVANCE.OBSERVA is not NULL )  )) AS VALOR

FROM   S_POA_META MET
WHERE ( MET.RAMO = $P{RAMO} ) AND
      ( MET.YEAR = $P{YEAR} ) AND
      ( MET.PRG = $P{PROG} ) AND
      ( MET.TIPO_PROY = $P{TIPO_PROY} ) AND
      ( MET.PROY = $P{PROY} )]]>
	</queryString>
