	<queryString>
		<![CDATA[SELECT
	PROY.TIPO_PROY
	,PROY.PROY
	,VWSPPROYACT.DESCR PROY_DESCR
FROM
	POA.PROYECTO PROY,
	DGI.VW_SP_PROY_ACT VWSPPROYACT

WHERE
	PROY.PROY = $P{PROY}
	AND PROY.RAMO = $P{RAMO}
	AND PROY.PRG = $P{PRG}
	AND PROY.YEAR = $P{YEAR}
	AND PROY.TIPO_PROY = $P{TIPO_PROY}
	AND PROY.DEPTO_RESP = $P{DEPTO}
	AND VWSPPROYACT.YEAR = PROY.YEAR
	AND VWSPPROYACT.TIPO_PROY = PROY.TIPO_PROY
	AND VWSPPROYACT.PROY = PROY.PROY

ORDER BY
	PROY.PROY,
	PROY.TIPO_PROY]]>
