	<queryString>
		<![CDATA[SELECT BE.IDENTIFICADOR,
    CM.RAMO,
    CM.RAMO||' '||R.DESCR RDESC,
    CM.OFICIO,
    POA.F_DESCR_CARATULA(CM.YEAR,CM.RAMO,CM.ID_CARATULA) CARDESC,
    BE.APP_LOGIN,
    TO_CHAR(BE.FECHA_EVALUACION,'DD/MM/YYYY HH:MM') FECHA_EVALUACION,
    P.SECRETARIA,
    P.GOBIERNO
FROM
    POA.BITACORA_EVALUACION BE,
    POA.CARATULA_MOVOFICIO CM,
    POA.MOVOFICIOS M,
    DGI.PARAMETROS P,
    DGI.RAMOS R
WHERE
    R.YEAR = $P{YEAR}
    AND CM.RAMO = R.RAMO
    AND BE.IDENTIFICADOR = CM.ID_CARATULA
    AND M.OFICIO = CM.OFICIO
    AND R.YEAR = CM.YEAR
    AND EXTRACT(YEAR FROM M.FECHAELAB) = R.YEAR
ORDER BY BE.IDENTIFICADOR,CM.RAMO, CM.OFICIO]]>
	</queryString>
