	</parameter>
	<parameter name="RAMO" class="java.lang.String"/>
	<parameter name="ID_CARATULA" class="java.lang.String"/>
	<parameter name="FECHA_REVISION" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
	PPTO_INI.YEAR,
	PPTO_INI.RAMO,
	(
		SUM(NVL(PPTO_INI.ENE,0)) +
		SUM(NVL(PPTO_INI.FEB,0)) +
		SUM(NVL(PPTO_INI.MZO,0)) +
		SUM(NVL(PPTO_INI.ABR,0)) +
		SUM(NVL(PPTO_INI.MAY,0)) +
		SUM(NVL(PPTO_INI.JUN,0)) +
		SUM(NVL(PPTO_INI.JUL,0)) +
		SUM(NVL(PPTO_INI.AGO,0)) +
		SUM(NVL(PPTO_INI.SEP,0)) +
		SUM(NVL(PPTO_INI.OCT,0)) +
		SUM(NVL(PPTO_INI.NOV,0)) +
		SUM(NVL(PPTO_INI.DIC,0))
	) PPTO_INICIAL,
	(
		SELECT
			NVL(SUM(PPTO_ACUM.AMPS),0)
		FROM
			(
				SELECT
					EXTRACT(YEAR FROM MOVOF.FECPPTO) YEAR,
					AMP.RAMO,
					(
						SELECT
							SG.GRUPO
						FROM
							S_DGI_SUBGRUPOS SG
						WHERE
							SG.YEAR = EXTRACT(YEAR FROM MOVOF.FECPPTO) AND
							SG.SUBGRUPO = (
											SELECT
												SSG.SUBGRUPO
											FROM
												S_DGI_SUBSUBGPO SSG
											WHERE
												SSG.YEAR = EXTRACT(YEAR FROM MOVOF.FECPPTO) AND
												SSG.SUBSUBGRUPO = (
																	SELECT
																		PAR.SUBSUBGPO
																	FROM
																		DGI.PARTIDA PAR
																	WHERE
																		PAR.YEAR = EXTRACT(YEAR FROM MOVOF.FECPPTO) AND
																		PAR.PARTIDA = AMP.PARTIDA
																	)
											)
					) GRUPO,
					SUM(CASE WHEN AMP.TIPOMOV IN('A','C') THEN AMP.IMPTE ELSE 0 END) AMPS
				FROM
					POA.MOVOFICIOS MOVOF,
					SPP.AMPLIACIONES AMP
				WHERE
					MOVOF.FECPPTO < TO_DATE($P{FECHA_REVISION},'dd/MM/yyyy') AND
					MOVOF.STATUS = 'A' AND
					AMP.RAMO = $P{RAMO} AND
					AMP.OFICIO = MOVOF.OFICIO
				GROUP BY
					MOVOF.FECPPTO,
					AMP.RAMO,
					AMP.PARTIDA

				UNION ALL

				SELECT
					EXTRACT(YEAR FROM MOVOF.FECPPTO) YEAR,
					TR.RAMO,
					(
						SELECT
							SG.GRUPO
						FROM
							S_DGI_SUBGRUPOS SG
						WHERE
							SG.YEAR = EXTRACT(YEAR FROM MOVOF.FECPPTO) AND
							SG.SUBGRUPO = (
											SELECT
												SSG.SUBGRUPO
											FROM
												S_DGI_SUBSUBGPO SSG
											WHERE
												SSG.YEAR = EXTRACT(YEAR FROM MOVOF.FECPPTO) AND
												SSG.SUBSUBGRUPO = (
																	SELECT
																		PAR.SUBSUBGPO
																	FROM
																		DGI.PARTIDA PAR
																	WHERE
																		PAR.YEAR = EXTRACT(YEAR FROM MOVOF.FECPPTO) AND
																		PAR.PARTIDA = TR.PARTIDA
																	)
											)
					) GRUPO,
					SUM(TR.IMPTE) AMPS
				FROM
					POA.MOVOFICIOS MOVOF,
					SPP.TRANSFREC TR
				WHERE
					MOVOF.FECPPTO < TO_DATE($P{FECHA_REVISION},'dd/MM/yyyy') AND
					MOVOF.STATUS = 'A' AND
					TR.RAMO = $P{RAMO} AND
					TR.OFICIO = MOVOF.OFICIO
				GROUP BY
					MOVOF.FECPPTO,
					TR.RAMO,
					TR.PARTIDA

			) PPTO_ACUM
		WHERE
			PPTO_ACUM.YEAR = PPTO_INI.YEAR AND
			PPTO_ACUM.RAMO = PPTO_INI.RAMO


	) AMP_ACUM,
	(
		SELECT
			NVL(SUM(PPTO_ACUM.DISM),0)
		FROM
			(
				SELECT
					EXTRACT(YEAR FROM MOVOF.FECPPTO) YEAR,
					AMP.RAMO,
					(
						SELECT
							SG.GRUPO
						FROM
							S_DGI_SUBGRUPOS SG
						WHERE
							SG.YEAR = EXTRACT(YEAR FROM MOVOF.FECPPTO) AND
							SG.SUBGRUPO = (
											SELECT
												SSG.SUBGRUPO
											FROM
												S_DGI_SUBSUBGPO SSG
											WHERE
												SSG.YEAR = EXTRACT(YEAR FROM MOVOF.FECPPTO) AND
												SSG.SUBSUBGRUPO = (
																	SELECT
																		PAR.SUBSUBGPO
																	FROM
																		DGI.PARTIDA PAR
																	WHERE
																		PAR.YEAR = EXTRACT(YEAR FROM MOVOF.FECPPTO) AND
																		PAR.PARTIDA = AMP.PARTIDA
																	)
								)
					) GRUPO,
					SUM(CASE WHEN AMP.TIPOMOV = 'R' THEN AMP.IMPTE ELSE 0 END) DISM
				FROM
					POA.MOVOFICIOS MOVOF,
					SPP.AMPLIACIONES AMP
				WHERE
					MOVOF.FECPPTO < TO_DATE($P{FECHA_REVISION},'dd/MM/yyyy') AND
					MOVOF.STATUS = 'A' AND
					AMP.RAMO = $P{RAMO} AND
					AMP.OFICIO = MOVOF.OFICIO
				GROUP BY
					MOVOF.FECPPTO,
					AMP.RAMO,
					AMP.PARTIDA

				UNION ALL

				SELECT
					EXTRACT(YEAR FROM MOVOF.FECPPTO) YEAR,
					TRANSF.RAMO,
					(
						SELECT
							SG.GRUPO
						FROM
							S_DGI_SUBGRUPOS SG
						WHERE
							SG.YEAR = EXTRACT(YEAR FROM MOVOF.FECPPTO) AND
							SG.SUBGRUPO = (
											SELECT
												SSG.SUBGRUPO
											FROM
												S_DGI_SUBSUBGPO SSG
											WHERE
												SSG.YEAR = EXTRACT(YEAR FROM MOVOF.FECPPTO) AND
												SSG.SUBSUBGRUPO = (
																	SELECT
																		PAR.SUBSUBGPO
																	FROM
																		DGI.PARTIDA PAR
																	WHERE
																		PAR.YEAR = EXTRACT(YEAR FROM MOVOF.FECPPTO) AND
																		PAR.PARTIDA = TRANSF.PARTIDA
																	)
								)
					) GRUPO,
					SUM(TRANSF.IMPTE) DISM
				FROM
					POA.MOVOFICIOS MOVOF,
					SPP.TRANSFERENCIAS TRANSF
				WHERE
					MOVOF.FECPPTO < TO_DATE($P{FECHA_REVISION},'dd/MM/yyyy') AND
					MOVOF.STATUS = 'A' AND
					TRANSF.RAMO = $P{RAMO} AND
					TRANSF.OFICIO = MOVOF.OFICIO
				GROUP BY
					MOVOF.FECPPTO,
					TRANSF.RAMO,
					TRANSF.PARTIDA
			) PPTO_ACUM
		WHERE
			PPTO_ACUM.YEAR = PPTO_INI.YEAR AND
			PPTO_ACUM.RAMO = PPTO_INI.RAMO

	) DISM_ACUM,
	(
		SELECT
			NVL(SUM(PPTO_ACUM.AMPS),0)
		FROM
			(
				SELECT
					EXTRACT(YEAR FROM MOVOF.FECPPTO) YEAR,
					AMP.RAMO,
					(
						SELECT
							SG.GRUPO
						FROM
							S_DGI_SUBGRUPOS SG
						WHERE
							SG.YEAR = EXTRACT(YEAR FROM MOVOF.FECPPTO) AND
							SG.SUBGRUPO = (
											SELECT
												SSG.SUBGRUPO
											FROM
												S_DGI_SUBSUBGPO SSG
											WHERE
												SSG.YEAR = EXTRACT(YEAR FROM MOVOF.FECPPTO) AND
												SSG.SUBSUBGRUPO = (
																	SELECT
																		PAR.SUBSUBGPO
																	FROM
																		DGI.PARTIDA PAR
																	WHERE
																		PAR.YEAR = EXTRACT(YEAR FROM MOVOF.FECPPTO) AND
																		PAR.PARTIDA = AMP.PARTIDA
																	)
								)
					) GRUPO,
					SUM(CASE WHEN AMP.TIPOMOV IN('A','C') THEN AMP.IMPTE ELSE 0 END) AMPS
				FROM
					POA.CARATULA_MOVOFICIO CARMOVOF,
					POA.MOVOFICIOS MOVOF,
					SPP.AMPLIACIONES AMP
				WHERE
					CARMOVOF.ID_CARATULA = $P{ID_CARATULA} AND
					CARMOVOF.YEAR = $P{YEAR} AND
					CARMOVOF.RAMO = $P{RAMO} AND
					MOVOF.OFICIO = CARMOVOF.OFICIO AND
					MOVOF.FECPPTO >= TO_DATE($P{FECHA_REVISION},'dd/MM/yyyy') AND
					MOVOF.STATUS = 'A' AND
					AMP.RAMO = CARMOVOF.RAMO AND
					AMP.OFICIO = MOVOF.OFICIO
				GROUP BY
					MOVOF.FECPPTO,
					AMP.RAMO,
					AMP.PARTIDA

				UNION ALL

				SELECT
					EXTRACT(YEAR FROM MOVOF.FECPPTO) YEAR,
					TR.RAMO,
					(
						SELECT
							SG.GRUPO
						FROM
							S_DGI_SUBGRUPOS SG
						WHERE
							SG.YEAR = EXTRACT(YEAR FROM MOVOF.FECPPTO) AND
							SG.SUBGRUPO = (
											SELECT
												SSG.SUBGRUPO
											FROM
												S_DGI_SUBSUBGPO SSG
											WHERE
												SSG.YEAR = EXTRACT(YEAR FROM MOVOF.FECPPTO) AND
												SSG.SUBSUBGRUPO = (
																	SELECT
																		PAR.SUBSUBGPO
																	FROM
																		DGI.PARTIDA PAR
																	WHERE
																		PAR.YEAR = EXTRACT(YEAR FROM MOVOF.FECPPTO) AND
																		PAR.PARTIDA = TR.PARTIDA
																	)
								)
					) GRUPO,
					SUM(TR.IMPTE) AMPS
				FROM
					POA.CARATULA_MOVOFICIO CARMOVOF,
					POA.MOVOFICIOS MOVOF,
					SPP.TRANSFREC TR
				WHERE
					CARMOVOF.ID_CARATULA = $P{ID_CARATULA} AND
					CARMOVOF.YEAR = $P{YEAR} AND
					CARMOVOF.RAMO = $P{RAMO} AND
					MOVOF.OFICIO = CARMOVOF.OFICIO AND
					MOVOF.FECPPTO >= TO_DATE($P{FECHA_REVISION},'dd/MM/yyyy') AND
					MOVOF.STATUS = 'A' AND
					TR.RAMO = CARMOVOF.RAMO AND
					TR.OFICIO = MOVOF.OFICIO
				GROUP BY
					MOVOF.FECPPTO,
					TR.RAMO,
					TR.PARTIDA
			) PPTO_ACUM
		WHERE
			PPTO_ACUM.YEAR = PPTO_INI.YEAR AND
			PPTO_ACUM.RAMO = PPTO_INI.RAMO

	) AMP_CARATULA,
	(
		SELECT
			NVL(SUM(PPTO_ACUM.DISM),0)
		FROM
			(
				SELECT
					EXTRACT(YEAR FROM MOVOF.FECPPTO) YEAR,
					AMP.RAMO,
					(
						SELECT
							SG.GRUPO
						FROM
							S_DGI_SUBGRUPOS SG
						WHERE
							SG.YEAR = EXTRACT(YEAR FROM MOVOF.FECPPTO) AND
							SG.SUBGRUPO = (
											SELECT
												SSG.SUBGRUPO
											FROM
												S_DGI_SUBSUBGPO SSG
											WHERE
												SSG.YEAR = EXTRACT(YEAR FROM MOVOF.FECPPTO) AND
												SSG.SUBSUBGRUPO = (
																	SELECT
																		PAR.SUBSUBGPO
																	FROM
																		DGI.PARTIDA PAR
																	WHERE
																		PAR.YEAR = EXTRACT(YEAR FROM MOVOF.FECPPTO) AND
																		PAR.PARTIDA = AMP.PARTIDA
																	)
								)
					) GRUPO,
					SUM(CASE WHEN AMP.TIPOMOV = 'R' THEN AMP.IMPTE ELSE 0 END) DISM
				FROM
					POA.CARATULA_MOVOFICIO CARMOVOF,
					POA.MOVOFICIOS MOVOF,
					SPP.AMPLIACIONES AMP
				WHERE
					CARMOVOF.ID_CARATULA = $P{ID_CARATULA} AND
					CARMOVOF.YEAR = $P{YEAR} AND
					CARMOVOF.RAMO = $P{RAMO} AND
					MOVOF.OFICIO = CARMOVOF.OFICIO AND
					MOVOF.FECPPTO >= TO_DATE($P{FECHA_REVISION},'dd/MM/yyyy') AND
					MOVOF.STATUS = 'A' AND
					AMP.RAMO = CARMOVOF.RAMO AND
					AMP.OFICIO = MOVOF.OFICIO
				GROUP BY
					MOVOF.FECPPTO,
					AMP.RAMO,
					AMP.PARTIDA

				UNION ALL

				SELECT
					EXTRACT(YEAR FROM MOVOF.FECPPTO) YEAR,
					TRANSF.RAMO,
					(
						SELECT
							SG.GRUPO
						FROM
							S_DGI_SUBGRUPOS SG
						WHERE
							SG.YEAR = EXTRACT(YEAR FROM MOVOF.FECPPTO) AND
							SG.SUBGRUPO = (
											SELECT
												SSG.SUBGRUPO
											FROM
												S_DGI_SUBSUBGPO SSG
											WHERE
												SSG.YEAR = EXTRACT(YEAR FROM MOVOF.FECPPTO) AND
												SSG.SUBSUBGRUPO = (
																	SELECT
																		PAR.SUBSUBGPO
																	FROM
																		DGI.PARTIDA PAR
																	WHERE
																		PAR.YEAR = EXTRACT(YEAR FROM MOVOF.FECPPTO) AND
																		PAR.PARTIDA = TRANSF.PARTIDA
																	)
								)
					) GRUPO,
					SUM(TRANSF.IMPTE) DISM
				FROM
					POA.CARATULA_MOVOFICIO CARMOVOF,
					POA.MOVOFICIOS MOVOF,
					SPP.TRANSFERENCIAS TRANSF
				WHERE
					CARMOVOF.ID_CARATULA = $P{ID_CARATULA} AND
					CARMOVOF.YEAR = $P{YEAR} AND
					CARMOVOF.RAMO = $P{RAMO} AND
					MOVOF.OFICIO = CARMOVOF.OFICIO AND
					MOVOF.FECPPTO >= TO_DATE($P{FECHA_REVISION},'dd/MM/yyyy') AND
					MOVOF.STATUS = 'A' AND
					TRANSF.RAMO = CARMOVOF.RAMO AND
					TRANSF.OFICIO = MOVOF.OFICIO
				GROUP BY
					MOVOF.FECPPTO,
					TRANSF.RAMO,
					TRANSF.PARTIDA
			) PPTO_ACUM
		WHERE
			PPTO_ACUM.YEAR = PPTO_INI.YEAR AND
			PPTO_ACUM.RAMO = PPTO_INI.RAMO

	) DISM_CARATULA,
	(
		SELECT
			NVL(SUM(INGCIM.DEVENGADO),0)
		FROM
			S_POA_VW_CIERRE_IEM INGCIM
		WHERE
			INGCIM.EJERCICIO = TO_CHAR(TO_NUMBER(PPTO_INI.YEAR)-1) AND
			INGCIM.RAMO_PPTAL = PPTO_INI.RAMO
	) CIERRE_ANT
FROM
	(
		SELECT
			PPTO.YEAR,
			PPTO.RAMO,
			GPO.GRUPO AS GRUPO,
			GPO.DESCR AS GRUPO_DESCR,
			DECODE(SPP.PPTO.MES,1,SUM(NVL(SPP.PPTO.ASIGNADO,0)),0) AS ENE,
			DECODE(SPP.PPTO.MES,2,SUM(NVL(SPP.PPTO.ASIGNADO,0)),0) AS FEB,
			DECODE(SPP.PPTO.MES,3,SUM(NVL(SPP.PPTO.ASIGNADO,0)),0) AS MZO,
 			DECODE(SPP.PPTO.MES,4,SUM(NVL(SPP.PPTO.ASIGNADO,0)),0) AS ABR,
			DECODE(SPP.PPTO.MES,5,SUM(NVL(SPP.PPTO.ASIGNADO,0)),0) AS MAY,
			DECODE(SPP.PPTO.MES,6,SUM(NVL(SPP.PPTO.ASIGNADO,0)),0) AS JUN,
			DECODE(SPP.PPTO.MES,7,SUM(NVL(SPP.PPTO.ASIGNADO,0)),0) AS JUL,
			DECODE(SPP.PPTO.MES,8,SUM(NVL(SPP.PPTO.ASIGNADO,0)),0) AS AGO,
			DECODE(SPP.PPTO.MES,9,SUM(NVL(SPP.PPTO.ASIGNADO,0)),0) AS SEP,
			DECODE(SPP.PPTO.MES,10,SUM(NVL(SPP.PPTO.ASIGNADO,0)),0) AS OCT,
			DECODE(SPP.PPTO.MES,11,SUM(NVL(SPP.PPTO.ASIGNADO,0)),0) AS NOV,
			DECODE(SPP.PPTO.MES,12,SUM(NVL(SPP.PPTO.ASIGNADO,0)),0) AS DIC
		FROM
			DGI.PARTIDA,
			SPP.PPTO,
			S_DGI_GRUPOS GPO,
			S_DGI_SUBGRUPOS SUBGPO,
			S_DGI_SUBSUBGPO SUBSUBGPO
		WHERE
			GPO.GRUPO=SUBGPO.GRUPO AND
			GPO.YEAR=SUBGPO.YEAR AND
			SUBGPO.GRUPO=SUBSUBGPO.GRUPO AND
			SUBGPO.SUBGRUPO=SUBSUBGPO.SUBGRUPO AND
			SUBGPO.YEAR=SUBSUBGPO.YEAR AND
			SUBSUBGPO.SUBSUBGRUPO=DGI.PARTIDA.SUBSUBGPO AND
			SUBSUBGPO.YEAR=DGI.PARTIDA.YEAR AND
			DGI.PARTIDA.PARTIDA=SPP.PPTO.PARTIDA AND
			DGI.PARTIDA.YEAR=SPP.PPTO.YEAR AND
			SPP.PPTO.YEAR= $P{YEAR} AND
			SPP.PPTO.RAMO= $P{RAMO}
		GROUP BY
			PPTO.YEAR,
			PPTO.RAMO,
			GPO.GRUPO,
			GPO.DESCR,
			SPP.PPTO.MES
	) PPTO_INI
WHERE
	PPTO_INI.YEAR = $P{YEAR} AND
	PPTO_INI.RAMO = $P{RAMO}
GROUP BY
	PPTO_INI.YEAR,
	PPTO_INI.RAMO]]>
	</queryString>
