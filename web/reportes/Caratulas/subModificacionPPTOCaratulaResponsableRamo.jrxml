	<queryString>
		<![CDATA[SELECT * FROM
(
SELECT
    VW_EMP.NOMBRE||' '||VW_EMP.PATERNO||' '||VW_EMP.MATERNO  RESP_PRG,
    VW_EMP.DESCR
FROM
    DGI.VW_EMPLEADOS VW_EMP,
    DGI.RAMOS RAMPRG
WHERE
    RAMPRG.RFC = VW_EMP.RFC AND
    RAMPRG.HOMOCLAVE = VW_EMP.HOMOCLAVE AND
    RAMPRG.RAMO = $P{RAMO} AND
    RAMPRG.YEAR = $P{YEAR}
) ROWSS

WHERE
	ROWNUM = 1]]>
