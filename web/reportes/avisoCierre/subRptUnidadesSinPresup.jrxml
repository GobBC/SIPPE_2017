	<parameter name="YEAR" class="java.lang.String"/>
	<parameter name="USUARIO" class="java.lang.String"/>
	<queryString>
		<![CDATA[select distinct r.ramo||' '||r.descr as ramo,
       d.depto||' '||d.descr as depto
from dgi.dependencia d,
     S_POA_CODPROG cp,
     dgi.ramos r
where d.ramo=cp.ramo and
      d.depto=cp.depto and
      d.year=cp.year and
      cp.year= $P{YEAR} and
      r.ramo=cp.ramo and
      r.year=cp.year and
      cp.ramo in(select ur.ramo
                from POA.VW_ACC_USR_RAMO ur
                where ur.app_login= $P{USUARIO} and
                      ur.year= $P{YEAR}
               ) and
      not exists(select 1 from S_POA_ACCION_REQ ar where ar.ramo=cp.ramo and ar.depto=cp.depto and ar.year=cp.year)
ORDER BY ramo, depto]]>
	</queryString>
