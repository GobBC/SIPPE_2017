       a.accion as accion,
       a.descr as accion_descr
from S_POA_ACCION a,
     dgi.ramos r,
     S_POA_META m
where r.year=a.year and
      r.ramo=a.ramo and
      m.year=a.year and
      m.ramo=a.ramo and
      m.meta=a.meta and
      a.year= $P{YEAR} and
      a.ramo in(select ur.ramo
                from POA.VW_ACC_USR_RAMO ur
                where ur.app_login= $P{USUARIO} and
                      ur.year= $P{YEAR}
                ) and
      not exists(select 1 from dgi.accion_estimacion ae where ae.ramo=a.ramo and ae.year=a.year and ae.meta=a.meta and ae.accion=a.accion)
ORDER BY a.ramo, m.meta, a.accion]]>
