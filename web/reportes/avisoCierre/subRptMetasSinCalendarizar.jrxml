	<queryString>
		<![CDATA[select r.ramo||'-'||r.descr as ramo,
       m.meta,
       m.descr
from S_POA_META m,
     dgi.ramos r
where r.ramo=m.ramo and
      r.year=m.year and
      m.year= $P{YEAR} and
      m.ramo in(select ur.ramo
                from POA.VW_ACC_USR_RAMO ur
                where ur.app_login= $P{USUARIO} and
                      ur.year = $P{YEAR}
                ) and
      not exists(select 1 from dgi.estimacion e where e.ramo=m.ramo and e.year=m.year and e.meta=m.meta and e.valor>0)]]>
	</queryString>
