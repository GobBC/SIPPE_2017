	<queryString>
		<![CDATA[SELECT
    MET.META,
    MET.RAMO,
    MET.YEAR ,
	MET.LINEA,
	MET.DESCR,
	MEDMET.DESCR DES_UNIMED
    FROM
        S_POA_META MET,
        S_POA_MEDIDA_META MEDMET
    WHERE
         MET.RAMO = $P{RAMO}  AND
         MET.YEAR = $P{YEAR}  AND
		 MET.PRG = $P{PRG} AND
		 MET.TIPO_PROY = $P{TIPO_PROY}  AND
         MET.PROY = $P{PROY} AND
		MET.CVE_MEDIDA = MEDMET.MEDIDA  AND
		MET.YEAR = MEDMET.YEAR]]>
