	<queryString>
		<![CDATA[SELECT PTO.OFICIO, PTO.RAMO, PTO.DEPTO
    , D.DESCR AS UE_DESCR, PTO.PRG
    , PTO.TIPO_PROY||' '||PTO.PROY AS PROY
    , LPAD(PTO.META,2,0) AS META
    , CASE WHEN SPM.DESCR IS NOT NULL THEN SPM.DESCR ELSE MOM.DESCR END AS METADESCR
    , CASE WHEN PTO.ACCION >= 0 AND PTO.ACCION <= 99 THEN LPAD(TO_CHAR(PTO.ACCION),2,0) ELSE TO_CHAR(PTO.ACCION) END AS ACCION
    , CASE WHEN SPA.DESCR IS NOT NULL THEN SPA.DESCR ELSE MOA.DESCR END AS ACCION_DESCR
    , PTO.MUNICIPIO AS CODMUNICIPIO
    , PTO.DELEGACION AS CODLOCALIDAD
    , PTO.PARTIDA
    , P.DESCR AS DESCR_PARTIDA
    , PTO.FUENTE
    , PTO.FONDO
    , EXTRACT(YEAR FROM MO.FECPPTO) AS EJERCICIO
    , NVL(MAX(PCP.ASIGNADO),0) AS PPTOINICIALAPROBADO
    , NVL(MAX(PCP.AMPLIACION),0) AS AUMENTOS
    , NVL(MIN(PCP.REDUCCION)*-1,0) AS DISMINUCIONES
    , NVL(MAX(PCP.ASIGNADO)+MAX(PCP.AMPLIACION)+MIN(PCP.REDUCCION),0) AS PPTOMODIFICADOAPROBADO
    , MAX(PTO.AMPS) AS AMPS, MIN(PTO.DISM)*-1 AS DISM, MAX(PTO.TAMPS) AS TAMPS, MIN(PTO.TDISM) AS TDISM
    , NVL(MAX(PCP.ASIGNADO),0)+MAX(PTO.AMPS)+MIN(PTO.DISM)+MAX(PTO.TAMPS)-MAX(PTO.TDISM) AS TOTALMOVS
    , MAX(PTO.AMPS)+MIN(PTO.DISM)+MAX(PTO.TAMPS)-MAX(PTO.TDISM) AS PPTOMODIFSOLICITADO
    , CASE WHEN PTO.APMETA > 0 THEN 'SI' ELSE 'NO' END APMETA
    , CASE WHEN PTO.APACCION > 0 THEN 'SI' ELSE 'NO' END APACCION

FROM
    (
          SELECT A.OFICIO, A.RAMO, A.DEPTO, A.PRG, A.PROYECTO AS PROY, A.TIPO_PROY, A.META, A.ACCION, A.PARTIDA, A.FUENTE, A.FONDO,A.RECURSO, A.MUNICIPIO, A.DELEGACION
           ,ROUND(SUM(CASE WHEN TIPOMOV IN('A','C') THEN  IMPTE ELSE 0 END),0) AS AMPS
           ,ROUND(SUM(CASE WHEN TIPOMOV = 'R' THEN  IMPTE ELSE 0 END),0) AS DISM
           , TO_NUMBER(0) "TAMPS", TO_NUMBER(0) "TDISM"
           ,COUNT(MOM.OFICIO) AS APMETA, COUNT(MOA.OFICIO) AS APACCION
          FROM SPP.AMPLIACIONES A
               LEFT JOIN POA.MOVOFICIOS_ACCION MOA ON
                    MOA.OFICIO = A.OFICIO AND MOA.YEAR = $P{P_YEAR}
                    AND MOA.RAMO = A.RAMO AND MOA.META = A.META AND MOA.ACCION = A.ACCION
               LEFT JOIN POA.MOVOFICIOS_META MOM ON
                    MOM.OFICIO = A.OFICIO AND MOM.YEAR = $P{P_YEAR}
                    AND MOM.RAMO = A.RAMO AND MOM.META = A.META
          GROUP BY A.OFICIO, A.RAMO, A.PRG, A.DEPTO, A.PROYECTO, A.TIPO_PROY, A.META, A.ACCION, A.PARTIDA, A.FUENTE, A.FONDO, A.RECURSO, A.MUNICIPIO, A.DELEGACION

    UNION

          SELECT    TR.OFICIO, TR.RAMO, TR.DEPTO, TR.PRG, TR.PROYECTO AS PROY, TR.TIPO_PROY, TR.META, TR.ACCION, TR.PARTIDA, TR.FUENTE, TR.FONDO, TR.RECURSO, TR.MUNICIPIO, TR.DELEGACION
            , TO_NUMBER(0) "AMPS", TO_NUMBER(0) "DISM",ROUND(SUM(TR.IMPTE),0) AS TAMPS, TO_NUMBER(0) "TDISM"
            ,COUNT(MOM.OFICIO) AS APMETA, COUNT(MOA.OFICIO) AS APACCION
          FROM SPP.TRANSFREC TR
                LEFT JOIN POA.MOVOFICIOS_ACCION MOA ON
                    MOA.OFICIO = TR.OFICIO AND MOA.YEAR = $P{P_YEAR}
                    AND MOA.RAMO = TR.RAMO AND MOA.META = TR.META AND MOA.ACCION = TR.ACCION
                LEFT JOIN POA.MOVOFICIOS_META MOM ON
                    MOM.OFICIO = TR.OFICIO  AND MOM.YEAR = $P{P_YEAR}
                    AND MOM.RAMO = TR.RAMO AND MOM.META = TR.META
          GROUP BY TR.OFICIO, TR.RAMO, TR.DEPTO, TR.PRG, TR.PROYECTO, TR.TIPO_PROY, TR.META, TR.ACCION, TR.PARTIDA, TR.FUENTE, TR.FONDO, TR.RECURSO, TR.MUNICIPIO, TR.DELEGACION

    UNION

          SELECT   TR.OFICIO, TR.RAMO, TR.DEPTO, TR.PRG, TR.PROYECTO, TR.TIPO_PROY, TR.META, TR.ACCION, TR.PARTIDA, TR.FUENTE, TR.FONDO, TR.RECURSO, TR.MUNICIPIO, TR.DELEGACION
            , TO_NUMBER(0) "AMPS", TO_NUMBER(0) "DISM", TO_NUMBER(0) "TAMPS",ROUND(SUM(TR.IMPTE),0) AS TDISM
            , COUNT(MOM.OFICIO) AS APMETA, COUNT(MOA.OFICIO) AS APACCION
          FROM SPP.TRANSFERENCIAS TR
                LEFT JOIN POA.MOVOFICIOS_ACCION MOA ON
                    MOA.OFICIO = TR.OFICIO  AND MOA.YEAR = $P{P_YEAR}
                    AND MOA.RAMO = TR.RAMO AND MOA.META = TR.META AND MOA.ACCION = TR.ACCION
                LEFT JOIN POA.MOVOFICIOS_META MOM ON
                    MOM.OFICIO = TR.OFICIO AND MOM.YEAR = $P{P_YEAR}
                    AND MOM.RAMO = TR.RAMO AND MOM.META = TR.META
          GROUP BY TR.OFICIO, TR.RAMO, TR.DEPTO, TR.PRG, TR.PROYECTO, TR.TIPO_PROY, TR.META, TR.ACCION, TR.PARTIDA, TR.FUENTE, TR.FONDO, TR.RECURSO, TR.MUNICIPIO, TR.DELEGACION

    ) PTO
    LEFT JOIN SPP.VW_PPTO_CTAPUBLICA PCP ON PCP.RAMO = PTO.RAMO AND PCP.PRG = PTO.PRG AND PCP.DEPTO = PTO.DEPTO AND PCP.META = PTO.META AND PCP.ACCION = PTO.ACCION AND PCP.PARTIDA = PTO.PARTIDA AND PCP.FUENTE = PTO.FUENTE AND PCP.FONDO = PTO.FONDO AND PCP.YEAR = $P{P_YEAR}
    LEFT JOIN DGI.DEPENDENCIA D ON D.YEAR = $P{P_YEAR}
                                AND D.RAMO = PTO.RAMO AND D.DEPTO = PTO.DEPTO
    LEFT JOIN DGI.PARTIDA P ON P.YEAR = $P{P_YEAR}
		AND P.PARTIDA = PTO.PARTIDA
    LEFT JOIN S_POA_ACCION SPA ON
             SPA.YEAR = $P{P_YEAR}
             AND SPA.RAMO = PTO.RAMO AND SPA.PRG = PTO.PRG AND SPA.DEPTO = PTO.DEPTO AND SPA.META = PTO.META AND SPA.ACCION = PTO.ACCION
    LEFT JOIN S_POA_META SPM ON
              SPM.YEAR = $P{P_YEAR}
              AND SPM.RAMO = PTO.RAMO AND SPM.META = PTO.META
    LEFT JOIN POA.MOVOFICIOS_ACCION MOA ON
            MOA.RAMO = PTO.RAMO AND MOA.PRG = PTO.PRG AND MOA.DEPTO = PTO.DEPTO AND MOA.META = PTO.META AND MOA.ACCION = PTO.ACCION AND MOA.OFICIO = PTO.OFICIO AND MOA.YEAR = $P{P_YEAR}
    LEFT JOIN POA.MOVOFICIOS_META MOM ON
            MOM.RAMO = PTO.RAMO AND MOM.PRG = PTO.PRG AND MOM.TIPO_PROY = PTO.TIPO_PROY AND MOM.PROY = PTO.PROY AND MOM.META = PTO.META AND MOM.OFICIO = PTO.OFICIO AND MOM.YEAR = $P{P_YEAR}
    LEFT JOIN POA.MOVOFICIOS MO ON
            MO.OFICIO = PTO.OFICIO
WHERE
   PTO.RAMO = $P{P_RAMO}

GROUP BY PTO.OFICIO, PTO.RAMO, PTO.DEPTO
    , D.DESCR
    , PTO.PRG, PTO.PROY, PTO.TIPO_PROY, PTO.META
    , CASE WHEN SPM.DESCR IS NOT NULL THEN SPM.DESCR ELSE MOM.DESCR END, PTO.ACCION
    , CASE WHEN PTO.ACCION >= 0 AND PTO.ACCION <= 99 THEN LPAD(TO_CHAR(PTO.ACCION),2,0) ELSE TO_CHAR(PTO.ACCION) END
    , CASE WHEN SPA.DESCR IS NOT NULL THEN SPA.DESCR ELSE MOA.DESCR END, PTO.PARTIDA, P.DESCR, PTO.FUENTE, PTO.FONDO, EXTRACT(YEAR FROM MO.FECPPTO), PTO.MUNICIPIO, PTO.DELEGACION, PTO.APMETA, PTO.APACCION
ORDER BY PTO.OFICIO, PTO.RAMO, PTO.PRG, PTO.TIPO_PROY, PTO.PROY, PTO.META, PTO.ACCION, PTO.PARTIDA]]>
	</queryString>
