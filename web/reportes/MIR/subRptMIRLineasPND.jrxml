    POA.RAMO_PRG_LINEA_PND LN,
    POA.PND_LINEA_ACCION LA,
    POA.PND_ESTRATEGIA LE,
    POA.PND_OBJETIVO LO,
    POA.PND_META_NACIONAL LM
WHERE
    LN.YEAR = $P{YEAR} AND
    LN.RAMO = $P{RAMO} AND
    LN.PRG = $P{PRG} AND
    LN.YEAR = LA.YEAR AND
    LN.LINEA_PND = LA.LINEA_ACCION AND
    LA.YEAR = LE.YEAR AND
    LA.ESTRATEGIA = LE.ESTRATEGIA AND
    LA.YEAR = LO.YEAR AND
    LA.OBJETIVO = LO.OBJETIVO AND
    LA.YEAR = LM.YEAR AND
    LA.META_NACIONAL = LM.META_NACIONAL]]>
