    LO.OBJETIVO||' - '||LO.DESCR AS OBJETIVO,
    LE.ESTRATEGIA||' - '||LE.DESCR AS ESTRATEGIA,
    LA.LINEA_ACCION||' - '||LA.DESCR AS LINEAPND
