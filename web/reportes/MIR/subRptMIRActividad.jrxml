	<parameter name="COMPONENTEROW" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
   'A'||ROW_NUMBER() OVER (ORDER BY  A.ACTIVIDAD ASC) ||'. '||A.DESCR  AS DESCR,
    A.META,
    A.ACCION,
    IACT.DESCR AS INDICADOR,
    IACT.MEDIOS_VERIFICACION AS MEDIOS_VERIFICACION,
    IACT.SUPUESTO AS SUPUESTO,
    ' ' AS RELLENO
FROM  POA.MIR_ACTIVIDAD A
    LEFT JOIN DGI.VW_MIR_INDICADOR IACT ON IACT.YEAR = A.YEAR AND IACT.RAMO = A.RAMO AND IACT.PRG = A.PRG AND IACT.COMPONENTE = A.COMPONENTE AND IACT.DIMENSION = 4
WHERE
        A.YEAR = $P{YEAR}
    AND A.RAMO = $P{RAMO}
    AND A.PRG = $P{PRG}
    AND A.COMPONENTE = $P{COMPONENTE}
ORDER BY A.ACTIVIDAD]]>
	</queryString>
