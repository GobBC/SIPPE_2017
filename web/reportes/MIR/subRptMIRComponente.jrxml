	<queryString>
		<![CDATA[SELECT
   'C'||ROW_NUMBER() OVER (ORDER BY  A.COMPONENTE ASC) ||'. '||A.DESCR AS DESCR,
    A.COMPONENTE,
    A.META,
   'C'||ROW_NUMBER() OVER (ORDER BY  A.COMPONENTE ASC)  AS COMPONENTEROW,
    ICOM.DESCR AS INDICADOR,
    ICOM.MEDIOS_VERIFICACION AS MEDIOS_VERIFICACION,
    ICOM.SUPUESTO AS SUPUESTO
FROM  POA.MIR_COMPONENTE A
    LEFT JOIN DGI.VW_MIR_INDICADOR ICOM ON ICOM.YEAR = A.YEAR AND ICOM.RAMO = A.RAMO AND ICOM.PRG = A.PRG AND ICOM.COMPONENTE = A.COMPONENTE AND ICOM.DIMENSION = 3
WHERE
        A.YEAR = $P{YEAR}
    AND A.RAMO = $P{RAMO}
    AND A.PRG = $P{PRG}
ORDER BY A.COMPONENTE]]>
	</queryString>
