	<queryString>
		<![CDATA[SELECT
    CF.YEAR,CF.RAMO,CF.PRG,
    SF.FINALIDAD||'.'||SF.FUNCION||'.'||SF.SUBFUNCION||'. '||SF.DESCR AS CLASIF_FUNCIONAL
FROM
    POA.PROYECTO_FUNCIONAL CF,
    POA.SUBFUNCION SF
WHERE
    CF.YEAR = SF.YEAR
    AND CF.FINALIDAD = SF.FINALIDAD
    AND CF.FUNCION = SF.FUNCION
    AND CF.SUBFUNCION = SF.SUBFUNCION
    AND CF.YEAR = $P{YEAR}
    AND CF.RAMO = $P{RAMO}
    AND CF.PRG = $P{PRG}
GROUP BY CF.YEAR,CF.RAMO,CF.PRG,SF.FINALIDAD||'.'||SF.FUNCION||'.'||SF.SUBFUNCION||'. '||SF.DESCR]]>
	</queryString>
