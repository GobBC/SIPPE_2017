	<queryString>
		<![CDATA[SELECT
    to_char(RP.FIN, '000' ) AS FIN, FIN.DESCR AS FIN_DESCR,
    IFIN.DESCR AS FIN_INDICADOR,
    IFIN.MEDIOS_VERIFICACION AS FIN_MEDIOS_VERIFICACION,
    IFIN.SUPUESTO AS FIN_SUPUESTO,
    RP.PROPOSITO,
    IPROP.DESCR AS PROP_INDICADOR,
    IPROP.MEDIOS_VERIFICACION AS PROP_MEDIOS_VERIFICACION,
    IPROP.SUPUESTO AS PROP_SUPUESTO,
    PA.GOBIERNO,PA.SECRETARIA,SYSDATE AS HOY
FROM POA.RAMO_PROGRAMA RP
    LEFT JOIN S_POA_FIN FIN  ON RP.YEAR = FIN.YEAR AND TO_CHAR(RP.FIN,'000') = TO_CHAR(FIN.FIN,'000')
    LEFT JOIN DGI.VW_MIR_INDICADOR IFIN ON IFIN.YEAR = RP.YEAR AND IFIN.RAMO = RP.RAMO AND IFIN.PRG = RP.PRG AND IFIN.DIMENSION = 1
    LEFT JOIN DGI.VW_MIR_INDICADOR IPROP ON IPROP.YEAR = RP.YEAR AND IPROP.RAMO = RP.RAMO AND IPROP.PRG = RP.PRG AND IPROP.DIMENSION = 2
    LEFT JOIN DGI.PARAMETROS PA ON PA.GOBIERNO = PA.GOBIERNO
WHERE
        RP.YEAR = $P{YEAR}
       AND RP.RAMO = $P{RAMO}
       AND RP.PRG = $P{PRG}]]>
	</queryString>
