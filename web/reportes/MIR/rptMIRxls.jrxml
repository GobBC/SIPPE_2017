	<queryString>
		<![CDATA[SELECT
    RP.YEAR, RP.RAMO, R.RAMO||' - '||R.DESCR AS RAMO_DESCR,
    RP.PRG, P.PRG ||' - '|| P.DESCR AS PRG_DESCR,
    P.PRG_CONAC, NVL(CONAC.PRG_CONAC ||' - '||CONAC.DESCR,'') AS CLASIF_PROGRAMATICA,
    RP.DEPTO, DEP.DEPTO||' - '||DEP.DESCR AS UNIDAD_RESPONSABLE,
    UPPER(RP.PROBLEMA_FOCAL) AS PROBLEMA_FOCAL,
    RP.PONDERADO, NVL(PON.PONDERADO||' - '||UPPER(PON.DESCR),'') AS PONDERADO_DESCR,
    UPPER(NVL(EMP.NOMBRE||' ','')||NVL(EMP.PATERNO||' ','')||NVL(EMP.MATERNO,'')) AS UE_RESPONSABLE,
    to_char(RP.FIN, '000' ) AS FIN, UPPER(FIN.DESCR) AS FIN_DESCR,
    UPPER(RP.PROPOSITO) AS PROPOSITO,PA.GOBIERNO,PA.SECRETARIA,SYSDATE AS HOY
FROM POA.RAMO_PROGRAMA RP
    INNER JOIN DGI.RAMOS R ON R.YEAR = RP.YEAR AND R.RAMO = RP.RAMO
    INNER JOIN S_POA_PROGRAMA P ON RP.YEAR = P.YEAR AND RP.PRG = P.PRG
    LEFT JOIN DGI.DEPENDENCIA DEP ON  RP.YEAR = DEP.YEAR AND RP.RAMO = DEP.RAMO AND RP.DEPTO = DEP.DEPTO
    LEFT JOIN S_POA_PROGRAMA_CONAC CONAC ON P.YEAR = CONAC.YEAR AND P.PRG_CONAC = CONAC.PRG_CONAC
    LEFT JOIN DGI.PONDERADO PON ON RP.PONDERADO = PON.PONDERADO
    LEFT JOIN DGI.VW_EMPLEADOS EMP ON RP.RFC = EMP.RFC AND RP.HOMOCLAVE = EMP.HOMOCLAVE
    LEFT JOIN S_POA_FIN FIN  ON RP.YEAR = FIN.YEAR AND TO_CHAR(RP.FIN,'000') = TO_CHAR(FIN.FIN,'000')
    LEFT JOIN DGI.PARAMETROS PA ON PA.GOBIERNO = PA.GOBIERNO
WHERE
        RP.YEAR = $P{YEAR}
       AND RP.RAMO = $P{RAMO}
       AND RP.PRG = $P{PROGRAMA}]]>
	</queryString>
